	SendClientMessageEx(playerid, RENK_KARAKTER, "%s", yazi);
 	format(yazi, sizeof(yazi), "(Hesap ID: %03d) - (Kiyafet: %d) - (SQL ID: %03d) - (Zirh: %.0f) - (Meslek: %s)", vericek_int(0, "hesap"), vericek_int(0, "Kiyafet"), vericek_int(0, "ID"), vericek_float(0, "Zirh"), MeslekIsim(vericek_int(0, "Meslek")));
