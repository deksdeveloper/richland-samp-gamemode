		Log_Kaydet("loglar/admin_silah.txt", "%s", log_icerik);
		YoneticiMesaji(i, "%s adli yetkili sana %s model silahi %d mermiyle verdi.", OyuncuBilgi[playerid][oAdminNick], SilahIsim(silahid), mermi);
		sayi++;
	}
	SunucuMesaji(playerid, "Yakinindaki %d adet kisiye %s model silahi %d mermiyle verdin.", sayi, SilahIsim(silahid), mermi);
	return 1;
}

