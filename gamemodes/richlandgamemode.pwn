forward BugluKoyKontrol();
public BugluKoyKontrol()
{
	foreach (new i : Player) if(OyuncuBilgi[i][oGiris] == true)
	{
		if(HataModu[i] != 1)
		{
		    if(IsPlayerInRangeOfPoint(i, 100.0, 0.0, 0.0, 0.0))
		    {
			   OyuncuPosDegistir(i, 1496.02, -1693.39, 13.3988);
			   TogglePlayerControllable(i, false);
	           OyuncuBilgi[i][oKontrol] = false;
			   SunucuMesaji(i, "Buglu koyde dogdugun tespit edildi. Hata moduna alindin 10 saniye icerisinde sorun duzeltilecek.");
			   SunucuMesaji(i, "10 saniye boyunca donmus sekilde duracaksin, daha sonrasinda otomatik olarak cozuleceksin.");
			   HataModu[i] = 1;
			   SetTimerEx("BugluKoyDuzelt", 10000, false, "i", i);
		    }	
	    }
    }
 	return 1;
}

forward BugluKoyDuzelt(i);
public BugluKoyDuzelt(i)
{
	OyuncuPosDegistir(i, 1496.02, -1693.39, 13.3988);
    TogglePlayerControllable(i, true);
	OyuncuBilgi[i][oKontrol] = true;
    SunucuMesaji(i, "Sorun duzeltildi, eger sorun devam ederse destek atabilirsin, iyi roller!");
	HataModu[i] = 0;
	return 1;
}

CMD:sampmedia(playerid, params[])
{
    if(OyuncuBilgi[playerid][oAdmin] < 7) return YetkinizYok(playerid);	
    new oyuncuid;
	if(sscanf(params, "u", oyuncuid)) return KullanimMesaji(playerid, "/sampmedia [oyuncu id/isim]");
	if(OyuncuBilgi[oyuncuid][pSampMedia] != 0) return HataMesaji(playerid, "Bu kisi zaten odul almis.");
	
	OyuncuBilgi[oyuncuid][oVIP] = 2;
	new gun = 1 * 86400;
	OyuncuBilgi[oyuncuid][oVIPSure] = gettime() + gun;
	ParaEkle(oyuncuid, 2500);
	OyuncuBilgi[oyuncuid][pSampMedia] = 1;
	SunucuMesaji(oyuncuid, "%s adli yetkili, SA-MP Media Roleplay oylamalarinda bize destek oldugunuz icin size hediye verdi.", OyuncuBilgi[playerid][oAdminNick]);
	SunucuMesaji(playerid, "%s adli oyuncuya SA-MP Media Roleplay oylamalarinda bize destek oldugu icin hediyesini verdiniz.", OyuncuIsim(oyuncuid));
	return 1;	
}

ResetPlayerVariables(playerid)
{
    PlayerAnim[playerid] = false;
	PlayerAction[playerid] = 0;
	return continue(playerid);
}

stock ApplyAnimationEx(playerid, animlib[], animname[], Float:fDelta, loop, lockx, locky, freeze, time, forcesync = 1, toggleable = 1) // ReWrap
{
	forcesync = 1;
	if(OyuncuBilgi[playerid][oHapisSure][0] > 0) return HataMesaji(playerid, "OOC hapisteyken animasyon kullanamazsin.");
	if(OyuncuBilgi[playerid][oYaraliSure] > 0) return HataMesaji(playerid, "Yaraliyken animasyon kullanamazsin.");
	if(OyuncuBilgi[playerid][oDonduruldu] > 0) return HataMesaji(playerid, "Dondurulma etkisindeyken animasyon kullanamazsin.");
    if(OyuncuBilgi[playerid][oKelepceli] == 1) return HataMesaji(playerid, "Kelepceliyken animasyon kullanamazsin.");
	    
	if(!PlayerAnim[playerid] && toggleable == 1)
		PlayerAnim[playerid] = true;

	ApplyAnimation(playerid, animlib, "null", 0.0, 0, 0, 0, 0, 0, 0);
	ApplyAnimation(playerid, animlib, animname, fDelta, loop, lockx, locky, freeze, time, forcesync);
	return 1;
}


stock SetPlayerSpecialActionEx(playerid, actionid) // ReWrap
{
	if(OyuncuBilgi[playerid][oHapisSure][0] > 0) return HataMesaji(playerid, "OOC hapisteyken animasyon kullanamazsin.");
	if(OyuncuBilgi[playerid][oYaraliSure] > 0) return HataMesaji(playerid, "Yaraliyken animasyon kullanamazsin.");
	if(OyuncuBilgi[playerid][oDonduruldu] > 0) return HataMesaji(playerid, "Dondurulma etkisindeyken animasyon kullanamazsin.");
    if(OyuncuBilgi[playerid][oKelepceli] == 1) return HataMesaji(playerid, "Kelepceliyken animasyon kullanamazsin.");
	    
	SetPlayerSpecialAction(playerid, actionid);

	if(!PlayerAnim[playerid])
		PlayerAnim[playerid] = true;
	return 1;
}

