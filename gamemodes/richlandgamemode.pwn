Dialog:BusCard(playerid, response, listitem, inputtext[])
{
	if (response)
	{
	    switch (listitem)
	    {
	        case 0:
	        {
				if(OyuncuBilgi[playerid][pOtobusKart] == 1) return HataMesaji(playerid, "Zaten BusCard'a sahipsin.");
	            new str[256];
				format(str, sizeof(str), ""GR"%s"B" ucretle, "R"BusCard"B" satin almak istiyor musun?", NumaraFormati(BUSCARD_UCRET));
				Dialog_Show(playerid, BusCardSatinAl, DIALOG_STYLE_MSGBOX, "BusCard Satin Al", str, "Evet", "Hayir");
	        }
	        case 1:
	        {
                if(OyuncuBilgi[playerid][pOtobusKart] == 0) return HataMesaji(playerid, "BusCard'in yokken, bakiye yukleme islemi yapamazsin.");
	            new str[512];
	            format(str, sizeof str, "BusCard'in icin, kac bakiye yuklemek istiyorsun? (1 BusCard Bakiyesi: 2$)");
	            Dialog_Show(playerid, BakiyeYukle, DIALOG_STYLE_INPUT, "BusCard Bakiye Yukle", str, "Yukle", "Cikis");
	        }

	    }
	}
	return true;
}

Dialog:BakiyeYukle(playerid, response, listitem, inputtext[])
{
	if (response)
	{
	    new Miktar = strval(inputtext);
	    if (!IsNumeric(inputtext))
	    {
	        new str[512];
            format(str, sizeof str, ""R"Bir miktar belirtmelisin!"B"\nBusCard'in icin, kac bakiye yuklemek istiyorsun? (1 BusCard Bakiyesi: 2$");
            return Dialog_Show(playerid, BakiyeYukle, DIALOG_STYLE_INPUT, "BusCard Bakiye Yukle", str, "Yukle", "Cikis");
	    }
		if (OyuncuBilgi[playerid][oPara] < Miktar*2)
		{
		    new str[512];
            format(str, sizeof str, ""R"Yeterli miktarda paran bulunmuyor!"B"\nBusCard'?n i?in, ka? bakiye y?klemek istiyorsun? (1 BusCard Bakiyesi: 2$");
            return Dialog_Show(playerid, BakiyeYukle, DIALOG_STYLE_INPUT, "BusCard Bakiye Y?kle", str, "Yukle", "Cikis");
		}
		SunucuMesaji(playerid, ""R"%s"B" ?deyerek, "GR"%d"B" BusCard bakiyesi y?kledin!", NumaraFormati(Miktar*2), Miktar);
		OyuncuBilgi[playerid][pOtobusKartBakiye] += Miktar;
		ParaEkle(playerid, -2*Miktar);
	}
	return true;
}

Dialog:BusCardSatinAl(playerid, response, listitem, inputtext[])
{
	if(!response)
	{
	    HataMesaji(playerid, ""R"BusCard"B" sat?n alma i?lemini iptal ettin.");
	}
	if(response)
	{
        if(OyuncuBilgi[playerid][oPara] < BUSCARD_UCRET) return HataMesaji(playerid, "Yeterli miktarda paran yok.");
		SunucuMesaji(playerid, ""GR"%s"B" ?cretle, "R"BusCard"B" sat?n ald?n!", NumaraFormati(BUSCARD_UCRET));
		OyuncuBilgi[playerid][pOtobusKart] = 1;
		ParaEkle(playerid, -BUSCARD_UCRET);
	}
	return 1;
}

