public Birlik_LevelYukselt(oyuncuid)
{
	new rows, fields;
	cache_get_row_count(rows);
	cache_get_field_count(fields);
	new birlikid = OyuncuBilgi[oyuncuid][oBirlik];
	if(BirlikBilgi[birlikid][birlikMaksRutbe] != OyuncuBilgi[oyuncuid][oBirlikRutbe] && OyuncuBilgi[oyuncuid][oBirlikLider] != BirlikBilgi[birlikid][birlikID]) return HataMesaji(oyuncuid, "Bu secenegi sadece birlik liderleri kullanabilir."), cmd_birlik(oyuncuid, "null");
	switch(BirlikBilgi[birlikid][birlikLevel]) {
		case 1: {
			if(rows < 3) return HataMesaji(oyuncuid, "Birlik level yukseltme gereksinimini karsilayamiyorsun.");
			BirlikBilgi[birlikid][birlikLevel]++;
			Birlik_Kaydet(birlikid);
			SunucuMesaji(oyuncuid, "Birliginiz level atladi, yeni level: %d", BirlikBilgi[birlikid][birlikLevel]);
		}
		case 2: {
			if(rows < 5) return HataMesaji(oyuncuid, "Birlik level yukseltme gereksinimini karsilayamiyorsun.");
			BirlikBilgi[birlikid][birlikLevel]++;
			Birlik_Kaydet(birlikid);
			SunucuMesaji(oyuncuid, "Birliginiz level atladi, yeni level: %d", BirlikBilgi[birlikid][birlikLevel]);
		}
		case 3: {
			if(rows < 7 && OyuncuBilgi[oyuncuid][oPara] < BIRLIK_LEVELUCRET) return HataMesaji(oyuncuid, "Birlik level yukseltme gereksinimini karsilayamiyorsun.");

			if(rows < 7) ParaEkle(oyuncuid, -BIRLIK_LEVELUCRET), OyuncuKaydet(oyuncuid);

			BirlikBilgi[birlikid][birlikLevel]++;
			Birlik_Kaydet(birlikid);
			SunucuMesaji(oyuncuid, "Birliginiz level atladi, yeni level: %d", BirlikBilgi[birlikid][birlikLevel]);
		}
		case 4: {
			if(rows < 10 && OyuncuBilgi[oyuncuid][oPara] < BIRLIK_LEVELUCRET) return HataMesaji(oyuncuid, "Birlik level yukseltme gereksinimini karsilayamiyorsun.");

			if(rows < 10) ParaEkle(oyuncuid, -BIRLIK_LEVELUCRET), OyuncuKaydet(oyuncuid);

			BirlikBilgi[birlikid][birlikLevel]++;
			Birlik_Kaydet(birlikid);
			SunucuMesaji(oyuncuid, "Birliginiz level atladi, yeni level: %d", BirlikBilgi[birlikid][birlikLevel]);
		}
		case 5..9: {
		    if(rows < 15 && OyuncuBilgi[oyuncuid][oPara] < BIRLIK_LEVELUCRET) return HataMesaji(oyuncuid, "Birlik level yukseltme gereksinimini karsilayamiyorsun.");

			if(rows < 15) ParaEkle(oyuncuid, -BIRLIK_LEVELUCRET), OyuncuKaydet(oyuncuid);

			BirlikBilgi[birlikid][birlikLevel]++;
			Birlik_Kaydet(birlikid);
			SunucuMesaji(oyuncuid, "Birliginiz level atladi, yeni level: %d", BirlikBilgi[birlikid][birlikLevel]);
		}
	}
	new log_icerik[144], oyuncu_isim[MAX_PLAYER_NAME + 1];
	GetPlayerName(oyuncuid, oyuncu_isim, sizeof(oyuncu_isim));
	format(log_icerik, sizeof(log_icerik), "[%s] %s(%d) birlik levelini yukseltti. (%d / birlik: %s).", Tarih(), oyuncu_isim, OyuncuBilgi[oyuncuid][oID], BirlikBilgi[birlikid][birlikLevel], BirlikBilgi[birlikid][birlikAd]);
	//Gunluk_LogKaydetKaydet(log_icerik);
	Log_Kaydet("loglar/birlik.txt", "%s", log_icerik);
	return 1;
}

