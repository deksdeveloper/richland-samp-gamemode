CMD:telefon(playerid, params[])
{
	if(OyuncuBilgi[playerid][oTelefon] == 0 || OyuncuBilgi[playerid][oTelefonNumarasi] == 0) return HataMesaji(playerid, "Telefonun yok veya operatore bagli degil.");
	if(GetPlayerSpecialAction(playerid) == SPECIAL_ACTION_CUFFED) return HataMesaji(playerid, "su anda telefonunu kullanamazsin.");

	if(OyuncuBilgi[playerid][oGiris] == false) return HataMesaji(playerid, "su anda bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oDonduruldu] > 0) return HataMesaji(playerid, "su anda bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oHapisSure][0] > 0 || OyuncuBilgi[playerid][oHapisSure][1] > 0) return HataMesaji(playerid, "Hapiste bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oYaraliSure] > 0) return HataMesaji(playerid, "Yaraliyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oAdminDurum]) return HataMesaji(playerid, "Yonetici isbasindayken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oPaintball] != -1) return HataMesaji(playerid, "Paintballda bu komutu kullanamazsin.");

	new icerik[512];
	if(OyuncuBilgi[playerid][oTelefonDurum] == false) format(icerik, sizeof(icerik), "%s\n", (OyuncuBilgi[playerid][oTelefonDurum] == false) ? ("Telefonu Ac") : ("Telefonu Kapat"));
	else {
		format(icerik, sizeof(icerik), "{C2CE1C}[TELEFON] {FFFFFF}Arama\t\n{C2CE1C}[TELEFON] {FFFFFF}Mesaj\t\n{C2CE1C}[TELEFON] {FFFFFF}Arama Kayitlari\t\n{C2CE1C}[TELEFON] {FFFFFF}Rehber\t\n");
	    format(icerik, sizeof(icerik), "%s{C2CE1C}[TELEFON] {FFFFFF}Gizli Numara\t%s\n", icerik, (OyuncuBilgi[playerid][oGizliNumara] == false) ? ("Ac") : ("Kapat"));
	    format(icerik, sizeof(icerik), "%s{5781FF}[UYGULAMA] {FFFFFF}Konum Gonder\t\n{5781FF}[UYGULAMA] {FFFFFF}SanAdvertisement\t\n{5781FF}[UYGULAMA] {FFFFFF}RichBank Mobil\t\n{5781FF}[UYGULAMA] {FFFFFF}Uber\t\n{5781FF}[UYGULAMA] {FFFFFF}Pennings Emlak\t\n", icerik);
		format(icerik, sizeof(icerik), "%s{AFAFAF}[AYARLAR] {FFFFFF}SOS\t\n{AFAFAF}[AYARLAR] {FFFFFF}Ses\t%s\n%s\t\n", icerik, (OyuncuBilgi[playerid][oTelefonSes] == false) ? ("Ac") : ("Kapat"), (OyuncuBilgi[playerid][oTelefonDurum] == false) ? ("Telefonu Ac") : ("Telefonu Kapat"));
	}
	Dialog_Show(playerid, DIALOG_TELEFON, DIALOG_STYLE_TABLIST, "Cep Telefonu", icerik, "Sec", "iptal");
	if(!!strcmp(params, "xmZcQmmp")) cmd_me(playerid, "elini cebine goturur, telefonunu cikarir.");
	return 1;

}

Dialog:DIALOG_ARAMA(playerid, response, listitem, inputtext[])
{
    if(!response) return false;

		new numara, num[144];
		if(sscanf(inputtext, "d", numara))
		    return Dialog_Show(playerid, DIALOG_ARAMA, DIALOG_STYLE_INPUT, "> Arama Yap", "Arama yapmak istediginiz numarayi girin:", "Ara", "Kapat");

		format(num, sizeof(num), "%d", numara);
		cmd_ara(playerid, num);	
	return 1;
}

Dialog:DIALOG_MESAJ(playerid, response, listitem, inputtext[])
{
	if(!response) return 0;

		new numara;
		if(sscanf(inputtext, "d", numara))
			return Dialog_Show(playerid, DIALOG_MESAJ, DIALOG_STYLE_INPUT, "RichMessage", "Mesaj yollamak istedigin numarayi giriniz:", "Tamam", "Kapat");

		foreach(new i : Player) if(OyuncuBilgi[i][oTelefonNumarasi] == numara) {
			SetPVarInt(playerid, "mNumara", numara);
			return Dialog_Show(playerid, DIALOG_MESAJ2, DIALOG_STYLE_INPUT, "RichMessage", "Lutfen gondermek istediginiz mesaji girin:", "Gonder", "Kapat");
		}
		UyariMesaji(playerid, "Gecersiz numara girdin.");
		Dialog_Show(playerid, DIALOG_MESAJ, DIALOG_STYLE_INPUT, "RichMessage", "Mesaj yollamak istedigin numarayi giriniz:", "Tamam", "Kapat");
	return 1;
}

Dialog:DIALOG_MESAJ2(playerid, response, listitem, inputtext[])
{
	if(!response) return 0;

		if(isnull(inputtext))
			return Dialog_Show(playerid, DIALOG_MESAJ2, DIALOG_STYLE_INPUT, "RichMessage", "Lutfen gondermek istediginiz mesaji girin:", "Gonder", "Kapat");

		new numara = GetPVarInt(playerid, "mNumara"), num = -1;
		if(numara == 0) return HataMesaji(playerid, "Bu telefon numarasi gecersizdir!");
		if(numara == 911) return HataMesaji(playerid, "911 hattina mesaj gonderemezsin.");

		foreach(new i : Player) if(OyuncuBilgi[i][oTelefonNumarasi] == numara) {
			if(OyuncuBilgi[i][oTelefonDurum] == false) return HataMesaji(playerid, "Mesaj iletilemedi.");

			SendClientMessageEx(i, RENK_SARI, "SMS: %s | Gonderen: %d", inputtext, OyuncuBilgi[playerid][oTelefonNumarasi]);
			cmd_do(i, "Telefonundan mesaj bildirim sesi geldi.");
			ParaEkle(playerid, -SMS_UCRET);
			num = i;
		}
		SetPVarInt(playerid, "mNumara", 0);
		if(num == -1)
		    return UyariMesaji(playerid, "Mesaj iletilemedi.");

		UyariMesaji(playerid, "Mesaj iletildi.");
	return 1;
}

