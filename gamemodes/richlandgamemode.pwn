	if(OyuncuBilgi[oyuncuid][oHapisSure][0] > 0 || OyuncuBilgi[oyuncuid][oHapisSure][1] > 0) return HataMesaji(playerid, "Oyuncu hapiste, bu komutu su an kullanamazsin.");
	if(OyuncuBilgi[oyuncuid][oYarali] > 0) return HataMesaji(playerid, "Oyuncu yaralanmis, bu komutu su an kullanamazsin.");
	if(OyuncuBilgi[oyuncuid][oDonduruldu] > 0) return HataMesaji(playerid, "Oyuncu dondurulmus, bu komut su an kullanilamaz.");
    if(OyuncuBilgi[oyuncuid][oIzlenen] != INVALID_PLAYER_ID) return HataMesaji(playerid, "Oyuncu birisini izliyor, su an kullanilamaz.");

	new Float:oyuncu_pos[3];
