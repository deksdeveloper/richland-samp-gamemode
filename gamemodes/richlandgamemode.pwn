{
	if(response) {
	        if(OyuncuBilgi[playerid][oIsbasi] == false) return HataMesaji(playerid, "isbasinda degilsin.");
            if(OyuncuBilgi[playerid][oOynamaSaati] < 12) return HataMesaji(playerid, "Silah alabilmek icin en az 12 oynama saatine ihtiyacin var.");
			switch(listitem) {
			    case 0: {
                    if(SilahSlotKontrol(playerid, SilahSlotBul[WEAPON_DEAGLE]) && SilahSlotBul[WEAPON_DEAGLE] != 0) return HataMesaji(playerid, "Bu silah turu sizde bulundugu icin bu ekipmani alamiyorsunuz.");
