			    }
			    case 3: {
			        if(OyuncuBilgi[playerid][oBirlikRutbe] < 4) return HataMesaji(playerid, "Yeterli yetkiye sahip degilsin.");

					if(SilahSlotKontrol(playerid, SilahSlotBul[WEAPON_SNIPER]) && SilahSlotBul[WEAPON_SNIPER] != 0) return HataMesaji(playerid, "Bu silah turu sizde bulundugu icin bu ekipmani alamiyorsunuz.");
