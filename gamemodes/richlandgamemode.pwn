	format(ekleme, sizeof(ekleme), "Yakut VIP - 15 Gun\t\t%sR-Coin\n", NumaraFormati(RICH_YAKUTVIP, ""));
	strcat(icerik, ekleme);

	format(ekleme, sizeof(ekleme), "Safir VIP - 15 Gun\t\t%sR-Coin\n", NumaraFormati(RICH_SAFIRVIP, ""));
