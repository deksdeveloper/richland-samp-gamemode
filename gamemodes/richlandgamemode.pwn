public Surucusuz_Arac_Respawn(playerid, durum)
{
	aracrespawn = false;
	new sayi = 0;
	for(new a = 0; a != MAKSIMUM_ARAC; a++) if(AracBilgi[a][aracKullanimda] && IsValidVehicle(AracBilgi[a][aracYarat]) && SurucuBul(AracBilgi[a][aracYarat]) == INVALID_PLAYER_ID) ASpawn(AracBilgi[a][aracYarat], 1), sayi++;

    if(durum == 1)
    {
		if(!sayi) return HataMesaji(playerid, "Re-spawn edilebilecek arac yok.");

