CMD:asilahver(playerid, params[])
{
    if(OyuncuBilgi[playerid][oAdmin] < 5) return YetkinizYok(playerid);
    if(OyuncuBilgi[playerid][oAdminDurum] == false && OyuncuBilgi[playerid][oAdmin] < 6) return IsbasiYapmalisin(playerid);

	new oyuncuid, silahid, mermi;
	if(sscanf(params, "udI(50)", oyuncuid, silahid, mermi)) return KullanimMesaji(playerid, "/asilahver [oyuncu ID/isim] [silah id] [mermi]");
	if(!IsPlayerConnected(oyuncuid)) return HataMesaji(playerid, "Girilen kisi oyunda degil.");
	if(OyuncuBilgi[oyuncuid][oOynamaSaati] < 12) return HataMesaji(playerid, "Bu oyuncu 12 oynama saati olmadigi icin silah alamaz.");
	if(silahid <= 0 || silahid > 46 || (silahid >= 19 && silahid <= 21)) return HataMesaji(playerid, "Gecersiz silah belirlendi.");
	if(silahid == 35 && OyuncuBilgi[playerid][oAdmin] < 7 || silahid == 38 && OyuncuBilgi[playerid][oAdmin] < 7) return HataMesaji(playerid, "Bu silahi vermek icin seviye 7 yonetici olman gerekiyor.");
	if(OyuncuBilgi[oyuncuid][oPaintball] != -1) return HataMesaji(playerid, "Oyuncu paintballda, silah verilemez.");
	if(PetKontrol[oyuncuid] != -1) return HataMesaji(playerid, "Pet kontrol modundaki birisine silah veremezsin.");

	if(mermi < 1) return HataMesaji(playerid, "Mermi en az 1 olmalidir.");
	new limit = 0;
	switch(silahid) {
		case WEAPON_DEAGLE: limit = DEAGLE_LIMIT;
		case WEAPON_AK47: limit = AK47_LIMIT;
		case WEAPON_MP5: limit = MP5_LIMIT;
	    case WEAPON_TEC9: limit = TEC9_LIMIT;
	    case WEAPON_SHOTGUN: limit = SHOTGUN_LIMIT;
	    case WEAPON_RIFLE: limit = RIFLE_LIMIT;
	    case WEAPON_UZI: limit = UZI_LIMIT;
	    case WEAPON_COLT45: limit = COLT45_LIMIT;
	    case WEAPON_M4: limit = M4_LIMIT;
	    case WEAPON_SNIPER: limit = SNIPER_LIMIT;
	    case WEAPON_SHOTGSPA: limit = COMBATSHOTGUN_LIMIT;
	    default: limit = 600;
	}
	if(limit != 0 && mermi > limit) {
		HataMesaji(playerid, "Bu silaha bu kadar mermi veremezsin. (maksimum: %d)", limit);
		return 1;
	}
	Silahver(oyuncuid, silahid, mermi);
	if(GetPlayerState(oyuncuid) == PLAYER_STATE_DRIVER) SetPlayerArmedWeapon(oyuncuid, 0);
	if(GetPlayerState(oyuncuid) == PLAYER_STATE_PASSENGER && silahid != WEAPON_UZI && silahid != WEAPON_MP5 && silahid != WEAPON_AK47 && silahid != WEAPON_M4 && silahid != WEAPON_TEC9) SetPlayerArmedWeapon(oyuncuid, 0);
	YoneticiMesaji(RENK_ACIKKIRMIZI, "%s adli kisiye %s model silahi %d mermiyle verdin.", OyuncuIsim(oyuncuid), SilahIsim(silahid), mermi);
	YoneticiMesaji(RENK_ACIKKIRMIZI, "%s adli yetkili sana %s model silahi %d mermiyle verdi.", OyuncuBilgi[playerid][oAdminNick], SilahIsim(silahid), mermi);
	new oyuncu_isim[MAX_PLAYER_NAME + 1];
	GetPlayerName(oyuncuid, oyuncu_isim, sizeof(oyuncu_isim));
	    new string[120];
format(string, sizeof(string), "[%s] %s(%d), %s(%d) adli kisiye %s model silah verdi.", Tarih(), OyuncuBilgi[playerid][oAdminNick], OyuncuBilgi[playerid][oID], oyuncu_isim, OyuncuBilgi[oyuncuid][oID], SilahIsim(silahid));
AdminLogDCM(string, "1023211032891629588", "Give-Weapon");
	//format(log_icerik, sizeof(log_icerik), "[%s] %s(%d), %s(%d) adli kisiye %s model silah verdi.", Tarih(), OyuncuBilgi[playerid][oAdminNick], OyuncuBilgi[playerid][oID], oyuncu_isim, OyuncuBilgi[oyuncuid][oID], SilahIsim(silahid));
	//Gunluk_LogKaydetKaydet(log_icerik);
	//Log_Kaydet("loglar/admin_silah.txt", "%s", log_icerik);
	return 1;
}

