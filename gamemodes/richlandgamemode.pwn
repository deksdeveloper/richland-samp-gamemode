CMD:cop(playerid)
{
    new id = -1;
    if ((id = Cop_Yakin(playerid)) == -1) return HataMesaji(playerid, "Yakinlarinizde cop mevcut degil.");
    if(CLData[id][Exists] == false) return HataMesaji(playerid, "Hata olustu. Kod-1");
    if(OyuncuBilgi[playerid][oMeslek] != 12) return HataMesaji(playerid, "Copculuk mesleginde degilsiniz.");
    if(OyuncuBilgi[playerid][pTasinanCop] != -1) return HataMesaji(playerid, "Zaten bir cop tasiyorsun, eger hata oldugunu dusunuyorsan relog atabilirsin.");
