CMD:durakekle(playerid, params[])
{
	new name[30];
	static fiyat, sure;
	if (!IsPlayerConnected(playerid)) return 1;
    if (OyuncuBilgi[playerid][oAdmin] < 6) return HataMesaji(playerid, "Bu komutu kullanmak icin yetkiniz yok.");
    if (sscanf(params, "dds[30]", fiyat, sure, name)) return KullanimMesaji(playerid, "/durakekle [Fiyat] [Sure] [Isim]");
    if (strlen(name) > 30) return HataMesaji(playerid, "Durak Adi 30 karakteri gecemez.");
    new Float:pos[3], Float:angle;
    GetPlayerPos(playerid, pos[0], pos[1], pos[2]);
    GetPlayerFacingAngle(playerid, angle);
    new id = Durak_Ekle(name, pos[0], pos[1], pos[2], fiyat, sure, angle);
    if (id == -1) return HataMesaji(playerid, "Sunucu maksimum durak limitine ulasti.");
    SunucuMesaji(playerid, "Durak eklendi, ID: %d", id);
    SendClientMessageToAllEx(COLOR_LIGHTRED, "AdmLog: %s, %d nolu duragi olusturdu.", OyuncuBilgi[playerid][oAdminNick], id);
	return 1;
}

