	OyuncuBilgi[playerid][oYarali] = 0, BayginlikBitir(playerid);
	CanDuzenle(playerid, 100);
	ParaEkle(playerid, -gerekliucret);
	SunucuMesaji(playerid, "Tedavi islemi basarili, artik tamamen iyilestin ve taburcu oldun. Sagligina dikkat et.");
	return 1;
}

