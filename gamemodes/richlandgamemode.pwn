stock ConvertTime(sure)
{
    new gun, saat, dakika, saniye, string[64];
    gun = sure / 86400;
    saat = (sure / 3600) - (gun * 24);
    dakika = (sure / 60) - (gun * 1440) - (saat * 60);
    saniye = sure % 60;
    format(string, sizeof(string), "%d gun %d saat %d dakika %d saniye", gun, saat, dakika, saniye);
    return string;
}

