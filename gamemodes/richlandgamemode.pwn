	if(!OyuncuBilgi[oyuncuid][oGiris]) return HataMesaji(playerid, "Oyuncu giris yapmamis, komut uzerinde kullanilamaz.");
	if(OyuncuBilgi[oyuncuid][oMaskeDurum] == 0) return HataMesaji(playerid, "Oyuncu zaten maske takmamis.");
	if(OyuncuBilgi[oyuncuid][oKelepceli] != 1 && OyuncuBilgi[oyuncuid][oYaraliSure] < 1 && OyuncuBilgi[playerid][oDonduruldu] == 0) return HataMesaji(playerid, "Girilen oyuncu yarali veya etkisiz halde degil, komut uzerinde kullanilamaz.");

	OyuncuBilgi[oyuncuid][oMaskeDurum] = 0;
	IsimLabelGuncelle(oyuncuid);
	new emote[64];
	format(emote, sizeof(emote), "%s adli kisinin maskesini yuzunden cikartir.", OyuncuIsim(oyuncuid));
	cmd_me(playerid, emote);
	new log_icerik[144], oyuncu_isim[MAX_PLAYER_NAME + 1], hedef_isim[MAX_PLAYER_NAME + 1];
	GetPlayerName(playerid, oyuncu_isim, sizeof(oyuncu_isim));
	GetPlayerName(oyuncuid, hedef_isim, sizeof(hedef_isim));
	format(log_icerik, sizeof(log_icerik), "[%s] %s(SID: %d), %s(SID: %d) adli kisinin maskesini cikardi.", Tarih(), oyuncu_isim, OyuncuBilgi[playerid][oID], hedef_isim, OyuncuBilgi[oyuncuid][oID]);
	//Gunluk_LogKaydetKaydet(log_icerik);
	return 1;
}

