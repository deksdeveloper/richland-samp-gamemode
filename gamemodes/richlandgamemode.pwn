		format(bilgi, sizeof(bilgi), "{FFFFFF}%s adli mesleginden ayrilmak istiyor musun?\n\n{B70000}UYARI: {FFFFFF}Bu islemin geri donusu yoktur, lutfen islemlerinizi dikkatli yapiniz.\n{FFFFFF}Ayrilma isleminin sonunda hicbir iade alamayacaksiniz.", MeslekIsim(OyuncuBilgi[playerid][oMeslek]));
		DialogGoster(playerid, DIALOG_MESLEKAYRIL, DIALOG_STYLE_MSGBOX, "RichMeslek - Ayrilma", bilgi, "Evet", "Hayir");
	    return 1;
	}

	if(Meslek_Yakin(playerid) == -1) return HataMesaji(playerid, "Bir meslek noktasina yakin degilsin.");

	new id = Meslek_Yakin(playerid);
	if(MeslekBilgi[id][mTur] == 6) {
	    if(OyuncuBilgi[playerid][oSeviye] < 10) return HataMesaji(playerid, "Bu meslege girebilmek icin en az 10 seviye olmalisin.");
	    if(OyuncuBilgi[playerid][oAvukatLisans] == 0) return HataMesaji(playerid, "Bu meslege girebilmek icin Avukatlik Lisansina ihtiyacin var.");
	}
	if(MeslekBilgi[id][mTur] == 4) {
	    if(OyuncuBilgi[playerid][oSeviye] < 5) return HataMesaji(playerid, "Hirsiz meslegine girmek icin en az 5 seviye olmalisin.");
	    if(OyuncuBirlikTuru(playerid) == BIRLIK_POLIS || OyuncuBirlikTuru(playerid) == BIRLIK_MEDIKAL || OyuncuBirlikTuru(playerid) == BIRLIK_DEVLET) return HataMesaji(playerid, "Devlet kurumlari bu meslegi kullanamaz.");
	    new icerik[256];
	    format(icerik, sizeof(icerik), "{FFFFFF}Bu meslege katilmak icin {33aa33}%s {FFFFFF}odeyeceksin, katilmak istiyor musun?", NumaraFormati(HIRSIZ_MESLEK_UCRET));
	    DialogGoster(playerid, DIALOG_MESLEK_HIRSIZ, DIALOG_STYLE_MSGBOX, "RichMeslek - Katil", icerik, "Evet", "Hayir");
		return 1;
	}
	if(MeslekBilgi[id][mTur] == 3) {
	    if(OyuncuBilgi[playerid][oSeviye] < 5) return HataMesaji(playerid, "Mekanik meslegine girmek icin en az 5 seviye olmalisin.");
	    if(OyuncuBirlikTuru(playerid) == BIRLIK_POLIS || OyuncuBirlikTuru(playerid) == BIRLIK_MEDIKAL || OyuncuBirlikTuru(playerid) == BIRLIK_DEVLET) return HataMesaji(playerid, "Devlet kurumlari bu meslegi kullanamaz.");
	    new icerik[256];
	    format(icerik, sizeof(icerik), "{FFFFFF}Bu meslege katilmak icin {33aa33}%s {FFFFFF}odeyeceksin, katilmak istiyor musun?", NumaraFormati(MEKANIK_MESLEK_UCRET));
	    DialogGoster(playerid, DIALOG_MESLEK_MEKANIK, DIALOG_STYLE_MSGBOX, "RichMeslek - Katil", icerik, "Evet", "Hayir");
		return 1;
	}
	OyuncuBilgi[playerid][oMeslek] = MeslekBilgi[id][mTur];
	SunucuMesaji(playerid, "%s meslegine katildin, /yardim yazarak meslek komutlarini goruntuleyebilirsin.", MeslekIsim(OyuncuBilgi[playerid][oMeslek]));
	SunucuMesaji(playerid, "Meslekten ayrilmak icin tekrar /meslek komutunu kullanmaniz yeterlidir.");
	return 1;
}



CMD:avukatsifirla(playerid, params[])
{
