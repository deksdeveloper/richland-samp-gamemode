stock Cop_Yukle()
{
    mysql_query(mysqlB, "SELECT * FROM `coplokasyonlari`");

	new rows = cache_num_rows();
    printf("Toplam %d Cop Lokasyonu yuklenmistir.", rows);

	if (rows)
	{
	    new lkID;
		for (new i = 0; i < rows; i ++) if (i < MAX_CL)
	    {
	        cache_get_value_name_int(i, "ID", lkID);

			if(IsValidDynamicObject(CLData[lkID][copObje])) DestroyDynamicObject(CLData[lkID][copObje]), CLData[lkID][copObje] = INVALID_OBJECT_ID;
            if (IsValidDynamic3DTextLabel(CLData[lkID][copLabel])) DestroyDynamic3DTextLabel(CLData[lkID][copLabel]);
	        CLData[lkID][Exists] = true;
	        CLData[lkID][ID] = lkID;

	        cache_get_value_name_float(i, "posX", CLData[lkID][copPos][0]);
	        cache_get_value_name_float(i, "posY", CLData[lkID][copPos][1]);
	        cache_get_value_name_float(i, "posZ", CLData[lkID][copPos][2]);
	        cache_get_value_name_float(i, "Angle", CLData[lkID][copPos][3]);
	        cache_get_value_name_int(i, "ObjeID", CLData[lkID][copObjeID]);
	        cache_get_value_name_int(i, "Kalite", CLData[lkID][copKalite]);
            CLData[lkID][copDurum] = COP_TOPLANMAMIS;
            CLData[lkID][copObje] = CreateDynamicObject(CLData[lkID][copObjeID], CLData[lkID][copPos][0], CLData[lkID][copPos][1], CLData[lkID][copPos][2], 0.0, 0.0, CLData[lkID][copPos][3], 0, 0);
            CLData[lkID][copLabel] = CreateDynamic3DTextLabel("COP\n"GR"Toplanmamis", 0x33CCFFAA, CLData[lkID][copPos][0], CLData[lkID][copPos][1], CLData[lkID][copPos][2]+0.5, 10.0);
		    Iter_Add(CLIT, lkID);
		    CopAktor = CreateDynamicActor(234, 517.9634, -1841.0005, 4.8977, 178.3068);
		    SetDynamicActorVirtualWorld(CopAktor, 0);
		    SetDynamicActorFacingAngle(CopAktor, 178.3068);
            CreateDynamic3DTextLabel("Rusty Pail\nEtkilesim: ALT", RENK_SERVER, 517.9634, -1841.0005, 4.8977, 5.0, INVALID_PLAYER_ID, INVALID_VEHICLE_ID, 1, -1);
		    CL_Yenile(lkID);
	    }
	}
    return 1;
}


stock CL_Sil(id)
{
	new query[100];
	format(query, sizeof(query), "DELETE FROM `coplokasyonlari` WHERE `ID` = '%d'", CLData[id][ID]);
	mysql_query(mysqlB, query, false);
	CLData[id][Exists] = false;
	if(IsValidDynamicObject(CLData[id][copObje])) DestroyDynamicObject(CLData[id][copObje]), CLData[id][copObje] = INVALID_OBJECT_ID;
	if(IsValidDynamic3DTextLabel(CLData[id][copLabel])) DestroyDynamic3DTextLabel(CLData[id][copLabel]), CLData[id][copLabel] = Text3D:INVALID_3DTEXT_ID;

	CLData[id][copPos][0] = 0.0;
	CLData[id][copPos][1] = 0.0;
	CLData[id][copPos][2] = 0.0;
	CLData[id][copPos][3] = 0.0;
	CLData[id][copObjeID] = -1;
	CLData[id][copKalite] = -1;
	Iter_Remove(CLIT, id);
	return 1;
}

stock CL_Ekle(Float:posX, Float:posY, Float:posZ, Float:angle, model, kalite)
{
    new i = Iter_Free(CLIT);

	if (i != -1)
	{
	    if (CLData[i][Exists] == false)
	    {
	        CLData[i][Exists] = true;

			CLData[i][ID] = i;

	        CLData[i][copPos][0] = posX;
	        CLData[i][copPos][1] = posY;
	        CLData[i][copPos][2] = posZ;
	        CLData[i][copPos][3] = angle;
	        CLData[i][copObjeID] = model;
	        CLData[i][copKalite] = kalite;

            CLData[i][copObje] = CreateDynamicObject(CLData[i][copObjeID], CLData[i][copPos][0], CLData[i][copPos][1], CLData[i][copPos][2], 0.0, 0.0, CLData[i][copPos][3], 0, 0);
	        new query[256];
			mysql_format(mysqlB, query, sizeof(query), "INSERT INTO `coplokasyonlari` (`ID`, `posX`, `posY`, `posZ`, `Angle`, `ObjeID`, `Kalite`) VALUES ('%d', '%.4f', '%.4f', '%.4f', '%.4f', '%d', '%d')", i, posX, posY, posZ, angle, model, kalite);
			mysql_query (mysqlB, query);
			Iter_Add(CLIT, i);
			return i;
	    }
	}
	return -1;
}

stock CL_Yenile(copid)
{
	if(copid != -1 && CLData[copid][Exists])
	{
	    if(IsValidDynamicObject(CLData[copid][copObje])) DestroyDynamicObject(CLData[copid][copObje]), CLData[copid][copObje] = INVALID_OBJECT_ID;
        if(IsValidDynamic3DTextLabel(CLData[copid][copLabel])) DestroyDynamic3DTextLabel(CLData[copid][copLabel]), CLData[copid][copLabel] = Text3D:INVALID_3DTEXT_ID;

        new str[64];
        if(CLData[copid][copDurum] == COP_TOPLANMAMIS) format(str, 64, "COP\n"GR"Toplanmamis");
        else if(CLData[copid][copDurum] == COP_TOPLANMIS) format(str, 64, "COP\n"R"Toplanmis");
        CLData[copid][copLabel] = CreateDynamic3DTextLabel(str, 0x33CCFFAA, CLData[copid][copPos][0], CLData[copid][copPos][1], CLData[copid][copPos][2]+0.6, 10.0);

        if(CLData[copid][copDurum] == COP_TOPLANMAMIS) CLData[copid][copObje] = CreateDynamicObject(CLData[copid][copObjeID], CLData[copid][copPos][0], CLData[copid][copPos][1], CLData[copid][copPos][2], 0.0, 0.0, CLData[copid][copPos][3], 0, 0);
		return 1;
	}
	return 0;
}

stock CL_Kaydet(copid)
{
	static query[512];
	format(query, sizeof(query), "UPDATE `coplokasyonlari` SET `posX` = '%.4f', `posY` = '%.4f', `posZ` = '%.4f', `Angle` = '%.4f', `ObjeID` = '%d', `Kalite` = '%d' WHERE `ID` = '%d'",
		CLData[copid][copPos][0],
	    CLData[copid][copPos][1],
	    CLData[copid][copPos][2],
	    CLData[copid][copPos][3],
	    CLData[copid][copObjeID],
	    CLData[copid][copKalite],
	    CLData[copid][ID]
	);
	return mysql_tquery(mysqlB, query);
}

stock Cop_Yakin(playerid)
{
    for (new i = 0; i != MAX_CL; i ++) if (GetPlayerVirtualWorld(playerid) == 0 && IsPlayerInRangeOfPoint(playerid, 5.0, CLData[i][copPos][0], CLData[i][copPos][1], CLData[i][copPos][2]))
	{
			return i;
	}
	return -1;
}

public OnQueryError(errorid, const error[], const callback[], const query[], MySQL:handle)
{
	foreach(new i : Player) if(OyuncuBilgi[i][oGiris] == true && OyuncuBilgi[i][oAdmin] > 1)
	{
        SendClientMessageEx(i, RENK_TURUNCU, "SQL HATASI - CALLBACK: %s", callback);
		SendClientMessageEx(i, RENK_TURUNCU, "HATA: (#%d) %s", errorid, error);
		SendClientMessageEx(i, RENK_TURUNCU, "SORGU: %s", query);
	}
	return 1;
}

