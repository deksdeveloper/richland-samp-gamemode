 	    if(!response) return false;

		 if(listitem == 0) {
			switch(OyuncuBilgi[playerid][oPMDurum]) {
				case false: OyuncuBilgi[playerid][oPMDurum] = true, TogMenu(playerid);
				case true: OyuncuBilgi[playerid][oPMDurum] = false, TogMenu(playerid);
			}
 	    }
 	    else if(listitem == 1) {
			switch(OyuncuBilgi[playerid][oNewbieDurum]) {
				case false: OyuncuBilgi[playerid][oNewbieDurum] = true, TogMenu(playerid);
				case true: OyuncuBilgi[playerid][oNewbieDurum] = false, TogMenu(playerid);
			}
		}
		else if(listitem == 2) {
			switch(OyuncuBilgi[playerid][oBoomboxDurum]) {
				case false: OyuncuBilgi[playerid][oBoomboxDurum] = true, TogMenu(playerid);
				case true: {
				    if(OyuncuBilgi[playerid][oBoombox] != INVALID_PLAYER_ID) OyuncuBilgi[playerid][oBoombox] = INVALID_PLAYER_ID, StopAudioStreamForPlayer(playerid);
					OyuncuBilgi[playerid][oBoomboxDurum] = false, TogMenu(playerid);
				}
			}
		}
		else if(listitem == 3) {
		    switch(OyuncuBilgi[playerid][oKonusmaAnim]) {
		        case false: OyuncuBilgi[playerid][oKonusmaAnim] = true, TogMenu(playerid);
		        case true: OyuncuBilgi[playerid][oKonusmaAnim] = false, TogMenu(playerid);
		    }
		}
		else if(listitem == 4) {
			if(OyuncuBilgi[playerid][oAdminDurum] == true) return HataMesaji(playerid, "Yonetici isbasindayken bu secenegi kullanamazsin.");
			if(OyuncuBilgi[playerid][oOynamaSaati] < 8) return HataMesaji(playerid, "Referans sistemini kullanmak icin en az 8 oynama saatine ihtiyacin var."), TogMenu(playerid);
			if(!isnull(OyuncuBilgi[playerid][oReferans])) return HataMesaji(playerid, "Hesabina zaten bir referans belirlemissin."), TogMenu(playerid);

			return DialogGoster(playerid, DIALOG_REFERANS, DIALOG_STYLE_INPUT, "Referans Sistemi", "{FFFFFF}Lutfen size referans olan kisinin bir karakter adini giriniz;", "Sec", "Kapat");
		}
		else if(listitem == 5) {
		    if(OyuncuBilgi[playerid][oAdminDurum] == true) return HataMesaji(playerid, "Admin isbasindayken hediye alamazsin.");
			if(OyuncuBilgi[playerid][oHediye] < 5) return HataMesaji(playerid, "Hediye alinabilir durumda degil. (%d/5 maas kaldi)", OyuncuBilgi[playerid][oHediye]);

			new kazanc = randomEx(500, 1000);
			SunucuMesaji(playerid, "Hediye sisteminden {33aa33}%s {FFFFFF}kazandin.", NumaraFormati(kazanc));
			ParaEkle(playerid, kazanc);
			OyuncuBilgi[playerid][oHediye] = 0;
			OyuncuKaydet(playerid);
			new log_icerik[144], oyuncu_isim[MAX_PLAYER_NAME + 1];
			GetPlayerName(playerid, oyuncu_isim, sizeof(oyuncu_isim));
			format(log_icerik, sizeof(log_icerik), "[%s] %s hediye sisteminden %s kazandi.", Tarih(), oyuncu_isim, NumaraFormati(kazanc));
			//Gunluk_LogKaydetKaydet(log_icerik);
			Log_Kaydet("loglar/hediye_log.txt", "%s", log_icerik);
		}
		else if(listitem == 6) {
			if(OyuncuBilgi[playerid][oAdminDurum] == true) return HataMesaji(playerid, "Yonetici isbasindayken bu secenegi kullanamazsin.");
			return BildirimListesi(playerid);
		}
		else if(listitem == 7) {
			switch(OyuncuBilgi[playerid][oBirlikKanal]) {
				case false: OyuncuBilgi[playerid][oBirlikKanal] = true, TogMenu(playerid);
				case true: OyuncuBilgi[playerid][oBirlikKanal] = false, TogMenu(playerid);
			}
		} 		
	return 1;
}

CMD:adekor(playerid, params[])
{
	if(OyuncuBilgi[playerid][oAdmin] < 5) return YetkinizYok(playerid);

	new veriler[256 * MAKSIMUM_DEKOR], dekorucret[64];
	format(veriler, sizeof(veriler), "Dekor Ekle\t\n");
	for(new i = 0; i != MAKSIMUM_DEKOR; i ++) if(DekorListesi[i][dekorKullanimda]) {
		switch(DekorListesi[i][dekorOOC]) {
			case 1: format(dekorucret, sizeof(dekorucret), "%sTL", NumaraFormati(DekorListesi[i][dekorUcret], ""));
			default: format(dekorucret, sizeof(dekorucret), "%s", NumaraFormati(DekorListesi[i][dekorUcret]));
		}
		format(veriler, sizeof(veriler), "%s%s\t(%s)\n", veriler, DekorListesi[i][dekorIsim], dekorucret);
	}
	DialogGoster(playerid, DIALOG_ADMIN_DEKORLAR, DIALOG_STYLE_TABLIST, "> Dekor Duzenle", veriler, "Sec", "Kapat");
	return 1;
}

