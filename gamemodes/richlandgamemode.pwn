	}
	if(AracBilgi[aracid][aracModel] == 528 || AracBilgi[aracid][aracModel] == 427) {
		if(OyuncuBilgi[oyuncuid][oBirim] == 0) return HataMesaji(oyuncuid, "SWAT biriminde olmadigin icin bu aracin bagajini kullanamazsin."), SetTrunkStatus(AracBilgi[aracid][aracYarat], false);
		for(new i = 0; i != MAKSIMUM_BIRIM; i ++) if(BirimBilgi[i][birimID] == OyuncuBilgi[oyuncuid][oBirim])
			if(!!strcmp(BirimBilgi[i][birimAd], "SWAT", true)) return HataMesaji(oyuncuid, "SWAT biriminde olmadigin icin bu aracin bagajini kullanamazsin."), SetTrunkStatus(AracBilgi[aracid][aracYarat], false);
	}
