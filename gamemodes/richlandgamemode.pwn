	SetPlayerVirtualWorld(oyuncuid, oyuncuid);
	OyuncuPosDegistir(oyuncuid, 1973.3918, 1154.0151, -4.4788);
	SetPlayerFacingAngle(oyuncuid, 0);
	OyuncuBilgi[oyuncuid][oKontrol] = false;
	TogglePlayerControllable(oyuncuid, false);
