	sayi = 0;
	foreach(new i: Player) if(YakinlikKontrol(playerid, i, 20.0) && OyuncuBilgi[i][oGiris] == true)
	{
	    if(OyuncuBilgi[i][oOynamaSaati] < 12) continue;
