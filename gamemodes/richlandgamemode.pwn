				case 3: if(Mobilya_Sayi(id) >= 150) return HataMesaji(playerid, "Daha fazla mobilya ekleyebilmek icin ev levelinizi yukseltmeniz gerekiyor.");
				case 4: if(Mobilya_Sayi(id) >= 200) return HataMesaji(playerid, "Daha fazla mobilya ekleyebilmek icin ev levelinizi yukseltmeniz gerekiyor.");
				case 5: if(Mobilya_Sayi(id) >= 250) return HataMesaji(playerid, "Daha fazla mobilya ekleyebilmek icin sinirsiz paket almalisiniz.");
				default: if(Mobilya_Sayi(id) >= 50) return HataMesaji(playerid, "Daha fazla mobilya ekleyebilmek icin ev levelinizi yukseltmeniz gerekiyor.");
			}
			Dialog_Show(playerid, DIALOG_MOBILYA_EKLE, DIALOG_STYLE_TABLIST, "Richland Roleplay: Mobilya Ekle", "Sandalyeler\nKoltuklar\nYataklar\nBanyo Esyalari\nElektronik Esyalari\nMutfak Esyalari\nMasalar\nDiger Esyalar\nTum Mobilyalar\n{B70000}Obje ID'si Kullanarak Ekle", "Sec", "<< Geri");
		}
		else if(!strcmp(inputtext, "Mobilya Duzenle", true)) return Dialog_Show(playerid, DIALOG_MOBILYA_DSECIM, DIALOG_STYLE_TABLIST, "Richland Roleplay: Mobilya Duzenleme", "Listeden Sec\nElle Sec", "Sec", "Kapat");
