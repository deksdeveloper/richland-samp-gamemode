CMD:barikat(playerid, params[])
{
	if(OyuncuBilgi[playerid][oGiris] == false) return HataMesaji(playerid, "su anda bu komutu kullanamazsin.");
	if(OyuncuBirlikTuru(playerid) != BIRLIK_POLIS) return HataMesaji(playerid, "LSPD degilsin.");
    if(OyuncuBilgi[playerid][oIsbasi] == false) return HataMesaji(playerid,"isbasinda degilken bu komutu kullanamazsin.");
	if(IsPlayerInAnyVehicle(playerid)) return HataMesaji(playerid, "Aracta barikat kuramazsin.");
	if(GetPlayerSpecialAction(playerid) == SPECIAL_ACTION_DUCK) return HataMesaji(playerid, "Egilirken barikat kuramazsin.");
	if(GetPVarInt(playerid, "anim") != 0) return HataMesaji(playerid, "Animasyon yaparken barikat kuramazsin.");
	if(OyuncuBilgi[playerid][oAdminDurum] == true) return HataMesaji(playerid, "Yonetici isbasindayken barikat kuramazsin.");
	if(GetPlayerState(playerid) == PLAYER_STATE_SPECTATING || OyuncuBilgi[playerid][oIzlenen] != INVALID_PLAYER_ID) return HataMesaji(playerid, "izleme yaparken barikat kuramazsin.");

	new barikat_secim[sizeof(Barikatlar)];
	for(new c = 0; c != sizeof(Barikatlar); c++) barikat_secim[c] = Barikatlar[c][0];
	ShowModelSelectionMenuEx(playerid, barikat_secim, sizeof(barikat_secim), "Barikat", MODELLER_BARIKAT, 0.0, 0.0, 0.0);
	return true;
}

CMD:barikatkaldir(playerid, params[])
{
	if(OyuncuBilgi[playerid][oGiris] == false) return HataMesaji(playerid, "su anda bu komutu kullanamazsin.");
	if(OyuncuBirlikTuru(playerid) != BIRLIK_POLIS) return HataMesaji(playerid, "LSPD degilsin.");
    if(OyuncuBilgi[playerid][oIsbasi] == false) return HataMesaji(playerid,"isbasi durumunda degilken bu komutu kullanamazsin.");
	if(IsPlayerInAnyVehicle(playerid)) return HataMesaji(playerid, "Aractayken barikat kaldiramazsin.");
	if(GetPlayerSpecialAction(playerid) == SPECIAL_ACTION_DUCK) return HataMesaji(playerid, "Egilirken barikat kaldiramazsin.");
	if(GetPVarInt(playerid, "anim") != 0) return HataMesaji(playerid, "Anim kullanirken barikat kaldiramazsin.");
	if(OyuncuBilgi[playerid][oAdminDurum] == true) return HataMesaji(playerid, "Yonetici isbasindayken barikat kaldiramazsin.");
	if(GetPlayerState(playerid) == PLAYER_STATE_SPECTATING || OyuncuBilgi[playerid][oIzlenen] != INVALID_PLAYER_ID) return HataMesaji(playerid, "izleme yaparken barikat kaldiramazsin.");

	new id;
	if(sscanf(params, "d", id)) return KullanimMesaji(playerid, "/barikatkaldir [id]");
	if((id < 0 || id >= MAKSIMUM_BARIKAT) || !BarikatBilgi[id][barikatKullanimda]) return HataMesaji(playerid, "Gecersiz barikat!");

	if(!IsPlayerInRangeOfPoint(playerid, 3.0, BarikatBilgi[id][barikatPos][0], BarikatBilgi[id][barikatPos][1], BarikatBilgi[id][barikatPos][2]))
	    return HataMesaji(playerid, "Barikata yakin degilsin.");

	Barikat_Kaldir(id);
	SunucuMesaji(playerid, "Barikat kaldirildi. (ID: %d)", id);
	return true;
}

CMD:barikatlarikaldir(playerid, params[])
{
	if(OyuncuBilgi[playerid][oGiris] == false) return HataMesaji(playerid, "su anda bu komutu kullanamazsin.");
	if(OyuncuBirlikTuru(playerid) != BIRLIK_POLIS && OyuncuBilgi[playerid][oAdmin] < 3) return YetkinizYok(playerid);
	if(OyuncuBirlikTuru(playerid) == BIRLIK_POLIS && BirlikBilgi[OyuncuBilgi[playerid][oBirlik]][birlikMaksRutbe] != OyuncuBilgi[playerid][oBirlikRutbe] && OyuncuBilgi[playerid][oAdmin] < 3) return YetkinizYok(playerid);

	new sayi = 0;
	for(new i = 0; i != MAKSIMUM_BARIKAT; i ++) if(BarikatBilgi[i][barikatKullanimda]) Barikat_Kaldir(i), sayi++;
	if(sayi == 0) return HataMesaji(playerid, "Sunucuda barikat yok.");

	UyariMesaji(playerid, "%s adet barikat kaldirildi.", NumaraFormati(sayi, ""));
	return true;
}

