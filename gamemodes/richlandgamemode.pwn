CMD:anick(playerid, params[])
{
	if(OyuncuBilgi[playerid][oAdmin] < 5) return YetkinizYok(playerid);

	new oyuncuid, isim[20];
	if(sscanf(params, "us[20]", oyuncuid, isim)) return KullanimMesaji(playerid, "/anick [oyuncu isim/id] [nick]");
	if(!IsPlayerConnected(oyuncuid)) return HataMesaji(playerid, "Girilen kisi oyunda degil.");
	if(OyuncuBilgi[oyuncuid][oAdmin] < 0 && OyuncuBilgi[oyuncuid][oHelper] < 0) return HataMesaji(playerid, "Girilen kisi yetkili veya helper degil.");
	if(OyuncuBilgi[oyuncuid][oAdminDurum]) return HataMesaji(playerid, "Bu yonetici isbasinda ismini degistirebilmek icin isbasindan cikmalidir.");
	if(strlen(isim) < 3 || strlen(isim) > 20)
	{
		HataMesaji(playerid, "isim 3 karakterden az, 20 karakterden fazla olamaz.");
		return 1;
	}
	if(!strcmp(isim, "Yok", true))
		return HataMesaji(playerid, "Girilen yonetici ismi gecersizdir.");

	format(OyuncuBilgi[oyuncuid][oAdminNick], 20, "%s", isim);
	OyuncuKaydet(oyuncuid);
	SunucuMesaji(playerid, "%s adli kisinin yetkili ismini %s olarak degistirdin.", OyuncuIsim(oyuncuid), isim);
	SunucuMesaji(oyuncuid, "%s adli yetkili, yetkili ismini %s olarak degistirdi.", OyuncuBilgi[playerid][oAdminNick], isim);
	new log_icerik[144], oyuncu_isim[MAX_PLAYER_NAME + 1];
	GetPlayerName(oyuncuid, oyuncu_isim, sizeof(oyuncu_isim));
	format(log_icerik, sizeof(log_icerik), "[%s] %s(%d), %s(%d) adli kisinin yetkili ismini %s yapti.", Tarih(), OyuncuBilgi[playerid][oAdminNick], OyuncuBilgi[playerid][oID], oyuncu_isim, OyuncuBilgi[oyuncuid][oID], isim);
	//Gunluk_LogKaydetKaydet(log_icerik);
	Log_Kaydet("loglar/yonetim.txt", "%s", log_icerik);
	return 1;
}

