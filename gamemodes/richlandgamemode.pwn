public Asansor(playerid, kat)
{
	switch(kat) {
		case 0: OyuncuPosDegistir(playerid, 1493.4991, -1791.1809, 2981.3540), SetPlayerFacingAngle(playerid, 90);
		case 1: OyuncuPosDegistir(playerid, 1493.6021, -1786.3867, 2676.0129), SetPlayerFacingAngle(playerid, 90);
		case 2: OyuncuPosDegistir(playerid, 1482.5685, -1848.7686, 3645.6270), SetPlayerFacingAngle(playerid, 270);
		case 3: OyuncuPosDegistir(playerid, 1564.83, -1666.99, 28.39), SetPlayerFacingAngle(playerid, 90);
		case 4: OyuncuPosDegistir(playerid, 1565.4377, -1693.2151, 62.1910), SetPlayerFacingAngle(playerid, 0);
		case 5: OyuncuPosDegistir(playerid, 1569.3027, -1679.0232, 2113.0349), SetPlayerFacingAngle(playerid, 90);
		case 6: OyuncuPosDegistir(playerid, 1567.7587, -1672.6642, 2982.2800), SetPlayerFacingAngle(playerid, 270);
		case 7: OyuncuPosDegistir(playerid, 2267.9709, 2449.0500, -7.1953), SetPlayerFacingAngle(playerid, 180);
	}
	OyuncuBilgi[playerid][oAsansor] = -1;
	DondurulmaEtkisi(playerid);
	SetPlayerInterior(playerid, 1);

	if(kat == 3 || kat == 7) SetPlayerVirtualWorld(playerid, 0), SetPlayerInterior(playerid, 0);
	else SetPlayerVirtualWorld(playerid, 7001);
	return 1;
}

