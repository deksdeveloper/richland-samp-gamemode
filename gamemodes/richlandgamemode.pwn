CMD:tokalas(playerid, params[])
{
	if(OyuncuBilgi[playerid][oHapisSure][0] > 0) return HataMesaji(playerid, "OOC hapisteyken animasyon kullanamazsin.");
	if(OyuncuBilgi[playerid][oYaraliSure] > 0) return HataMesaji(playerid, "Yerde yaraliyken animasyon kullanamazsin.");
	if(OyuncuBilgi[playerid][oDonduruldu] > 0) return HataMesaji(playerid, "Dondurulma etkisindeyken animasyon kullanamazsin.");
	if(OyuncuBilgi[playerid][oKelepceli] == 1) return HataMesaji(playerid, "Kelepceliyken animasyon kullanamazsin.");

	new oyuncuid, stil;
	if(sscanf(params, "ud", oyuncuid, stil)) return KullanimMesaji(playerid, "/tokalas [oyuncu id/isim] [stil (1-10)]");
	if(!IsPlayerConnected(oyuncuid)) return HataMesaji(playerid, "Gecersiz oyuncu belirledin.");
	if(playerid == oyuncuid) return HataMesaji(playerid, "Kendinle tokalasamazsin.");
	if(!YakinlikKontrol(playerid, oyuncuid, 5.0)) return HataMesaji(playerid, "Belirledigin oyuncuya yeterince yakin degilsin.");
	if(stil < 1 || stil > 10) return HataMesaji(playerid, "Gecersiz tokalasma stili. (1-10)");

	OyuncuBilgi[oyuncuid][oTokalasIstek] = playerid;
	OyuncuBilgi[oyuncuid][oTokalasStil] = stil;
	SunucuMesaji(oyuncuid, "%s adli oyuncu seninle el sikismak istiyor. (/kabul tokalas)", OyuncuIsim(playerid));
	SunucuMesaji(playerid, "%s adli oyuncuya tokalasma istegin iletildi.", OyuncuIsim(oyuncuid));
	return 1;
}



