CMD:coplokasyonuekle(playerid, params[])
{
	if (!IsPlayerConnected(playerid)) return 1;
    if (OyuncuBilgi[playerid][oAdmin] < 6) return HataMesaji(playerid, "Bu komutu kullanmak icin yetkiniz yok.");
    new cop = -1;
    if ((cop = Cop_Yakin(playerid)) != -1) return HataMesaji(playerid, "Yakinlarinizde cop mevcut. Farkli bir yere eklemeyi deneyin.");
	new Float:pos[3], Float:angle, model, kalite;
    if(sscanf(params, "ii", model, kalite)) return KullanimMesaji(playerid, "/coplokasyonuekle [Obje ID] [Esya Kalitesi 0,1,2,3]"), SunucuMesaji(playerid, "0 (Kalitesiz): Hic para vermez. - 1(Dusuk Kalite): Dusuk miktarda para verir. - 2(Orta Kalite): Orta miktarda para verir. - 3(Yuksek Kalite): Yuksek miktarda (?) para verir.");
    if(kalite < 0 || kalite > 3) return HataMesaji(playerid, "Hatali kalite!");
    GetPlayerPos(playerid, pos[0], pos[1], pos[2]);
    GetPlayerFacingAngle(playerid, angle);
    new id = CL_Ekle(pos[0], pos[1], pos[2], angle, model, kalite);
    if (id == -1) return HataMesaji(playerid, "Sunucu maksimum cop lokasyonu limitine ulasti.");
    SunucuMesaji(playerid, "Cop Lokasyonu eklendi, ID: %d", id);
    SendClientMessageToAllEx(COLOR_LIGHTRED, "AdmLog: %s, %d nolu Cop Lokasyonunu olusturdu.", OyuncuBilgi[playerid][oAdminNick], id);
	return 1;
}

