CMD:park(playerid, params[])
{
	if(!IsPlayerInAnyVehicle(playerid)) return HataMesaji(playerid, "Herhangi bir aracin icerisinde degilsin.");
	if(OyuncuBilgi[playerid][oGiris] == false) return HataMesaji(playerid, "su anda bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oDonduruldu] > 0) return HataMesaji(playerid, "su anda bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oHapisSure][0] > 0 || OyuncuBilgi[playerid][oHapisSure][1] > 0) return HataMesaji(playerid, "Hapiste bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oYarali] > 0) return HataMesaji(playerid, "Yaraliyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oAdminDurum]) return HataMesaji(playerid, "Yonetici isbasindayken bu komutu kullanamazsin.");

	new aracid = GetPlayerVehicleID(playerid), Float:hasar;
	if((aracid = Arac_IDBul(GetPlayerVehicleID(playerid))) != -1 && Arac_SahipKontrol(playerid, aracid))
	{
	    if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return HataMesaji(playerid, "Surucu koltugunda degilsin.");
		if(GetEngineStatus(GetPlayerVehicleID(playerid)) != 0 && BisikletKontrol(GetPlayerVehicleID(playerid)) == 0) return HataMesaji(playerid, "Park etmek icin aracinin motorunu kapatmalisin.");

		GetVehicleHealth(GetPlayerVehicleID(playerid), hasar);
		if(hasar < 800.0) return HataMesaji(playerid, "Bu arac hasarli, park etmek icin araci tamir ettirmelisin.");

		GetVehiclePos(AracBilgi[aracid][aracYarat], AracBilgi[aracid][aracPos][0], AracBilgi[aracid][aracPos][1], AracBilgi[aracid][aracPos][2]);
		GetVehicleZAngle(AracBilgi[aracid][aracYarat], AracBilgi[aracid][aracPos][3]);
		Arac_Kaydet(aracid);
		SunucuMesaji(playerid, "%s model aracini park ettin.", AracModel(AracBilgi[aracid][aracModel]));
	 	return 1;
	}
	if((aracid = Arac_IDBul(GetPlayerVehicleID(playerid))) != -1 && AracBilgi[aracid][aracBirlik] == OyuncuBilgi[playerid][oBirlikSQLID])
	{
	    if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return HataMesaji(playerid, "Surucu koltugunda degilsin.");
		if(GetEngineStatus(GetPlayerVehicleID(playerid)) != 0 && BisikletKontrol(GetPlayerVehicleID(playerid)) == 0) return HataMesaji(playerid, "Park etmek icin aracinin motorunu kapatmalisin.");
		if(BirlikBilgi[OyuncuBilgi[playerid][oBirlik]][birlikMaksRutbe] != OyuncuBilgi[playerid][oBirlikRutbe]) return HataMesaji(playerid, "Bu komutu sadece birlik lideri kullanabilir.");
		GetVehicleHealth(GetPlayerVehicleID(playerid), hasar);
		if(hasar < 800.0) return HataMesaji(playerid, "Bu arac hasarli, park etmek icin araci tamir ettirmelisin.");

		GetVehiclePos(AracBilgi[aracid][aracYarat], AracBilgi[aracid][aracPos][0], AracBilgi[aracid][aracPos][1], AracBilgi[aracid][aracPos][2]);
		GetVehicleZAngle(AracBilgi[aracid][aracYarat], AracBilgi[aracid][aracPos][3]);
		Arac_Kaydet(aracid);
	 	SunucuMesaji(playerid, "%s model birlik aracini park ettin.", AracModel(AracBilgi[aracid][aracModel]));
	 	return 1;
	}
	else HataMesaji(playerid, "Bu arac sana ait degil.");
	return 1;
}

CMD:duzkontak(playerid, params[])
{
	if(OyuncuBilgi[playerid][oMeslek] != 3) return HataMesaji(playerid, "Mekanik mesleginde degilsin.");
	new vehicleid = GetPlayerVehicleID(playerid);
	if(!IsEngineVehicle(vehicleid)) return HataMesaji(playerid, "Herhangi bir aracta degilsin.");
	if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return HataMesaji(playerid, "Bu islemi yapabilmen icin surucu koltugunda olman gerekir!");
    if(OyuncuBilgi[playerid][oMotorCalistir]) return HataMesaji(playerid, "Zaten su anda motor calistiriyorsunuz!");
    if(OyuncuBilgi[playerid][oBenzinAliyor] != -1) return HataMesaji(playerid, "Benzin doldururken aracin motorunu calistiramazsin.");
    if(OyuncuBilgi[playerid][oDuzKontak] > 0) return HataMesaji(playerid, "Zaten duz kontak yapiyorsun.");
	if(OyuncuBilgi[playerid][oHirsizSure] > 0) {
		OyuncuBilgi[playerid][oHirsizSure] = 0;
		OyuncuBilgi[playerid][oHirsizArac] = INVALID_VEHICLE_ID;
		UyariMesaji(playerid, "Arac parcalatma duz kontak denedigin icin iptal edildi.");
	}
    if(BisikletKontrol(vehicleid) != 0 || BotKontrol(vehicleid) != 0 || UcakKontrol(vehicleid) != 0) return HataMesaji(playerid, "Bu araca duz kontak yapamazsin.");
	switch(GetEngineStatus(vehicleid))
	{
	    case false:
	    {
	        if((vehicleid = Arac_IDBul(vehicleid)) != -1) if(AracBilgi[vehicleid][aracBenzin] == 0) return HataMesaji(playerid, "Bu aracta benzin yok.");
	        if(AracBilgi[vehicleid][aracBirlik] > 0) return HataMesaji(playerid, "Bu araca duz kontak yapamazsiniz.");
	        UyariMesaji(playerid, "Duz kontak islemi basladi, %d saniye surecek.", DUZKONTAK_SURE);
	        OyuncuBilgi[playerid][oDuzKontak] = DUZKONTAK_SURE;
			OyuncuBilgi[playerid][oHirsizArac] = GetPlayerVehicleID(playerid);
		}
		case true: HataMesaji(playerid, "Bu aracin motoru zaten acik.");
	}
	return 1;
}

