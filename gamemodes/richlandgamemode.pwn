CMD:hesapla(playerid, params[])
{
	new islem[20], veri1, veri2;
	if(sscanf(params, "ds[20]d", veri1, islem, veri2))
	{
	 	KullanimMesaji(playerid, "/hesapla [veri1] [islem] [veri2]");
	    SendClientMessage(playerid, RENK_SARI, "[isLEMLER]:{FFFFFF} Topla (+), carp (*), Bol (/), cikar (-)");
		return 1;
	}
    if(veri2 == 0 || veri1 == 0) return HataMesaji(playerid, "Girilen veri sifir(0) olamaz.");
	if(strcmp(islem, "*",true) == 0)
	{
		SunucuMesaji(playerid, "%d carpi %d esittir %d", veri1, veri2, veri1 * veri2);
	}
	if(strcmp(islem, "/",true) == 0)
	{
		SunucuMesaji(playerid, "%d bolu %d esittir %d", veri1, veri2, veri1 / veri2);
	}
	if(strcmp(islem, "+",true) == 0)
	{
		SunucuMesaji(playerid, "%d arti %d esittir %d", veri1, veri2, veri1 + veri2);
	}
	if(strcmp(islem, "-",true) == 0)
	{
		SunucuMesaji(playerid, "%d eksi %d esittir %d", veri1, veri2, veri1 - veri2);
	}
	return 1;
}

CMD:kontrakt(playerid, params[])
{
	if(OyuncuBilgi[playerid][oHitman] < 1) {
		if(OyuncuBilgi[playerid][oTelefon] == 0 || OyuncuBilgi[playerid][oTelefonNumarasi] == 0) return HataMesaji(playerid, "Telefonun yok veya operatore bagli degil.");
		if(OyuncuBilgi[playerid][oTelefonDurum] == false) return HataMesaji(playerid, "Telefonun kapaliyken bu sisteme ulasamazsin.");

		DialogGoster(playerid, DIALOG_KONTRAKT_EKLE, DIALOG_STYLE_INPUT, "Hitman: Kontrakt Tut", "{FFFFFF}Lutfen kontrakt olusturmak istediginiz kisinin adini paylasin;", "Tamam", "Kapat");
		return 1;
	}
	new icerik[96];
	switch(OyuncuBilgi[playerid][oHitman]) {
		case 1: format(icerik, sizeof(icerik), "Kontrakt Listesi\n");
		case 2: format(icerik, sizeof(icerik), "Kontrakt Listesi\nuyeler\n");
	}
	DialogGoster(playerid, DIALOG_KONTRAKT, DIALOG_STYLE_TABLIST, "Hitman: Kontrakt Menu", icerik, "Sec", "Kapat");
	return 1;
}

CMD:hbirlik(playerid, params[])
{
	if(OyuncuBilgi[playerid][oHitman] == 0) return YetkinizYok(playerid);

	new mesaj[256 * MAKSIMUM_BIRLIK];
	format(mesaj, sizeof(mesaj), "ID\tisim\tTur\n");
	for(new i = 0; i != MAKSIMUM_BIRLIK; i ++) if(BirlikBilgi[i][birlikKullanimda] == true && (BirlikBilgi[i][birlikTur] == BIRLIK_POLIS || BirlikBilgi[i][birlikTur] == BIRLIK_MEDIKAL || BirlikBilgi[i][birlikTur] == BIRLIK_DEVLET))
	{
	    format(mesaj, sizeof(mesaj), "%s{FFFFFF}%d\t%s\t%s\n", mesaj, i, BirlikBilgi[i][birlikAd], birlikturadi(i));
	}
	format(mesaj, sizeof(mesaj), "%s{B70000}Birlik Sifirla\t\t\n", mesaj);
	DialogGoster(playerid, DIALOG_HITMAN_BIRLIK, DIALOG_STYLE_TABLIST_HEADERS, "Hitman: Birlik Secimi", mesaj, "Sec", "Kapat");
	return 1;
}

CMD:hdolap(playerid, params[])
{
	if(OyuncuBilgi[playerid][oHitman] == 0) return YetkinizYok(playerid);
	if(!IsPlayerInRangeOfPoint(playerid, 3.0, 1261.7869, -795.8909, 1084.2415)) return HataMesaji(playerid, "Hitman dolabina yakin degilsin.");
	if(OyuncuBilgi[playerid][oAdminDurum] == true) return HataMesaji(playerid, "Yetkili isbasinda bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oHapisSure][0] > 0 || OyuncuBilgi[playerid][oHapisSure][1] > 0) return HataMesaji(playerid, "Hapisteyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oYarali] > 0 || OyuncuBilgi[playerid][oYaraliSure] > 0) return HataMesaji(playerid, "Yaraliyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oDonduruldu] > 0) return HataMesaji(playerid, "Etkisiz haldeyken bu komutu kullanamazsin.");
	if(IsPlayerInAnyVehicle(playerid)) return HataMesaji(playerid, "Bu komut arac icerisinde kullanilamaz.");

	DialogGoster(playerid, DIALOG_HITMANDOLAP, DIALOG_STYLE_TABLIST, "Hitman Menusu", "Ekipmanlar\nSahte Level\nSahte isim\nSilah Sifirla", "Sec", "Kapat");
	return 1;
}

CMD:ktamamla(playerid, params[])
{
	if(OyuncuBilgi[playerid][oHitman] == 0) return 0;

	new oyuncuid;
	if(sscanf(params, "u", oyuncuid)) return KullanimMesaji(playerid, "/ktamamla [oyuncu id/isim]");
	if(!IsPlayerConnected(oyuncuid)) return HataMesaji(playerid, "Gecersiz oyuncu.");
	if(!YakinlikKontrol(playerid, oyuncuid, 4.0)) return HataMesaji(playerid, "Oyuncuya yakin degilsin.");

	for(new i = 0; i != MAKSIMUM_KONTRAKT; i++) if(KontraktVeri[i][kontraktKullanimda] == true && !strcmp(KontraktVeri[i][kontraktIsim], OyuncuBilgi[oyuncuid][oIsim], true)) {
	    if(OyuncuBilgi[oyuncuid][oYarali] == 0 && OyuncuBilgi[oyuncuid][oYaraliSure] == 0) return HataMesaji(playerid, "Oyuncu yarali durumda degil, gorevini tamamlamak icin onu yaralamalisin.");

		SunucuMesaji(playerid, "Gorevini basariyla tamamladin, %s banka hesabina yatirildi.", NumaraFormati(KontraktVeri[i][kontraktUcret]));
		OyuncuBilgi[playerid][oBankaPara] += KontraktVeri[i][kontraktUcret];
		OyuncuBilgi[oyuncuid][oCK] = 1;
		if(OyuncuBilgi[oyuncuid][oHapisSure][0] > 0) OyuncuBilgi[oyuncuid][oHapisSure][0] = 0;
		if(OyuncuBilgi[oyuncuid][oHapisSure][1] > 0) OyuncuBilgi[oyuncuid][oHapisSure][1] = 0;
		HapiseAt(oyuncuid);
		UyariMesaji(oyuncuid, "Bir kiralik katil tarafindan olduruldun, karakterin CK oldu.");
		Kontrakt_Kaldir(i);
		return 1;
	}
	HataMesaji(playerid, "Bu kisi adina tutulmus bir kontrakt yok.");
	return 1;
}

CMD:soyun(playerid, params[])
{
	if(OyuncuBilgi[playerid][oGiris] == false) return HataMesaji(playerid, "Giris yapmadan bu komutu kullanamazsin.");
	if((OyuncuBirlikTuru(playerid) == BIRLIK_POLIS || OyuncuBirlikTuru(playerid) == BIRLIK_MEDIKAL || OyuncuBirlikTuru(playerid) == BIRLIK_DEVLET) && OyuncuBilgi[playerid][oIsbasi] == true) return HataMesaji(playerid, "isbasindayken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oHapisSure][0] > 0 || OyuncuBilgi[playerid][oHapisSure][1] > 0) return HataMesaji(playerid, "Hapisteyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oYarali] > 0 || OyuncuBilgi[playerid][oYaraliSure] > 0) return HataMesaji(playerid, "Yaraliyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oDonduruldu] > 0) return HataMesaji(playerid, "Etkisiz haldeyken bu komutu kullanamazsin.");
	if(IsPlayerInAnyVehicle(playerid)) return HataMesaji(playerid, "Bu komut arac icerisinde kullanilamaz.");
	if(OyuncuBilgi[playerid][oGeciciKiyafet] > 0) {
		OyuncuBilgi[playerid][oGeciciKiyafet] = 0;
		SetPlayerSkin(playerid, OyuncuBilgi[playerid][oKiyafet]);
		cmd_me(playerid, "kiyafetlerini giydi.");
		return 1;
	}
	if(OyuncuBilgi[playerid][oCinsiyet] == 1) {
		switch(OyuncuBilgi[playerid][oTenRengi]) {
			case 1: {
				new kiyafetler[sizeof(SoyunErkekBeyaz)];
				for(new c = 0; c != sizeof(SoyunErkekBeyaz); c++) kiyafetler[c] = SoyunErkekBeyaz[c][0];
				ShowModelSelectionMenuEx(playerid, kiyafetler, sizeof(kiyafetler), "Soyun", MODELLER_SOYUN, 0.0, 0.0, 0.0);
			}
			case 2: {
				new kiyafetler[sizeof(SoyunErkekSiyah)];
				for(new c = 0; c != sizeof(SoyunErkekSiyah); c++) kiyafetler[c] = SoyunErkekSiyah[c][0];
				ShowModelSelectionMenuEx(playerid, kiyafetler, sizeof(kiyafetler), "Soyun", MODELLER_SOYUN, 0.0, 0.0, 0.0);
			}
		}
	}
	else if(OyuncuBilgi[playerid][oCinsiyet] == 2) {
		switch(OyuncuBilgi[playerid][oTenRengi]) {
			case 1: {
				new kiyafetler[sizeof(SoyunKadinBeyaz)];
				for(new c = 0; c != sizeof(SoyunKadinBeyaz); c++) kiyafetler[c] = SoyunKadinBeyaz[c][0];
				ShowModelSelectionMenuEx(playerid, kiyafetler, sizeof(kiyafetler), "Soyun", MODELLER_SOYUN, 0.0, 0.0, 0.0);
			}
			case 2: {
				new kiyafetler[sizeof(SoyunKadinSiyah)];
				for(new c = 0; c != sizeof(SoyunKadinSiyah); c++) kiyafetler[c] = SoyunKadinSiyah[c][0];
				ShowModelSelectionMenuEx(playerid, kiyafetler, sizeof(kiyafetler), "Soyun", MODELLER_SOYUN, 0.0, 0.0, 0.0);
			}
		}
	}
	return 1;
}
