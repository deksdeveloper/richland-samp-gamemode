stock Durak_Sil(id)
{
	new query[100];
	format(query, sizeof(query), "DELETE FROM `duraklar` WHERE `ID` = '%d'", Duraklar[id][ID]);
	mysql_query(mysqlB, query, false);
	Duraklar[id][Exists] = false;
	if (IsValidDynamic3DTextLabel(Duraklar[id][durakLabel])) DestroyDynamic3DTextLabel(Duraklar[id][durakLabel]);
	if (IsValidDynamicPickup(Duraklar[id][durakPickup])) DestroyDynamicPickup(Duraklar[id][durakPickup]);
	if(IsValidDynamicObject(Duraklar[id][durakObje])) DestroyDynamicObject(Duraklar[id][durakObje]), Duraklar[id][durakObje] = INVALID_OBJECT_ID;

	format(Duraklar[id][durakAd], 30, "");
	Duraklar[id][durakPos][0] = 0.0;
	Duraklar[id][durakPos][1] = 0.0;
	Duraklar[id][durakPos][2] = 0.0;
	Duraklar[id][durakPos][3] = 0.0;
	Duraklar[id][durakFiyat] = 0;
	Duraklar[id][durakSure] = 0;
	Iter_Remove(DurakIT, id);
	return 1;
}

stock Durak_Ekle(ad[], Float:posX, Float:posY, Float:posZ, fiyat, sure, Float:angle)
{
    new i = Iter_Free(DurakIT);

	if (i != -1)
	{
	    if (Duraklar[i][Exists] == false)
	    {
	        Duraklar[i][Exists] = true;

			Duraklar[i][ID] = i;


	        format(Duraklar[i][durakAd], 30, ad);
	        Duraklar[i][durakPos][0] = posX;
	        Duraklar[i][durakPos][1] = posY;
	        Duraklar[i][durakPos][2] = posZ;
	        Duraklar[i][durakPos][3] = angle;
	        Duraklar[i][durakFiyat] = fiyat;
	        Duraklar[i][durakSure] = sure;

	        new str[256];
	        format(str, sizeof(str), "{E74C3C}(( %s - Otobus Duragi #%d ))\n/durak", Duraklar[i][durakAd], i);
			Duraklar[i][durakLabel] = CreateDynamic3DTextLabel(str, 0x33CCFFAA, Duraklar[i][durakPos][0], Duraklar[i][durakPos][1], Duraklar[i][durakPos][2], 10.0);
			Duraklar[i][durakPickup] = CreateDynamicPickup(19134, 23, Duraklar[i][durakPos][0], Duraklar[i][durakPos][1], Duraklar[i][durakPos][2], 0, 0);
            Duraklar[i][durakObje] = CreateDynamicObject(1257, Duraklar[i][durakPos][0], Duraklar[i][durakPos][1], Duraklar[i][durakPos][2], 0.0, 0.0, Duraklar[i][durakPos][3], 0, 0);
	        new query[256];
			mysql_format(mysqlB, query, sizeof(query), "INSERT INTO `duraklar` (`ID`, `Ad`, `posX`, `posY`, `posZ`, `durakFiyat`, `durakSure`, `Angle`) VALUES ('%d', '%s', '%.4f', '%.4f', '%.4f', '%d', '%d', '%.4f')", i, ad, posX, posY, posZ, fiyat, sure, angle);
			mysql_query (mysqlB, query);
			Iter_Add(DurakIT, i);
			return i;
	    }
	}
	return -1;
}

