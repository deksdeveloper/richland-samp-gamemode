CMD:yereyatir(playerid, params[])
{
	if(OyuncuBirlikTuru(playerid) != BIRLIK_POLIS) return HataMesaji(playerid, "Bu komutu kullanmak icin polis olman gerekiyor.");
	if(OyuncuBilgi[playerid][oHapisSure][0] > 0 || OyuncuBilgi[playerid][oHapisSure][1] > 0) return HataMesaji(playerid, "Hapisteyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oKelepceli] == 1) return HataMesaji(playerid, "Kelepceliyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oYarali] > 0) return HataMesaji(playerid, "Yaraliyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oDonduruldu] > 0) return HataMesaji(playerid, "Bu komut su anda kullanilamaz.");
	if(IsPlayerInAnyVehicle(playerid)) return HataMesaji(playerid, "Bu komut aracta kullanilamaz.");
	if(OyuncuBilgi[playerid][oPaintball] != -1) return HataMesaji(playerid, "Paintballda bunu yapamazsin.");
    if(GetPVarInt(playerid, "yereyatir") > gettime()) {
	    HataMesaji(playerid, "Yeniden bu komutu kullanmak icin %d saniye beklemelisin.", GetPVarInt(playerid, "yereyatir") - gettime());
	    return 1;
	}

	new oyuncuid;
	if(sscanf(params, "u", oyuncuid)) return KullanimMesaji(playerid, "/yereyatir [oyuncu id/isim]");
	if(!IsPlayerConnected(oyuncuid)) return HataMesaji(playerid, "Girilen kisi oyunda degil.");
    if(oyuncuid == playerid) return HataMesaji(playerid, "Bu komutu kendi uzerinde kullanamazsin.");
	if(!YakinlikKontrol(playerid, oyuncuid, 2.5)) return HataMesaji(playerid, "Bu oyuncuya yakin olmalisin.");
	if(OyuncuBilgi[oyuncuid][oYaraliSure] > 0 || OyuncuBilgi[oyuncuid][oDonduruldu] > 0 || IsPlayerInAnyVehicle(oyuncuid)) return HataMesaji(playerid, "Bu komut su anda bu kisinin uzerinde kullanilamaz.");
	if(OyuncuBilgi[oyuncuid][oAdminDurum] == true) return HataMesaji(playerid, "Bu secenek isbasindaki yoneticiler icin kullanilamaz.");

	new bilgi[144];
	switch(random(4)) {
	    case 0, 1, 3: format(bilgi, sizeof(bilgi), "%s adli kisinin uzerine atlar ve yere yatirir.", OyuncuIsim(oyuncuid)), OyuncuBilgi[oyuncuid][oDonduruldu] = 15;
	    case 2, 4: format(bilgi, sizeof(bilgi), "%s adli kisinin uzerine atlamayi dener, basaramaz.", OyuncuIsim(oyuncuid));
	}
	cmd_me(playerid, bilgi);
	OyuncuBilgi[playerid][oDonduruldu] = 3;
	SetPVarInt(playerid, "yereyatir", gettime() + 5);
	if(OyuncuBilgi[oyuncuid][oDonduruldu] > 0) {
	    if(OyuncuBilgi[oyuncuid][oTamirKitiSure] > 0) return HataMesaji(oyuncuid, "Etkisi hale geldigin icin tamir islemin iptal edildi."), OyuncuBilgi[playerid][oTamirArac] = INVALID_VEHICLE_ID, OyuncuBilgi[playerid][oTamirKiti]--;
		ApplyAnimation(oyuncuid, "CRACK", "crckdeth2", 4.1, 1, 0, 0, 0, 0);
		TogglePlayerControllable(oyuncuid, false);
		new log_icerik[144], oyuncu_isim[MAX_PLAYER_NAME + 1], hedef_isim[MAX_PLAYER_NAME + 1];
		GetPlayerName(playerid, oyuncu_isim, sizeof(oyuncu_isim));
		GetPlayerName(oyuncuid, hedef_isim, sizeof(hedef_isim));
		format(log_icerik, sizeof(log_icerik), "[%s] %s(%d), %s(%d) adli kisiyi yere yatirdi.", Tarih(), oyuncu_isim, OyuncuBilgi[playerid][oID], hedef_isim, OyuncuBilgi[oyuncuid][oID]);
		//Gunluk_LogKaydetKaydet(log_icerik);
		Log_Kaydet("loglar/polis.txt", "%s", log_icerik);
	}
	return 1;
}

