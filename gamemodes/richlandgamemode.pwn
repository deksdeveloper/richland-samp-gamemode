	OyuncuBilgi[oyuncuid][oCevaplandi] = true;
	OyuncuBilgi[playerid][oAramaSure] = 0;
	OyuncuBilgi[oyuncuid][oAramaSure] = 0;
	Kayit_Olustur(playerid, oyuncuid, TELEFON_GELEN);
	Kayit_Olustur(oyuncuid, playerid, TELEFON_GIDEN);
