CMD:firmaekle(playerid, params[])
{
	if (!IsPlayerConnected(playerid)) return 1;
    if (OyuncuBilgi[playerid][oAdmin] < 6) return HataMesaji(playerid, "Bu komutu kullanmak icin yetkiniz yok.");
    new firma = -1;
    if ((firma = Firma_Yakin(playerid)) != -1) return HataMesaji(playerid, "Yakinlarinizde zaten firma var. Farkli bir yere eklemeyi deneyin.");
	new Float:pos[3], Float:angle, name[64], tur, gerekenlevel, bonus, kazanc1, kazanc2, arac;
    if (sscanf(params, "dddddds[64]", tur, gerekenlevel, bonus, kazanc1, kazanc2, arac, name)) return KullanimMesaji(playerid, "/firmaekle [Firma Turu 1-4] [Gereken Level] [Bonus (1-3 arasinda sayi girin)] [Kazanc 1] [Kazanc 2] [Arac ID] [Firma Adi]"), SunucuMesaji(playerid, "Firma turleri: 1: Ev Esyasi - 2: Gida ve Icecek - 3: Sanayi Urunleri - 4: Insaat Urunleri"), SunucuMesaji(playerid, "Kazanc 1 ve kazanc 2 su sekilde calismakta; 1. ve 2. kazanc arasinda bir sayi belirlenir ve rastgele ucret verir."), SunucuMesaji(playerid, "Ornegin 10 ve 20 girdiniz, kazanc 10 ve 20'nin arasinda cikacaktir. 15 gibi.");

    if (strlen(name) > 64) return HataMesaji(playerid, "Firma adi 64 karakteri gecemez.");
    if(tur < 1 || tur > 4) return HataMesaji(playerid, "Hatali firma turu!");
    if(bonus < 1 || bonus > 3) return HataMesaji(playerid, "Hatali bonus!");
    GetPlayerPos(playerid, pos[0], pos[1], pos[2]);
    GetPlayerFacingAngle(playerid, angle);
    new id = Firma_Ekle(name, pos[0], pos[1], pos[2], kazanc1, kazanc2, bonus, gerekenlevel, tur, arac);
    if (id == -1) return HataMesaji(playerid, "Sunucu maksimum firma limitine ulasti.");
    SunucuMesaji(playerid, "Firma eklendi, ID: %d", id);
    SendClientMessageToAllEx(COLOR_LIGHTRED, "AdmLog: %s, %d nolu Firmayi olusturdu.", OyuncuBilgi[playerid][oAdminNick], id);
	return 1;
}

CMD:firmasil(playerid, params[])
{
	new id;
    if (!IsPlayerConnected(playerid)) return 1;
    if (OyuncuBilgi[playerid][oAdmin] < 6) return HataMesaji(playerid, "Bu komutu kullanmak icin yetkiniz yok.");
    if (sscanf(params, "i", id)) return KullanimMesaji(playerid, "/firmasil [ID]");
    if (!Iter_Contains(FirmaIT, id)) return HataMesaji(playerid, "Hatali ID.");
    Firma_Sil(id);
    SunucuMesaji(playerid, "Firma basariyla silindi.");
    SendClientMessageToAllEx(COLOR_LIGHTRED, "AdmLog: %s, %d nolu firmayi sildi.", OyuncuBilgi[playerid][oAdminNick], id);
	return 1;
}


stock Firma_Yukle()
{
    mysql_query(mysqlB, "SELECT * FROM `firmalar`");

	new rows = cache_num_rows();
    printf("Toplam %d Firma yuklenmistir.", rows);

	if (rows)
	{
	    new lkID;
		for (new i = 0; i < rows; i ++) if (i < MAX_FIRMA)
	    {
	        cache_get_value_name_int(i, "ID", lkID);

	        FirmaData[lkID][Exists] = true;
	        FirmaData[lkID][ID] = lkID;
	        
            cache_get_value_name(i, "firmaAd", FirmaData[lkID][firmaAd], 64);
	        cache_get_value_name_float(i, "firmaTeslimatX", FirmaData[lkID][firmaTeslimat][0]);
	        cache_get_value_name_float(i, "firmaTeslimatY", FirmaData[lkID][firmaTeslimat][1]);
	        cache_get_value_name_float(i, "firmaTeslimatZ", FirmaData[lkID][firmaTeslimat][2]);
	        cache_get_value_name_int(i, "firmaKazanc1", FirmaData[lkID][firmaKazanc1]);
	        cache_get_value_name_int(i, "firmaKazanc2", FirmaData[lkID][firmaKazanc2]);
	        cache_get_value_name_int(i, "firmaBonus", FirmaData[lkID][firmaBonus]);
	        cache_get_value_name_int(i, "firmaGLevel", FirmaData[lkID][firmaGLevel]);
	        cache_get_value_name_int(i, "firmaTur", FirmaData[lkID][firmaTur]);
	        Iter_Add(FirmaIT, lkID);
		    Firma_Yenile(lkID);
	    }
	}
    return 1;
}

stock Firma_Yenile(lkID)
{
	if(lkID != -1 && FirmaData[lkID][Exists] == true)
	{
        if(IsValidDynamic3DTextLabel(FirmaData[lkID][firmaLabel])) DestroyDynamic3DTextLabel(FirmaData[lkID][firmaLabel]), FirmaData[lkID][firmaLabel] = Text3D:INVALID_3DTEXT_ID;
        if(IsValidDynamicPickup(FirmaData[lkID][firmaPickup])) DestroyDynamicPickup(FirmaData[lkID][firmaPickup]), FirmaData[lkID][firmaPickup] = INVALID_PICKUP_ID;

        new str[128];
	    format(str, 128, "%s\nTeslimat Noktasi", FirmaData[lkID][firmaAd]);
        FirmaData[lkID][firmaLabel] = CreateDynamic3DTextLabel(str, 0x33CCFFAA, FirmaData[lkID][firmaTeslimat][0], FirmaData[lkID][firmaTeslimat][1], FirmaData[lkID][firmaTeslimat][2]+0.5, 10.0);
        FirmaData[lkID][firmaPickup] = CreateDynamicPickup(1239, 0, FirmaData[lkID][firmaTeslimat][0], FirmaData[lkID][firmaTeslimat][1], FirmaData[lkID][firmaTeslimat][2], 0, 0);

	}
	return 1;
}

stock Firma_Sil(id)
{
	new query[100];
	format(query, sizeof(query), "DELETE FROM `firmalar` WHERE `ID` = '%d'", FirmaData[id][ID]);
	mysql_query(mysqlB, query, false);
	FirmaData[id][Exists] = false;
	if(IsValidDynamic3DTextLabel(FirmaData[id][firmaLabel])) DestroyDynamic3DTextLabel(FirmaData[id][firmaLabel]), FirmaData[id][firmaLabel] = Text3D:INVALID_3DTEXT_ID;

    format(FirmaData[id][firmaAd], 30, "");
	FirmaData[id][firmaTeslimat][0] = 0.0;
	FirmaData[id][firmaTeslimat][1] = 0.0;
	FirmaData[id][firmaTeslimat][2] = 0.0;

	FirmaData[id][firmaKazanc1] = -1;
	FirmaData[id][firmaKazanc2] = -1;
	FirmaData[id][firmaBonus] = -1;
	FirmaData[id][firmaGLevel] = -1;
	FirmaData[id][firmaTur] = -1;
	
	Iter_Remove(FirmaIT, id);
	return 1;
}


stock Firma_Ekle(ad[], Float:posX, Float:posY, Float:posZ, kazanc1, kazanc2, bonus, gerekenlevel, tur, arac)
{
    new i = Iter_Free(FirmaIT);

	if (i != -1)
	{
	    if (FirmaData[i][Exists] == false)
	    {
	        FirmaData[i][Exists] = true;

			FirmaData[i][ID] = i;

            format(FirmaData[i][firmaAd], 30, ad);
	        FirmaData[i][firmaTeslimat][0] = posX;
	        FirmaData[i][firmaTeslimat][1] = posY;
	        FirmaData[i][firmaTeslimat][2] = posZ;
	        FirmaData[i][firmaKazanc1] = kazanc1;
	        FirmaData[i][firmaKazanc2] = kazanc2;
	        FirmaData[i][firmaBonus] = bonus;
	        FirmaData[i][firmaGLevel] = gerekenlevel;
	        FirmaData[i][firmaTur] = tur;

	        new query[256];
			mysql_format(mysqlB, query, sizeof(query), "INSERT INTO `firmalar` (`ID`, `firmaAd`, `firmaTeslimatX`, `firmaTeslimatY`, `firmaTeslimatZ`, `firmaKazanc1`, `firmaKazanc2`, `firmaBonus`, `firmaGLevel`, `firmaTur`) VALUES ('%d', '%s', '%.4f', '%.4f', '%.4f', '%d', '%d', '%d', '%d', '%d')", i, ad, posX, posY, posZ, kazanc1, kazanc2, bonus, gerekenlevel, tur, arac);
			mysql_query (mysqlB, query);
			Iter_Add(FirmaIT, i);
			return i;
	    }
	}
	return -1;
}

stock Firma_Kaydet(i)
{
	static query[512];
	format(query, sizeof(query), "UPDATE `firmalar` SET `firmaAd` = '%s', `firmaTeslimatX` = '%.4f', `firmaTeslimatY` = '%.4f', `firmaTeslimatZ` = '%.4f', `firmaKazanc1` = '%d', `firmaKazanc2` = '%d', `firmaBonus` = '%d', `firmaGLevel` = '%d', `firmaTur` = '%d' WHERE `ID` = '%d'",
		FirmaData[i][firmaAd],
		FirmaData[i][firmaTeslimat][0],
        FirmaData[i][firmaTeslimat][1],
	    FirmaData[i][firmaTeslimat][2],
        FirmaData[i][firmaKazanc1],
        FirmaData[i][firmaKazanc2],
        FirmaData[i][firmaBonus],
        FirmaData[i][firmaGLevel],
        FirmaData[i][firmaTur],
	    FirmaData[i][ID]
	);
	return mysql_tquery(mysqlB, query);
}

