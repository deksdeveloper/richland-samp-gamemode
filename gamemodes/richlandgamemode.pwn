	if(id == -1) return HataMesaji(playerid, "Herhangi bir rafa yakin degilsin.");
	foreach(new i : Player) if(i != playerid && OyuncuBilgi[i][oRaf] == id) return HataMesaji(playerid, "Bu raf baskasi tarafindan kullaniliyor.");
    new evid = Oyuncu_Evde(playerid);
	if(evid > -1) {
		if(Ev_SahipKontrol(playerid, evid) || Ev_AnahtarKontrol(playerid, evid)) {
			if(OyuncuBirlikTuru(playerid) == BIRLIK_POLIS) return HataMesaji(playerid, "Bu birlik turu bu islemi yapamaz.");

			return Raf_SilahListele(playerid, id);
		}
		else if(!Ev_SahipKontrol(playerid, evid) && !Ev_AnahtarKontrol(playerid, evid) && OyuncuBirlikTuru(playerid) == BIRLIK_POLIS) {
			if(OyuncuBilgi[playerid][oIsbasi] == false) return HataMesaji(playerid, "Bu islemi yapabilmek icin isbasinda olmalisin.");

			return Raf_SilahListele(playerid, id);
		}
	}
	new binaid = Oyuncu_Binada(playerid);
	if(binaid > -1) {
		if(Bina_SahipKontrol(playerid, binaid) || Bina_AnahtarKontrol(playerid, binaid)) {
			if(OyuncuBirlikTuru(playerid) == BIRLIK_POLIS) return HataMesaji(playerid, "Bu birlik turu bu islemi yapamaz.");

			return Raf_SilahListele(playerid, id);
		}
		else if(OyuncuBirlikTuru(playerid) == BIRLIK_POLIS && !Bina_SahipKontrol(playerid, binaid) && !Bina_AnahtarKontrol(playerid, binaid)) {
			if(OyuncuBilgi[playerid][oIsbasi] == false) return HataMesaji(playerid, "Bu islemi yapabilmek icin isbasinda olmalisin.");

			return Raf_SilahListele(playerid, id);
		}
	}
	HataMesaji(playerid, "Bu ev veya bina senin degil.");
	return 1;
}

CMD:tedaviol(playerid, params[])
{
	if(!IsPlayerInRangeOfPoint(playerid, 3.0, 533.1987,197.2851,1019.9844)) return HataMesaji(playerid, "Tedavi olma noktasinda degilsin.");
	if(OyuncuBilgi[playerid][oYarali] == 0) return HataMesaji(playerid, "Yarali degilsin.");
	if(OyuncuBilgi[playerid][oYaraliSure] > 0) return HataMesaji(playerid, "Yarali suren bitmeden bu komutu kullanamazsin.");
	new gerekliucret = 0;
	switch(OyuncuBilgi[playerid][oVIP]) {
	    case 1: gerekliucret = 300;
	    case 2: gerekliucret = 200;
	    case 3: gerekliucret = 0;
	    default: gerekliucret = 400;
	}
	if(OyuncuBirlikTuru(playerid) == BIRLIK_POLIS || OyuncuBirlikTuru(playerid) == BIRLIK_MEDIKAL || OyuncuBirlikTuru(playerid) == BIRLIK_DEVLET) gerekliucret = 0;
	if(gerekliucret > 0 && OyuncuBilgi[playerid][oPara] < gerekliucret) {
		HataMesaji(playerid, "Tedavi icin yeterli paran yok. (%s)", NumaraFormati(gerekliucret));
