CMD:haracres(playerid, params[])
{
    if(OyuncuBilgi[playerid][oAdmin] < 7) return HataMesaji(playerid, "Bu komutu kullanma yetkin yok!");
    if(OyuncuBilgi[playerid][oAdminDurum] == false && OyuncuBilgi[playerid][oAdmin] < 6) return IsbasiYapmalisin(playerid);

	if(aracrespawn == true) {
		SendClientMessageToAllEx(RENK_ACIKKIRMIZI, "YETKILI: %s adli yetkili arac respawn islemini durdurdu.", OyuncuBilgi[playerid][oAdminNick]);
		KillTimer(arespawntimer);
		aracrespawn = false;
		return 1;
	}
	new saniye;
	if(sscanf(params, "d", saniye)) return KullanimMesaji(playerid, "/haracres [saniye]");
	if(saniye < 1 || saniye > 120) return HataMesaji(playerid, "Saniye en az 1, en fazla 120 olabilir!");

	if(aracrespawn == false) {
		SendClientMessageToAllEx(RENK_ACIKKIRMIZI, "YETKILI: %s adli yetkili arac respawn islemini baslatti %d saniye sonra araclar respawnlanacak.", OyuncuBilgi[playerid][oAdminNick], saniye);
	 	arespawntimer = SetTimerEx("Surucusuz_Arac_Respawn", saniye * 1000, false, "dd", playerid, 1);
		aracrespawn = true;
	}
	return 1;
}

