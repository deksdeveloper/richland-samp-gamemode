	new oyuncuid, yer[32];
	if(sscanf(params, "us[32]", oyuncuid, yer)) return KullanimMesaji(playerid, "/gonder [oyuncu ID/isim] [yer] (ls-lv-sf)");
	if(!IsPlayerConnected(oyuncuid)) return HataMesaji(playerid, "Girilen kisi oyunda degil.");
    if(OyuncuBilgi[oyuncuid][oHapisSure][0] > 0 || OyuncuBilgi[oyuncuid][oHapisSure][1] > 0) return HataMesaji(playerid, "Oyuncu hapiste, bu komutu onun uzerinde kullanamazsin.");
    if(OyuncuBilgi[oyuncuid][oCK] == 1) return HataMesaji(playerid, "Oyuncu CK olmus, bu komutu uzerinde kullanamazsin.");
