CMD:newbie(playerid, params[])
{
	if(newbie_kanali == false) return HataMesaji(playerid, "Bu kanal Management tarafindan devre disi birakilmis.");
    if(OyuncuBilgi[playerid][oNewbieDurum] == false) return HataMesaji(playerid, "Newbie kanalini kapatmissin, /ayarlar komutundan acabilirsin.");
	if(OyuncuBilgi[playerid][oNewbieMute] != 0) return HataMesaji(playerid, "Bu kanaldan susturulmussun.");
	if(OyuncuBilgi[playerid][oNewbieSure] > gettime()) {
		HataMesaji(playerid, "Bu kanala yeni bir yazi yazmak icin %d saniye beklemelisin.", OyuncuBilgi[playerid][oNewbieSure] - gettime());
	    return 1;
	}
	if(isnull(params)) return KullanimMesaji(playerid, "/newbie [mesajiniz]");
	new mesaj[144 + MAX_PLAYER_NAME];
	if(OyuncuBilgi[playerid][oAdmin] < 1 && OyuncuBilgi[playerid][oHelper] < 1) {
		new oyuncu_isim[MAX_PLAYER_NAME];
		GetPlayerName(playerid, oyuncu_isim, sizeof(oyuncu_isim));
		for(new i = 0, len = strlen(oyuncu_isim); i < len; i ++) if(oyuncu_isim[i] == '_') oyuncu_isim[i] = ' ';
		OyuncuBilgi[playerid][oNewbieSure] = gettime() + 60;
	    format(mesaj, sizeof(mesaj), "Oyuncu %s: %s", oyuncu_isim, params);
	}
	else if(OyuncuBilgi[playerid][oAdmin] > 0) format(mesaj, sizeof(mesaj), "%s %s: %s", Admin_RutbeIsim(OyuncuBilgi[playerid][oAdmin]), OyuncuBilgi[playerid][oAdminNick], params);
	else if(OyuncuBilgi[playerid][oHelper] > 0) format(mesaj, sizeof(mesaj), "%s %s: %s", Helper_RutbeIsim(OyuncuBilgi[playerid][oHelper]), OyuncuBilgi[playerid][oAdminNick], params);
	foreach(new i : Player) if(OyuncuBilgi[i][oGiris] == true && OyuncuBilgi[i][oNewbieDurum] == true) SendClientMessageEx(i, RENK_NEWBIE, "%s", mesaj);
	new log_icerik[256];
	format(log_icerik, sizeof(log_icerik), "[%s] [newbie] %s", Tarih(), mesaj);
	//Gunluk_LogKaydetKaydet(log_icerik);
	return 1;
}

CMD:nmute(playerid, params[])
{
    if(OyuncuBilgi[playerid][oAdmin] < 1 && OyuncuBilgi[playerid][oHelper] < 1) return YetkinizYok(playerid);

	new oyuncuid;
	if(sscanf(params, "u", oyuncuid)) return KullanimMesaji(playerid, "/nmute [oyuncu id/isim]");
	if(!IsPlayerConnected(oyuncuid) || OyuncuBilgi[oyuncuid][oGiris] == false) return HataMesaji(playerid, "Gecersiz oyuncu veya oyuncu giris yapmamis.");
	if(OyuncuBilgi[oyuncuid][oHelper] > 0 || OyuncuBilgi[oyuncuid][oAdmin] > 0) return HataMesaji(playerid, "Yetkililer bu kanaldan susturulamaz.");
	if(oyuncuid == playerid) return HataMesaji(playerid, "Kendini muteleyemezsin.");
	new bilgi[96];
	switch(OyuncuBilgi[oyuncuid][oNewbieMute]) {
		case 0: {
			OyuncuBilgi[oyuncuid][oNewbieMute] = 1;
			SunucuMesaji(oyuncuid, "%s tarafindan newbie kanalindan susturuldun, artik yazi yazamayacaksin.", OyuncuBilgi[playerid][oAdminNick]);
			SunucuMesaji(playerid, "%s adli kisiyi newbie kanalindan susturdun.", OyuncuIsim(oyuncuid));
			format(bilgi, sizeof(bilgi), "kisiyi muteledi");
		}
		case 1: {
			OyuncuBilgi[oyuncuid][oNewbieMute] = 0;
			SunucuMesaji(oyuncuid, "%s tarafindan newbie kanalindan susturulman acildi.", OyuncuBilgi[playerid][oAdminNick]);
			SunucuMesaji(playerid, "%s adli kisiyi newbie kanalindan susturulmasini actin.", OyuncuIsim(oyuncuid));
			format(bilgi, sizeof(bilgi), "kisinin mutesini kaldirdi");
		}
		default: {
			OyuncuBilgi[oyuncuid][oNewbieMute] = 1;
			SunucuMesaji(oyuncuid, "%s tarafindan newbie kanalindan susturuldun, artik yazi yazamayacaksin.", OyuncuBilgi[playerid][oAdminNick]);
			SunucuMesaji(playerid, "%s adli kisiyi newbie kanalindan susturdun.", OyuncuIsim(oyuncuid));
			format(bilgi, sizeof(bilgi), "kisiyi muteledi");
		}
	}
	new log_icerik[256], oyuncu_isim[MAX_PLAYER_NAME + 1];
	GetPlayerName(oyuncuid, oyuncu_isim, sizeof(oyuncu_isim));
	format(log_icerik, sizeof(log_icerik), "[%s] %s(%d), %s(%d) adli %s.", Tarih(), OyuncuBilgi[playerid][oAdminNick], OyuncuBilgi[playerid][oID], oyuncu_isim, OyuncuBilgi[oyuncuid][oID], bilgi);
	//Gunluk_LogKaydetKaydet(log_icerik);
	Log_Kaydet("loglar/mute.txt", "%s", log_icerik);
	return 1;
}

