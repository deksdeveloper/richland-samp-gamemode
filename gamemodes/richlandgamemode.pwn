Dialog:DIALOG_KASAAYARLA(playerid, response, listitem, inputtext[])
{
	if(!response) return cmd_birlik(playerid, inputtext);
		
		if((OyuncuBirlikTuru(playerid) == BIRLIK_POLIS || OyuncuBirlikTuru(playerid) == BIRLIK_MEDIKAL || OyuncuBirlikTuru(playerid) == BIRLIK_DEVLET) && OyuncuBilgi[playerid][oAdmin] < 7) return HataMesaji(playerid, "Bu birlik turunun kasasini sadece Management ayarlayabilir.");
		if(BirlikBilgi[OyuncuBilgi[playerid][oBirlik]][birlikMaksRutbe] != OyuncuBilgi[playerid][oBirlikRutbe] && OyuncuBilgi[playerid][oBirlikLider] != BirlikBilgi[OyuncuBilgi[playerid][oBirlik]][birlikID]) return HataMesaji(playerid, "Bu secenegi sadece birlik liderleri kullanabilir.");
		if(OyuncuBilgi[playerid][oPara] < KASA_KONUMLANDIRMA_UCRET) return HataMesaji(playerid, "Yeterli paran yok. (%s)", NumaraFormati(KASA_KONUMLANDIRMA_UCRET));

		new id = -1;
		for(new i = 0; i < MAKSIMUM_KASA; i ++) if(KasaBilgi[i][kasaBirlik] == OyuncuBilgi[playerid][oBirlikSQLID]) {
			if(KasaBilgi[i][kasaPara] > 0) return HataMesaji(playerid, "Kasanin icinde para varken yerini degistiremezsin.");
			Kasa_Kaldir(i);
		}
		id = Kasa_Yarat(playerid, OyuncuBilgi[playerid][oBirlikSQLID]);

		if(id == -1) return HataMesaji(playerid, "Kasa limitine ulasildigi icin yeni kasa yaratilamiyor.");
		SunucuMesaji(playerid, "Kasa basariyla yaratildi. (ID: %d)", id);

		new Float:oyuncu_pos[3];
		GetPlayerPos(playerid, oyuncu_pos[0], oyuncu_pos[1], oyuncu_pos[2]);
		OyuncuPosDegistir(playerid, oyuncu_pos[0], oyuncu_pos[1] + 1, oyuncu_pos[2]);
	    EditlemeSifirla(playerid);
		EditDynamicObject(playerid, KasaBilgi[id][kasaObje]);
		OyuncuBilgi[playerid][oDuzenlenenKasa] = id;
	return 1;
}



Dialog:DIALOG_BIRLIK(playerid, response, listitem, inputtext[])
{
   if(!response) return false;

		new birlikid = OyuncuBilgi[playerid][oBirlik];

		if(listitem == 0) SunucuMesaji(playerid, "Adi: %s | Maks. Rutbe: %d | Turu: %s | Seviyesi: %d", BirlikBilgi[birlikid][birlikAd], BirlikBilgi[birlikid][birlikMaksRutbe], birlikturadi(birlikid), BirlikBilgi[birlikid][birlikLevel]);
		else if(listitem == 1) 
		{
		    new sql[512];
			format(sql, sizeof(sql), "SELECT * FROM oyuncular WHERE birlik = %d", OyuncuBilgi[playerid][oBirlikSQLID]);
			mysql_tquery(mysqlB, sql, "Birlik_UyeArat", "d", playerid);
		}
		else if(listitem == 2) 
		{
			new mesaj[256 * MAKSIMUM_ARAC], sayi = 0;
			format(mesaj, sizeof(mesaj), "ID\tModel\tDurum\n");
			for(new i = 0; i != MAKSIMUM_ARAC; i++) if(AracBilgi[i][aracKullanimda] == true && OyuncuBilgi[playerid][oBirlikSQLID] == AracBilgi[i][aracBirlik])
			{
			    format(mesaj, sizeof(mesaj), "%s{FFFFFF}%d\t%s\t%s\n", mesaj, AracBilgi[i][aracYarat], AracModel(AracBilgi[i][aracModel]), AracDurum(i));
			    sayi++;
			}
			if(!sayi) return HataMesaji(playerid, "Birligine ait arac bulunmuyor.");
			Dialog_Show(playerid, DIALOG_BIRLIK_ARACLAR, DIALOG_STYLE_TABLIST_HEADERS, "> Birlik Araclari", mesaj, "Respawn", "<< Geri");
		}
		else if(listitem == 3) 
		{
			
			if(BirlikBilgi[OyuncuBilgi[playerid][oBirlik]][birlikMaksRutbe] != OyuncuBilgi[playerid][oBirlikRutbe]) return HataMesaji(playerid, "Bu secenegi sadece birlik liderleri kullanabilir.");
			Dialog_Show(playerid, DIALOG_BIRLIK_DAVET, DIALOG_STYLE_INPUT, "> Birlik Davet", "Birligine davet etmek istedigin kisinin ismini veya ID'sini gir;", "Sec", "iptal");
		}
		else if(listitem == 4) 
		{
			if(BirlikBilgi[OyuncuBilgi[playerid][oBirlik]][birlikMaksRutbe] != OyuncuBilgi[playerid][oBirlikRutbe]) return HataMesaji(playerid, "Bu secenegi sadece birlik liderleri kullanabilir.");
			Dialog_Show(playerid, DIALOG_BIRLIK_KOV, DIALOG_STYLE_INPUT, "> Birlik Kov", "Birliginden atmak istedigin kisinin ismini veya ID'sini gir;", "Sec", "iptal");
		}
		else if(listitem == 5) 
		{
			
			if(BirlikBilgi[OyuncuBilgi[playerid][oBirlik]][birlikMaksRutbe] != OyuncuBilgi[playerid][oBirlikRutbe]) return HataMesaji(playerid, "Bu secenegi sadece birlik liderleri kullanabilir.");
			Dialog_Show(playerid, DIALOG_BIRLIK_RUTBEVER, DIALOG_STYLE_INPUT, "> Rutbe Ver", "Rutbe vermek istedigin oyuncunun ismini veya ID'sini gir;", "Sec", "Geri");
		}
		else if(listitem == 6) 
		{
			
			if(BirlikBilgi[OyuncuBilgi[playerid][oBirlik]][birlikMaksRutbe] != OyuncuBilgi[playerid][oBirlikRutbe]) return HataMesaji(playerid, "Bu secenegi sadece birlik liderleri kullanabilir.");
            Dialog_Show(playerid, DIALOG_BIRLIK_YETKILI, DIALOG_STYLE_INPUT, "> Yetkili Ayarla", "{FFFFFF}Lutfen yetkili durumunu degistirmek istediginiz oyuncunun ismini veya ID'sini giriniz;\n{B70000}NOT: {FFFFFF}Yetkisi olan birisini secerseniz yetkisi alinir.", "Sec", "Geri"); 			
			return 1;
		}
		else if(listitem == 7) 
		{
			
			if(BirlikBilgi[OyuncuBilgi[playerid][oBirlik]][birlikMaksRutbe] != OyuncuBilgi[playerid][oBirlikRutbe]) return HataMesaji(playerid, "Bu secenegi sadece birlik liderleri kullanabilir.");
            Dialog_Show(playerid, DIALOG_BIRLIK_DUYURU, DIALOG_STYLE_INPUT, "> Birlik - Duyuru Duzenle", "Lutfen yeni birlik duyuru metnini girin;\nKaldirmak icin bosluga \"Yok\" yazin.", "Tamam", "Geri");			
			return 1;
		}
		else if(listitem == 8) 
		{
		    if(!IsPlayerInAnyVehicle(playerid)) return HataMesaji(playerid, "Bu secenek sadece aracta kullanilabilir."), cmd_birlik(playerid, inputtext);
			if(!Arac_SahipKontrol(playerid, Arac_IDBul(GetPlayerVehicleID(playerid)))) return HataMesaji(playerid, "Bu arac senin olmadigi icin bu secenegi kullanamazsin."), cmd_birlik(playerid, inputtext);
			Dialog_Show(playerid, DIALOG_BIRLIK_ARACEKLE, DIALOG_STYLE_MSGBOX, "> Birlik - Arac Ekle", "Bu araci birligine eklemek uzeresin, bu islem geri alinamaz.\nBu islemi yaptiginda aracin tamamen birligine baglanacak ve geri alamayacaksin.\n\nOnayliyor musun?", "Ayarla", "iptal");
		}
		else if(listitem == 9) 
		{
		    
			if(BirlikBilgi[OyuncuBilgi[playerid][oBirlik]][birlikMaksRutbe] != OyuncuBilgi[playerid][oBirlikRutbe] && OyuncuBilgi[playerid][oBirlikLider] != BirlikBilgi[OyuncuBilgi[playerid][oBirlik]][birlikID]) return HataMesaji(playerid, "Bu secenegi sadece birlik liderleri kullanabilir.");

			new sayi = 0;
			for(new i = 0; i < MAKSIMUM_ARAC; i ++) if(AracBilgi[i][aracBirlik] == BirlikBilgi[OyuncuBilgi[playerid][oBirlik]][birlikID] && SurucuBul(AracBilgi[i][aracYarat]) == INVALID_PLAYER_ID) {
				ASpawn(AracBilgi[i][aracYarat], 1);
				sayi++;
			}
			if(sayi == 0) return HataMesaji(playerid, "Birligine ait spawnlanabilecek arac bulunmuyor.");

			SunucuMesaji(playerid, "%d arac spawnlandi.", sayi);
			BirlikMesaji(OyuncuBilgi[playerid][oBirlik], RENK_BIRLIK, "BIRLIK: Birlik araclari(%d) %s tarafindan respawnlandi.", sayi, OyuncuIsim(playerid));
		}
		else if(listitem == 10) 
		{
		    
		    if(BirlikBilgi[OyuncuBilgi[playerid][oBirlik]][birlikMaksRutbe] != OyuncuBilgi[playerid][oBirlikRutbe]) return HataMesaji(playerid, "Bu secenegi sadece birlik liderleri kullanabilir.");

			Dialog_Show(playerid, DIALOG_BIRLIK_MAKSRUTBE, DIALOG_STYLE_INPUT, "> Birlik - Maksimum Rutbe", "Birliginin maksimum rutbesini kac yapmak istiyorsun?", "Ayarla", "iptal");
		}
		else if(listitem == 11) 
		{
			if(BirlikBilgi[birlikid][birlikMaksRutbe] != OyuncuBilgi[playerid][oBirlikRutbe] && OyuncuBilgi[playerid][oBirlikLider] != BirlikBilgi[OyuncuBilgi[playerid][oBirlik]][birlikID]) return HataMesaji(playerid, "Bu secenegi sadece birlik liderleri kullanabilir.");
		
			switch(BirlikBilgi[birlikid][oKanal]) {
				case 1: {
					BirlikBilgi[birlikid][oKanal] = 0;
					foreach(new i : Player) if(OyuncuBilgi[i][oBirlik] == birlikid) {
						SendClientMessageEx(i, RENK_SERVER, "BIRLIK: Birlik kanali %s tarafindan kapandi.", OyuncuIsim(playerid));
					}
					Birlik_Kaydet(birlikid);
					cmd_birlik(playerid, inputtext);
				}
				case 0: {
					BirlikBilgi[birlikid][oKanal] = 1;
					foreach(new i : Player) if(OyuncuBilgi[i][oBirlik] == birlikid) {
						SendClientMessageEx(i, RENK_SERVER, "BIRLIK: Birlik kanali %s tarafindan acildi.", OyuncuIsim(playerid));
					}
					Birlik_Kaydet(birlikid);
					cmd_birlik(playerid, inputtext);
				}
			}
		}
		else if(listitem == 12) BirlikRutbeDuzenle(playerid);
		else if(listitem == 13) BirlikMaasDuzenle(playerid);
		else if(listitem == 14) 
		{
		    
			if(BirlikBilgi[OyuncuBilgi[playerid][oBirlik]][birlikMaksRutbe] != OyuncuBilgi[playerid][oBirlikRutbe] && OyuncuBilgi[playerid][oBirlikLider] != BirlikBilgi[OyuncuBilgi[playerid][oBirlik]][birlikID]) return HataMesaji(playerid, "Bu secenegi sadece birlik liderleri kullanabilir.");

			if(BirlikBilgi[OyuncuBilgi[playerid][oBirlik]][birlikMaksRutbe] == OyuncuBilgi[playerid][oBirlikRutbe]) Dialog_Show(playerid, DIALOG_BIRIMLER, DIALOG_STYLE_TABLIST, "> Birlik - Birimler", "Birim Olustur\nBirim Davet\nBirim Duzenle\nBirim Kaldir", "Sec", "<< Geri");
			else if(OyuncuBilgi[playerid][oBirlikLider] == BirlikBilgi[OyuncuBilgi[playerid][oBirlik]][birlikID]) Dialog_Show(playerid, DIALOG_BIRIMLER, DIALOG_STYLE_TABLIST, "> Birlik - Birimler", "Birim Davet", "Sec", "<< Geri");
		}
		else if(listitem == 15) 
		{
			HataMesaji(playerid, "Bu sistem pasif."), cmd_birlik(playerid, inputtext);
		}
		else if(listitem == 16) 
		{
		   
			if(BirlikBilgi[OyuncuBilgi[playerid][oBirlik]][birlikMaksRutbe] != OyuncuBilgi[playerid][oBirlikRutbe]) return HataMesaji(playerid, "Bu secenegi sadece birlik liderleri kullanabilir.");
			if((OyuncuBirlikTuru(playerid) == BIRLIK_POLIS || OyuncuBirlikTuru(playerid) == BIRLIK_MEDIKAL || OyuncuBirlikTuru(playerid) == BIRLIK_DEVLET) && OyuncuBilgi[playerid][oAdmin] < 7) return HataMesaji(playerid, "Bu birlik turu icin secenegi sadece Management kullanilabilir.");

			new bilgi[144];
			format(bilgi, sizeof(bilgi), "Birligine ait kasayi %s karsiliginda konumlandiracaksin, onayliyor musun?", NumaraFormati(KASA_KONUMLANDIRMA_UCRET));
            Dialog_Show(playerid, DIALOG_KASAAYARLA, DIALOG_STYLE_MSGBOX, "> Birlik - Kasa Ayarla", bilgi, "Evet", "<< Geri");
		}
		else if(listitem == 17) 
		{

		    if(OyuncuBilgi[playerid][oAdminDurum] == true) return HataMesaji(playerid, "Yetkili isbasindayken bu secenegi kullanamazsin.");
		    
			for(new i = 0; i < MAKSIMUM_KASA; i ++) if(KasaBilgi[i][kasaBirlik] == OyuncuBilgi[playerid][oBirlikSQLID]) {
				if(!IsPlayerInRangeOfPoint(playerid, 5.0, KasaBilgi[i][kasaPos][0], KasaBilgi[i][kasaPos][1], KasaBilgi[i][kasaPos][2])) return HataMesaji(playerid, "Kasaya yakin degilsin.");

				OyuncuBilgi[playerid][oKasa] = i;
				new baslik[64];
				format(baslik, sizeof(baslik), "{FFFFFF}Birlik Kasa - [{0DFF00}%s{FFFFFF}]", NumaraFormati(KasaBilgi[i][kasaPara]));
	            Dialog_Show(playerid, DIALOG_KASAISLEM, DIALOG_STYLE_TABLIST, baslik, "Para Yatir\nPara cek", "Sec", "<< Geri");
				return 1;
			}
			HataMesaji(playerid, "Birligine ait bir kasa bulunmuyor.");
		}
		else if(listitem == 18)
		{
		    if(BirlikBilgi[OyuncuBilgi[playerid][oBirlik]][birlikMaksRutbe] == OyuncuBilgi[playerid][oBirlikRutbe]) return HataMesaji(playerid, "Birlik sahibi bu secenegi kullanamaz."), cmd_birlik(playerid, inputtext);
			Dialog_Show(playerid, DIALOG_BIRLIK_AYRIL, DIALOG_STYLE_MSGBOX, "> Birlik Ayril", "Birliginden ayrilmak istedigine emin misin?", "Ayril", "<< Geri");
		}
		else if(listitem == 19)
		{

			if(OyuncuBirlikTuru(playerid) == BIRLIK_POLIS && OyuncuBirlikTuru(playerid) == BIRLIK_MEDIKAL && OyuncuBirlikTuru(playerid) == BIRLIK_DEVLET) return HataMesaji(playerid, "Legal olusumlari silemezsiniz.");
            if(OyuncuBilgi[playerid][oBirlikLider] != BirlikBilgi[birlikid][birlikID]) return HataMesaji(playerid, "Birlik lideri degilsin.");
		    if(BirlikBilgi[OyuncuBilgi[playerid][oBirlik]][birlikMaksRutbe] != OyuncuBilgi[playerid][oBirlikRutbe]) return HataMesaji(playerid, "Bu secenegi kullanmak icin birlik lideri olman gerekiyor.");
		    new mesaj[256];
		    format(mesaj, sizeof(mesaj), "%s adli birligini silmek uzeresin, onayliyor musun?", BirlikBilgi[OyuncuBilgi[playerid][oBirlik]][birlikAd]);
			Dialog_Show(playerid, DIALOG_BIRLIK_KALDIR, DIALOG_STYLE_MSGBOX, "> Birlik Sil", mesaj, "Onayla", "<< Geri");
		}
		else if(listitem == 20) 
		{
			if(OyuncuBirlikTuru(playerid) != BIRLIK_POLIS && (BirlikBilgi[birlikid][birlikMaksRutbe] != OyuncuBilgi[playerid][oBirlikRutbe] || OyuncuBilgi[playerid][oBirlikLider] != BirlikBilgi[birlikid][birlikID])) return HataMesaji(playerid, "Birliginiz legal bir olusum degil.");

			switch(techizatizni) {
			    case false: {
					techizatizni = true, SunucuMesaji(playerid, "Techizat izni herkese acildi.");
					BirlikMesaji(OyuncuBilgi[playerid][oBirlik], RENK_BIRLIK, "[BIRLIK] %s techizat iznini tum birlige acti.", OyuncuIsim(playerid));
				}
				case true: {
					techizatizni = false, SunucuMesaji(playerid, "Techizat izni sadece rutbeye gore ayarlandi.");
					BirlikMesaji(OyuncuBilgi[playerid][oBirlik], RENK_BIRLIK, "[BIRLIK] %s techizat iznini rutbeye gore ayarladi.", OyuncuIsim(playerid));
				}
			}
		}
   return 1;
}

Dialog:DIALOG_EV_KAPI(playerid, response, listitem, inputtext[])
{
        if(!response) return false;
	    new id = Ev_Yakin(playerid), id2;
			if(listitem == 0) {
				foreach(new i : Player) {
				    id2 = Oyuncu_Evde(i);
				    if(id2 == id) {
				        SendClientMessageEx(i, RENK_DO, "* Zil caliyor. (( Ev ))");
				    }
				}
				cmd_me(playerid, "zile basar.");
			}
			else if(listitem == 1) {
			    if(!Ev_SahipKontrol(playerid, id) && EvBilgi[id][evKiraci] != OyuncuBilgi[playerid][oID] && !Ev_AnahtarKontrol(playerid, id)) return HataMesaji(playerid, "Bu evin anahtari sende yok.");

				switch(EvBilgi[id][evDurum]) {
					case 1: EvBilgi[id][evDurum] = 2, Ev_Yenile(id), cmd_me(playerid, "evin kapisini kilitler.");
					case 2: {
						EvBilgi[id][evDurum] = 1, Ev_Yenile(id), cmd_me(playerid, "evin kapisinin kilidini acar.");
						foreach(new i : Player) if(MaymuncukVeri[i][mTur] == 2 && MaymuncukVeri[playerid][mHedef] == id) return SunucuMesaji(i, "Maymuncuk kirildi."), MaymuncukVeri[i][mSaniye] = 0, OyuncuBilgi[i][oMaymuncuk]--;
					}
					default: HataMesaji(playerid, "Bu evin kilit durumunu degistiremezsin.");
				}
			}
			else if(listitem == 2) {
				if(EvBilgi[id][evKiralik] == 0) return HataMesaji(playerid, "Bu ev kiralanamaz.");
				if(EvBilgi[id][evKiraci] != 0) return HataMesaji(playerid, "Bu ev kiralanmis.");
				if(OyuncuBilgi[playerid][oPara] < EvBilgi[id][evKiraUcret]) return HataMesaji(playerid, "Kira secenegi karsilayamiyorsun.");
				if(IsPlayerInAnyVehicle(playerid)) return HataMesaji(playerid, "Bu secenegi aracta kullanilamaz.");
				if(GetPlayerState(playerid) != PLAYER_STATE_ONFOOT || OyuncuBilgi[playerid][oGiris] == false) return HataMesaji(playerid, "su an bu secenegi kullanamazsin.");
				if(OyuncuBilgi[playerid][oHapisSure][0] > 0 || OyuncuBilgi[playerid][oHapisSure][1] > 0) return HataMesaji(playerid, "Hapiste bu secenegi kullanamazsin.");
				if(OyuncuBilgi[playerid][oYarali] > 0) return HataMesaji(playerid, "Yaraliyken bu secenegi kullanamazsin.");
				if(Ev_SahipKontrol(playerid, id)) return HataMesaji(playerid, "Sahibi oldugun evi kiralayamazsin.");

				EvBilgi[id][evKiraci] = OyuncuBilgi[playerid][oID];
				format(EvBilgi[id][evKiraciIsim], 32, "%s", OyuncuIsim(playerid));
				ParaEkle(playerid, -EvBilgi[id][evKiraUcret]);
				EvBilgi[id][evKasa] += EvBilgi[id][evKiraUcret];
				Ev_Kaydet(id);
				Ev_Yenile(id);
			}
			else if(listitem == 3) {
				if(OyuncuBilgi[playerid][oGiris] == false) return HataMesaji(playerid, "su anda bu secenegi kullanamazsin.");
				if(OyuncuBilgi[playerid][oDonduruldu] > 0) return HataMesaji(playerid, "su anda bu secenegi kullanamazsin.");
				if(OyuncuBilgi[playerid][oHapisSure][0] > 0 || OyuncuBilgi[playerid][oHapisSure][1] > 0) return HataMesaji(playerid, "Hapiste bu secenegi kullanamazsin.");
				if(OyuncuBilgi[playerid][oYarali] > 0) return HataMesaji(playerid, "Yaraliyken bu secenegi kullanamazsin.");
				if(OyuncuBilgi[playerid][oAdminDurum]) return HataMesaji(playerid, "Yonetici isbasindayken bu secenegi kullanamazsin.");
				if(EvBilgi[id][evKiraci] != OyuncuBilgi[playerid][oID]) return HataMesaji(playerid, "Bu evin kiracisi degilsin.");

				DialogGoster(playerid, DIALOG_KIRACI_AYRIL, DIALOG_STYLE_MSGBOX, "> Kira - Ayril", "Kiraciliktan ayrilmak istiyor musun?", "{B70000}Evet", "Hayir");
			}
			else if(listitem == 4) {
		 	    if(EvBilgi[id][evDurum] != 5) return HataMesaji(playerid, "Kapi tamir edilebilir durumda degil.");
				if(OyuncuBilgi[playerid][oPara] < 3000) return HataMesaji(playerid, "Yeterli paran yok.");

                ParaEkle(playerid, -3000);
				EvBilgi[id][evDurum] = 3;
				EvBilgi[id][evBakimSure] = 15;
				Ev_Yenile(id);
				Ev_Kaydet(id);
				SunucuMesaji(playerid, "Kapi tamir islemi basladi, 15 dakika sonra tamir islemi tamamlanacak.");
			}
			else if(listitem == 5) {
		 	    if(EvBilgi[id][evDurum] == 1) return HataMesaji(playerid, "Bu evin kapilari zaten acik.");
			    if(EvBilgi[id][evDurum] == 3) return HataMesaji(playerid, "Bakimdaki evin kapisi kirilamaz.");
			    if(EvBilgi[id][evDurum] == 4) return HataMesaji(playerid, "Muhurlu evin kapisi kirilamaz.");

			    ApplyAnimation(playerid, "POLICE", "Door_Kick", 4.0, 0, 0, 0, 0, 0);
				cmd_me(playerid, "gerilir ve evin kapisina tekme atar.");
			    SetTimerEx("KapiKir_Ev", 1500, false, "dd", playerid, id);
			}
			else if(listitem == 6) {
                if(!Ev_SahipKontrol(playerid, id)) return HataMesaji(playerid, "Bu evin sahibi degilsin, bu islemi yapamazsin.");
				if(EvBilgi[id][evLevel] > 4) return HataMesaji(playerid, "Bu ev zaten maksimum levele ulasmis.");
				if(OyuncuBilgi[playerid][oPara] < EV_LEVEL_UCRET) return HataMesaji(playerid, "Level yukseltme ucretini karsilayamiyorsun. (%s)", NumaraFormati(EV_LEVEL_UCRET));

				ParaEkle(playerid, -EV_LEVEL_UCRET);
				EvBilgi[id][evLevel]++;
				EvBilgi[id][evDurum] = 3;
				EvBilgi[id][evBakimSure] = 60;
				Ev_Yenile(id);
				Ev_Kaydet(id);
				SunucuMesaji(playerid, "Evin yukseltme islemi basladi, 60 dakika sonra yukseltme islemi bitecektir.");
		  		new log_icerik[144], oyuncu_isim[MAX_PLAYER_NAME + 1];
		 		GetPlayerName(playerid, oyuncu_isim, sizeof(oyuncu_isim));
				format(log_icerik, sizeof(log_icerik), "[%s] %d(%d) ev levelini yukseltti, yeni level: %d/SQL: %d", Tarih(), oyuncu_isim, OyuncuBilgi[playerid][oID], EvBilgi[id][evLevel], EvBilgi[id][evID]);
				//Gunluk_LogKaydetKaydet(log_icerik);
				Log_Kaydet("loglar/ev.txt", "%s", log_icerik);
			}
			else if(listitem == 7) {
                if(!Ev_SahipKontrol(playerid, Ev_Yakin(playerid)))
                    return HataMesaji(playerid, "Bu secenegi sadece ev sahibi kullanabilir.");

				switch(EvBilgi[id][evKiralik]) {
					case 0: SunucuMesaji(playerid, "Ev artik kiralanabilir."), EvBilgi[id][evKiralik] = 1;
					case 1: SunucuMesaji(playerid, "Ev artik kiralanamaz."), EvBilgi[id][evKiralik] = 0;
				}
				EvBilgi[id][evKiraciIsim][0] = 0;
				Ev_Yenile(id);
				return Ev_Kaydet(id);
			}
			else if(listitem == 8) {
                if(!Ev_SahipKontrol(playerid, Ev_Yakin(playerid)))
                    return HataMesaji(playerid, "Bu secenegi sadece ev sahibi kullanabilir.");

				if(EvBilgi[id][evKiralik] == 0) return HataMesaji(playerid, "Bu ev kiralik degil.");
				return DialogGoster(playerid, DIALOG_KIRA_UCRET, DIALOG_STYLE_INPUT, "> Kira ucretini Degistir", "Lutfen yeni kira ucretini girin:", "Tamam", "iptal");
			}
			else if(listitem == 9) {
				if(Ev_SahipKontrol(playerid, id)) {
				    if(EvBilgi[id][evKiraci] == 0) return HataMesaji(playerid, "Kiracin yok.");

					EvBilgi[id][evKiraci] = 0;
					SunucuMesaji(playerid, "%s adli kiracini cikarttin.", EvBilgi[id][evKiraciIsim]);
				    EvBilgi[id][evKiraciIsim][0] = 0;
				    Ev_Kaydet(id);
				    return Ev_Yenile(id);
				}
				else return HataMesaji(playerid, "Bu evin sahibi degilsin.");
			}
			else if(listitem == 10) {
			    if(Ev_SahipKontrol(playerid, id)) {
			        new icerik[144];
			        if(EvBilgi[id][evSatilik] > 0) format(icerik, sizeof(icerik), "Satilik Durumunu Kaldir\nSisteme Sat\n");
			        else if(EvBilgi[id][evSatilik] == 0) format(icerik, sizeof(icerik), "Satiliga cikar\nSisteme Sat\n");

					return DialogGoster(playerid, DIALOG_EVKAPI_SAT, DIALOG_STYLE_LIST, "> Evi Sat", icerik, "Sec", "Kapat");
			    }
				else return HataMesaji(playerid, "Bu evin sahibi degilsin.");
			}
			else if(listitem == 1) {
				EvBilgi[id][evDurum] = 4;
				Ev_Yenile(id);
				Ev_Kaydet(id);
				cmd_me(playerid, "evi muhurledi.");
			}

    return 1;
}

Dialog:DIALOG_EV_SAHIP(playerid, response, listitem, inputtext[])
{
    if(!response) return false;
	    new id = Oyuncu_Evde(playerid);

		if(listitem == 0) {
		    if(Oyuncu_EvKapida(playerid) == -1 || Oyuncu_EvKapida(playerid) != id) return HataMesaji(playerid, "Kilit durumunu degistirmek icin kapiya yakin olmalisin.");

			switch(EvBilgi[id][evDurum]) {
				case 1: EvBilgi[id][evDurum] = 2, Ev_Yenile(id), cmd_me(playerid, "evin kapisini kilitler.");
				case 2: {
					EvBilgi[id][evDurum] = 1, Ev_Yenile(id), cmd_me(playerid, "evin kapisinin kilidini acar.");
					foreach(new i : Player) if(MaymuncukVeri[i][mTur] == 2 && MaymuncukVeri[playerid][mHedef] == id) return SunucuMesaji(i, "Maymuncuk kirildi."), MaymuncukVeri[i][mSaniye] = 0, OyuncuBilgi[i][oMaymuncuk]--;
				}
				default: HataMesaji(playerid, "Bu evin kilit durumunu degistiremezsin.");
			}
		}
		else if(listitem == 1) return cmd_evlamba(playerid, inputtext);
		else if(listitem == 2) return Dialog_Show(playerid, DIALOG_MOBILYALAR, DIALOG_STYLE_TABLIST, "> Ev - Mobilyalar", "Mobilya Ekle\nMobilya Duzenle\nMobilya Sil\nTum Mobilyalari Sil", "Sec", "<< Geri");
		else if(listitem == 3) return Dialog_Show(playerid, DIALOG_DOLAPLAR, DIALOG_STYLE_TABLIST, "Ev - Kiyafet Dolaplari", "Dolap Ekle\nDolap Kaldir", "Sec", "<< Geri");
		else if(listitem == 4)
		{
		    if(!Ev_SahipKontrol(playerid, id)) return HataMesaji(playerid, "Bulundugun ev senin degil.");
			if(Raf_Sayi(id, 1) >= EvBilgi[id][evLevel]) return HataMesaji(playerid, "Ev levelinize gore en fazla %d adet raf alabilirsiniz.", EvBilgi[id][evLevel]);
			if(Raf_Yakin(playerid) != -1) return HataMesaji(playerid, "Yakinlarda baska bir raf var.");
			if(OyuncuBilgi[playerid][oPara] < RAF_UCRET) return HataMesaji(playerid, "Yeterli paran yok.");

			id = Raf_Yarat(playerid, id, 1);
			if(id == -1) return HataMesaji(playerid, "Sunucu maksimum raf limitine ulasti.");

		    EditlemeSifirla(playerid);
			OyuncuBilgi[playerid][oDuzenlenenRaf] = id;
			EditDynamicObject(playerid, RafBilgi[id][rafObje][4]);

			ParaEkle(playerid, -RAF_UCRET);
			SunucuMesaji(playerid, "Raf satin almak icin %s odedin.", NumaraFormati(RAF_UCRET));
			SunucuMesaji(playerid, "{FFFF00}~k~~PED_SPRINT~{FFFFFF} tusuna basili tutarak kamerayi oynatabilirsiniz.");
   		}
		else if(listitem == 5)
		{
		    if(!Ev_SahipKontrol(playerid, id)) return HataMesaji(playerid, "Bulundugun ev senin degil.");

		    return DialogGoster(playerid, DIALOG_RAF_DUZENLE, DIALOG_STYLE_INPUT, "> Raf Duzenleme", "{FFFFFF}Lutfen duzenlemek istediginiz rafin ID'sini girin;", "Tamam", "Kapat");
   		}
   		else if(listitem == 6)
		{
		    if(!Ev_SahipKontrol(playerid, id)) return HataMesaji(playerid, "Bulundugun ev senin degil.");
			if((id = Raf_Yakin(playerid)) == -1) return HataMesaji(playerid, "Rafa yakin degilsin.");
			new sayi = 0;
			for(new i = 0; i < 4; i ++) if(RafBilgi[id][rafSilahlar][i]) sayi++;
			if(sayi > 0) return HataMesaji(playerid, "Rafta esya varken silemezsin.");

			Raf_Sil(id);
			SunucuMesaji(playerid, "Rafi basariyla sildin.");
		}
    return 1;
}


Dialog:DIALOG_MOBILYALAR(playerid, response, listitem, inputtext[])
{
    if(!response) return cmd_ev(playerid, inputtext);
        new id = Oyuncu_Evde(playerid);

		if(!strcmp(inputtext, "Tum Mobilyalari Sil", true)) {
		    if(!Ev_SahipKontrol(playerid, id)) return HataMesaji(playerid, "Bulundugun ev senin degil.");
			if(Mobilya_Sayi(id) == 0) return HataMesaji(playerid, "Evinizde mobilya bulunmuyor.");

			new sayi = Mobilyalari_Sifirla(id);
			SunucuMesaji(playerid, "Evinizdeki butun mobilyalari(%d) sildiniz.", sayi);
		}
		else if(!strcmp(inputtext, "Mobilya Ekle", true)) {
		    if(!Ev_SahipKontrol(playerid, id) && !Ev_AnahtarKontrol(playerid, id)) return HataMesaji(playerid, "Bu evin sahibi degilsin ve anahtarin yok.");
			switch(EvBilgi[id][evLevel]) {
				case 1: if(Mobilya_Sayi(id) >= 50) return HataMesaji(playerid, "Daha fazla mobilya ekleyebilmek icin ev levelinizi yukseltmeniz gerekiyor.");
