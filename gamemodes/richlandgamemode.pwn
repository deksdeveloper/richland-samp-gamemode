CMD:helperyap(playerid, params[])
{
	if(OyuncuBilgi[playerid][oAdmin] < 5) return YetkinizYok(playerid);

	new oyuncu, helperleveli;
	if(sscanf(params, "ud", oyuncu, helperleveli)) return KullanimMesaji(playerid, "/helperyap [oyuncu id/isim] [helper leveli]");
	if(helperleveli < 0 || helperleveli > 4) return HataMesaji(playerid, "Girilen helper seviyesi gecersizdir. (0-3)");
	if(!IsPlayerConnected(oyuncu)) return HataMesaji(playerid, "Girilen kisi oyunda degil.");
	if(oyuncu == playerid) return HataMesaji(playerid, "Kendi helper seviyeni degistiremezsin.");
	if(OyuncuBilgi[oyuncu][oAdmin]) return HataMesaji(playerid, "Girilen kisi bir yonetici, helper seviyesi degistirilemez.");
	if(OyuncuBilgi[oyuncu][oHelper] == helperleveli) return HataMesaji(playerid, "Oyuncu zaten bu seviye herperlige sahip.");

	if(OyuncuBilgi[oyuncu][oHelper] < helperleveli) {
	    SunucuMesaji(playerid, "%s adli kisinin helper seviyesini %s olarak yukselttin.", OyuncuIsim(oyuncu), Helper_RutbeIsim(helperleveli));
	    SunucuMesaji(oyuncu, "%s adli yetkili tarafindan helper seviyen %s olarak yukseltildi.", OyuncuBilgi[playerid][oAdminNick], Helper_RutbeIsim(helperleveli));
	}
	else if(helperleveli == 0) {
		SunucuMesaji(playerid, "%s adli kisiyi helper kadrosundan cikardin.", OyuncuIsim(oyuncu));
		SunucuMesaji(oyuncu, "%s adli kisi tarafindan helper kadrosundan cikarildin.", OyuncuBilgi[playerid][oAdminNick]);
	}
	else if(OyuncuBilgi[oyuncu][oHelper] > helperleveli) {
	    SunucuMesaji(playerid, "%s adli yetkilinin helper seviyesini %s olarak dusurdun.", OyuncuIsim(oyuncu), Helper_RutbeIsim(helperleveli));
	    SunucuMesaji(oyuncu, "%s adli yetkili tarafindan helper seviyen %s olarak dusuruldu.", OyuncuBilgi[playerid][oAdminNick], Helper_RutbeIsim(helperleveli));
	}
	OyuncuBilgi[oyuncu][oHelper] = helperleveli;
	new log_icerik[144], oyuncu_isim[MAX_PLAYER_NAME + 1];
	GetPlayerName(oyuncu, oyuncu_isim, sizeof(oyuncu_isim));
	format(log_icerik, sizeof(log_icerik), "[%s] %s(%d), %s(%d) adli kisinin helper levelini %s yapti.", Tarih(), OyuncuBilgi[playerid][oAdminNick], OyuncuBilgi[playerid][oID], oyuncu_isim, OyuncuBilgi[oyuncu][oID], Helper_RutbeIsim(helperleveli));
	//Gunluk_LogKaydetKaydet(log_icerik);
	Log_Kaydet("loglar/yonetim.txt", "%s", log_icerik);
	return 1;
}

CMD:adminyap(playerid, params[])
{
	if(OyuncuBilgi[playerid][oAdmin] < 6) return YetkinizYok(playerid);

	new oyuncu, adminleveli;
	if(sscanf(params, "ud", oyuncu, adminleveli)) return KullanimMesaji(playerid, "/adminyap [oyuncu isim/id] [admin leveli]");
	if(adminleveli < 0 || adminleveli > 7) return HataMesaji(playerid, "Girilen yonetici seviyesi gecersizdir. (0-7)");
	if(!IsPlayerConnected(oyuncu)) return HataMesaji(playerid, "Girilen kisi oyunda degil.");
	if(oyuncu == playerid) return HataMesaji(playerid, "Kendi yonetici seviyeni degistiremezsin.");
	if(OyuncuBilgi[oyuncu][oAdmin] == adminleveli) return HataMesaji(playerid, "Oyuncu zaten bu seviye yoneticilige sahip.");
	if(adminleveli > OyuncuBilgi[playerid][oAdmin]) return HataMesaji(playerid, "Kendinden yuksek seviye yoneticilik veremezsin.");
	if(OyuncuBilgi[playerid][oAdmin] < OyuncuBilgi[oyuncu][oAdmin]) return HataMesaji(playerid, "Kendinden yuksek seviye bir yoneticinin seviyesini degistiremezsin.");

	if(OyuncuBilgi[oyuncu][oAdmin] < adminleveli) {
	    SunucuMesaji(playerid, "%s adli kisinin yonetici seviyesini %d olarak yukselttin.", OyuncuIsim(oyuncu), adminleveli);
	    SunucuMesaji(oyuncu, "%s adli yetkili tarafindan yonetici seviyen %d olarak yukseltildi.", OyuncuBilgi[playerid][oAdminNick], adminleveli);
	}
	else if(adminleveli == 0) {
		SunucuMesaji(playerid, "%s adli kisiyi yonetici kadrosundan cikardin.", OyuncuIsim(oyuncu));
		SunucuMesaji(oyuncu, "%s adli kisi tarafindan yonetim kadrosundan cikarildin.", OyuncuBilgi[playerid][oAdminNick]);
		if(OyuncuBilgi[oyuncu][oIzlenen] != INVALID_PLAYER_ID) cmd_izle(oyuncu, "bitir");
		if(OyuncuBilgi[oyuncu][oAdminDurum] == true) cmd_aisbasi(oyuncu, "");
		format(OyuncuBilgi[oyuncu][oAdminNick], 32, "Yok");
	}
	else if(OyuncuBilgi[oyuncu][oAdmin] > adminleveli) {
	    SunucuMesaji(playerid, "%s adli yetkilinin yonetici seviyesini %d olarak dusurdun.", OyuncuIsim(oyuncu), adminleveli);
	    SunucuMesaji(oyuncu, "%s adli yetkili tarafindan yonetici seviyen %d olarak dusuruldu.", OyuncuBilgi[playerid][oAdminNick], adminleveli);
	}
	OyuncuBilgi[oyuncu][oAdmin] = adminleveli;
	if(adminleveli > 0 && OyuncuBilgi[oyuncu][oHelper] > 0) OyuncuBilgi[oyuncu][oHelper] = 0;
	new log_icerik[144], oyuncu_isim[MAX_PLAYER_NAME + 1];
	GetPlayerName(oyuncu, oyuncu_isim, sizeof(oyuncu_isim));
	format(log_icerik, sizeof(log_icerik), "[%s] %s(%d), %s(%d) adli kisinin admin levelini %d yapti.", Tarih(), OyuncuBilgi[playerid][oAdminNick], OyuncuBilgi[playerid][oID], oyuncu_isim, OyuncuBilgi[oyuncu][oID], adminleveli);
	//Gunluk_LogKaydetKaydet(log_icerik);
	Log_Kaydet("loglar/yonetim.txt", "%s", log_icerik);
	return 1;
}

CMD:vipyap(playerid, params[])
{
	if(OyuncuBilgi[playerid][oAdmin] < 7) return YetkinizYok(playerid);

	new oyuncuid, sure, level, viptext[64];
	if(sscanf(params, "udI(30)", oyuncuid, level, sure)) return KullanimMesaji(playerid, "/vipyap [ID/Isim] [seviye] [gun] (-1 sinirsiz)");

	if(!IsPlayerConnected(oyuncuid)) return HataMesaji(playerid, "Girilen kisi oyunda degil.");
	if(sure < -1) return HataMesaji(playerid, "Gun en az -1 olmalidir.");
	if(sure == 0) return HataMesaji(playerid, "Girilen gun 0 olamaz.");
	if(level < 0 || level > 3) return HataMesaji(playerid, "Girilen VIP seviyesi gecersiz. (0-3)");
	if(level == OyuncuBilgi[oyuncuid][oVIP]) return HataMesaji(playerid, "Girilen oyuncu zaten bu seviye VIP'ye sahip.");

	OyuncuBilgi[oyuncuid][oVIP] = level;
	if(OyuncuBilgi[oyuncuid][oVIP] > 0 && sure > -1) {
		new gun = sure * 86400;
		OyuncuBilgi[oyuncuid][oVIPSure] = gettime() + gun;
		format(viptext, sizeof(viptext), "%s gun", NumaraFormati(sure, ""));
	}
	if(sure == -1) OyuncuBilgi[oyuncuid][oVIPSure] = sure, format(viptext, sizeof(viptext), "Sinirsiz");
	if(level == 0) {
		SunucuMesaji(playerid, "%s adli kisinin VIP uyeligini iptal ettin.", OyuncuIsim(oyuncuid));
		SunucuMesaji(oyuncuid, "%s adli yetkili tarafindan VIP uyeligin iptal edildi.", OyuncuBilgi[playerid][oAdminNick]);
	}
	else {
	    SunucuMesaji(playerid, "%s adli kisinin VIP uyeligini %s(%s) olarak guncelledin.", OyuncuIsim(oyuncuid), VIPIsim(oyuncuid), viptext);
	    SunucuMesaji(oyuncuid, "%s adli yetkili tarafindan VIP seviyen %s(%s) olarak guncellendi.", OyuncuBilgi[playerid][oAdminNick], VIPIsim(oyuncuid), viptext);
	}
	new log_icerik[144], oyuncu_isim[MAX_PLAYER_NAME + 1];
	GetPlayerName(oyuncuid, oyuncu_isim, sizeof(oyuncu_isim));
	format(log_icerik, sizeof(log_icerik), "[%s] %s(%d), %s(%d) adli kisinin VIP levelini %d(%s) yapti.", Tarih(), OyuncuBilgi[playerid][oAdminNick], OyuncuBilgi[playerid][oID], oyuncu_isim, OyuncuBilgi[oyuncuid][oID], level, viptext);
	//Gunluk_LogKaydetKaydet(log_icerik);
	Log_Kaydet("loglar/yonetim.txt", "%s", log_icerik);
	return 1;
}

CMD:vipdolap(playerid, params[])
{
	if(OyuncuBilgi[playerid][oVIP] < 1) return HataMesaji(playerid, "VIP degilsin, dolabi kullanamazsin.");
	if(!IsPlayerInRangeOfPoint(playerid, 3.0, -2108.7786, 4509.6445, 65.7870)) return HataMesaji(playerid, "VIP dolabina yakin degilsin.");
	if(OyuncuBilgi[playerid][oAdminDurum] == true) return HataMesaji(playerid, "Yetkili isbasinda bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oHapisSure][0] > 0 || OyuncuBilgi[playerid][oHapisSure][1] > 0) return HataMesaji(playerid, "Hapisteyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oYarali] > 0 || OyuncuBilgi[playerid][oYaraliSure] > 0) return HataMesaji(playerid, "Yaraliyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oDonduruldu] > 0) return HataMesaji(playerid, "Etkisiz haldeyken bu komutu kullanamazsin.");
	if(IsPlayerInAnyVehicle(playerid)) return HataMesaji(playerid, "Bu komut arac icerisinde kullanilamaz.");

	new dolap_icerik[144], zirh_ucret = YAKUT_ZIRH_UCRET;
	switch(OyuncuBilgi[playerid][oVIP]) {
	    case 1: zirh_ucret = YAKUT_ZIRH_UCRET;
	    case 2: zirh_ucret = SAFIR_ZIRH_UCRET;
	    case 3: zirh_ucret = ELMAS_ZIRH_UCRET;
	    default: zirh_ucret = YAKUT_ZIRH_UCRET;
	}
	format(dolap_icerik, sizeof(dolap_icerik), "Zirh\t\t%s\nSilah Dolabi\t\t\n", NumaraFormati(zirh_ucret));
	DialogGoster(playerid, DIALOG_VIP_DOLAP, DIALOG_STYLE_TABLIST, "VIP Dolabi", dolap_icerik, "Sec", "Kapat");
	return 1;
}

