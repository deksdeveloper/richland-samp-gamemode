stock Log_Kaydet(const path[], const str[], {Float,_}:...)
{
	static args, start, end, File:file, string[1024];
	if((start = strfind(path, "/")) != -1) {
	    strmid(string, path, 0, start + 1);

	   if(!fexist(string)) fcreate(string);
	}
	#emit LOAD.S.pri 8
	#emit STOR.pri args

	file = fopen(path, io_append);

	if(!file)
	    return 0;

	if(args > 8)
	{
		#emit ADDR.pri str
		#emit STOR.pri start

	    for(end = start + (args - 8); end > start; end -= 4)
		{
	        #emit LREF.pri end
	        #emit PUSH.pri
		}
		#emit PUSH.S str
		#emit PUSH.C 1024
		#emit PUSH.C string
		#emit PUSH.C args
		#emit SYSREQ.C format

		fwrite(file, string);
		fwrite(file, "\r\n");
		fclose(file);

		#emit LCTRL 5
		#emit SCTRL 4
				if (!strcmp(path, "loglar/admin_log.txt", true))
		{
		AdminLogDCM(string, "1023222060354387988", "Set-Player");
		}
		if (!strcmp(path, "loglar/admin_silah.txt", true))
		{
		AdminLogDCM(string, "1023222060354387988", "Yaralanma");//
		}
		if (!strcmp(path, "logs/para.txt", true))
		{
		AdminLogDCM(string, "1023222060354387988", "Admin-Log");//
		}
		if (!strcmp(path, "logs/rapor.txt", true))
		{
		AdminLogDCM(string, "1023222060354387988", "Pay-Log");//
		}
		if (!strcmp(path, "logs/admin_arac.txt", true))
		{
		AdminLogDCM(string, "1023222060354387988", "Anti-Hile");//
		}
		#emit RETN
	}
	fwrite(file, str);
	fwrite(file, "\r\n");
	fclose(file);
	return 1;
}

