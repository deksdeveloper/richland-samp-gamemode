	     {
			 sayi++;
		     format(string, sizeof(string), "{FFFFFF}%d\t{FFFFFF}%s\t{FFFFFF}%d/sn\t{FF6347}%s\n", i, Duraklar[i][durakAd], Duraklar[i][durakSure], NumaraFormati(Duraklar[i][durakFiyat]));
	         strcat(str, string);
		 }
	}
	if (sayi == 0) return HataMesaji(playerid, "Hic durak bulunamadi.");
	format(string, sizeof(string), "Otobus Duraklari (%d adet)", sayi);
	Dialog_Show(playerid, Duraklar, DIALOG_STYLE_TABLIST_HEADERS, string, str, "Sec", "Cikis");
	return 1;
}

Dialog:Duraklar(playerid, response, listitem, inputtext[])
{
	if (!response) return OyuncuBilgi[playerid][pDurak] = -1, OyuncuBilgi[playerid][pSecilenDurak] = -1;
	if (response)
	{
	    new id = strval(inputtext);
	    if (Duraklar[id][Exists] == true)
	    {
	        OyuncuBilgi[playerid][pSecilenDurak] = id;
	        new str[256];
			format(str, sizeof(str), ""GR"%s"B" Ucretle, "R"%d/sn"B" boyunca "R"%s"B" duragina gitmek istiyor musun?", NumaraFormati(Duraklar[id][durakFiyat]), Duraklar[id][durakSure], Duraklar[id][durakAd]);
			Dialog_Show(playerid, DurakOnay, DIALOG_STYLE_MSGBOX, "Durak Onay", str, "Evet", "Hayir");
	    }
	}
	return 1;
}

Dialog:DurakOnay(playerid, response, listitem, inputtext[])
{
	if(!response)
	{
		if(OyuncuBilgi[playerid][pSecilenDurak] != -1)
		{
			HataMesaji(playerid, ""R"%s"B" duragina gitmeyi reddettin.", Duraklar[OyuncuBilgi[playerid][pSecilenDurak]][durakAd]);
			OyuncuBilgi[playerid][pSecilenDurak] = -1;
			OyuncuBilgi[playerid][pDurak] = -1;
		}
		else
		{
			HataMesaji(playerid, "Durak sisteminde sorun olustu!");
		}
	}
	if(response)
	{
		if(OyuncuBilgi[playerid][pSecilenDurak] != -1)
		{
			if(OyuncuBilgi[playerid][oPara] < Duraklar[OyuncuBilgi[playerid][pSecilenDurak]][durakFiyat]) return HataMesaji(playerid, "Yeterli miktarda paran yok.");
			SunucuMesaji(playerid, ""GR"%s"B" ucretle, "R"%d/sn"B" boyunca "R"%s"B" duragina gitmeyi kabul ettin!", NumaraFormati(Duraklar[OyuncuBilgi[playerid][pSecilenDurak]][durakFiyat]), Duraklar[OyuncuBilgi[playerid][pSecilenDurak]][durakSure], Duraklar[OyuncuBilgi[playerid][pSecilenDurak]][durakAd]);
			SunucuMesaji(playerid, "Yolculuk basliyor, iyi yolculuklar dileriz! Bakiyenizden "R"-%s"B" eksildi!", NumaraFormati(Duraklar[OyuncuBilgi[playerid][pSecilenDurak]][durakFiyat]));
			OyuncuPosDegistir(playerid, 2022.0996,2235.5178,2103.9536);
			SetPlayerFacingAngle(playerid, 1.2925);
	//		ObjelerYukleniyor(playerid);
			new sure = Duraklar[OyuncuBilgi[playerid][pSecilenDurak]][durakSure]*1000;
			SetTimerEx("DurakTimer", sure, false, "i", playerid);
			OyuncuBilgi[playerid][pOtobusKartBakiye] -= Duraklar[OyuncuBilgi[playerid][pSecilenDurak]][durakFiyat];
			OyuncuBilgi[playerid][pDurak] = -1;
		}
		else
		{
			HataMesaji(playerid, "Durak sisteminde sorun olustu!");
		}
	}
	return 1;
}

forward DurakTimer(playerid);
public DurakTimer(playerid)
{
	SunucuMesaji(playerid, "Basariyla "R"%s"B" duragina vardiniz.", Duraklar[OyuncuBilgi[playerid][pSecilenDurak]][durakAd]);
	OyuncuPosDegistir(playerid, Duraklar[OyuncuBilgi[playerid][pSecilenDurak]][durakPos][0], Duraklar[OyuncuBilgi[playerid][pSecilenDurak]][durakPos][1], Duraklar[OyuncuBilgi[playerid][pSecilenDurak]][durakPos][2]);
    OyuncuBilgi[playerid][pSecilenDurak] = -1;
    OyuncuBilgi[playerid][pDurak] = -1;
	return 1;
}

stock DurakYakin(playerid)
{
    for (new i = 0; i != MAX_DURAK; i ++) if (GetPlayerVirtualWorld(playerid) == 0 && IsPlayerInRangeOfPoint(playerid, 5.0, Duraklar[i][durakPos][0], Duraklar[i][durakPos][1], Duraklar[i][durakPos][2]))
