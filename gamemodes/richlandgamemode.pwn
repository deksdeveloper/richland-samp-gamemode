	    }
	}
	return true;
}

CMD:swapseats(playerid, params[])
{
	if(!IsPlayerInAnyVehicle(playerid)) return HataMesaji(playerid, "Bu komutu sadece aracta kullanabilirsin.");

    if(OyuncuBilgi[playerid][oHapisSure][0] > 0 || OyuncuBilgi[playerid][oHapisSure][1] > 0) return HataMesaji(playerid, "Hapisteyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oYarali] > 0 || OyuncuBilgi[playerid][oYaraliSure] > 0) return HataMesaji(playerid, "Yaraliyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oDonduruldu] > 0) return HataMesaji(playerid, "Etkisiz haldeyken bu komutu kullanamazsin.");
	new hedefid, kullananarac = GetPlayerVehicleID(playerid);
	if(sscanf(params, "u", hedefid))
	{
		KullanimMesaji(playerid, "/swapseats [Oyuncu Adi/ID]");
		KullanimMesaji(playerid, "Sadece aracinda bulunan kisilerin ID'sini yazarak istek yollayabilirsin.");
		return 1;
	}
	if(GetPlayerVehicleID(hedefid) != kullananarac) return HataMesaji(playerid, "Istek gondermek istediginiz kisiyle ayni aracta degilsin.");
	else
	{
        OyuncuBilgi[playerid][koltukistek] = hedefid;
        OyuncuBilgi[playerid][istekyolladi] = 1;

        OyuncuBilgi[hedefid][koltukistek] = playerid;
        OyuncuBilgi[hedefid][istekyolladi] = 0;


		SunucuMesaji(playerid, "%s(%d) isimli oyuncuya koltuk desistirme istegi yolladin.", OyuncuIsim(hedefid), hedefid);
		HataMesaji(hedefid, "Yeni koltuk degistirme istegi aldigin icin eski koltuk degistirme istekleri silindi.", OyuncuIsim(hedefid), hedefid);
		SunucuMesaji(hedefid, "%s(%d) isimli oyuncu sana koltuk degistirme istegi yolladi. (/swapkabul %d)", OyuncuIsim(playerid), playerid, playerid);
	}
	return 1;
}
CMD:kd(playerid, params[]) return cmd_swapseats(playerid, params);
CMD:koltukdegistir(playerid, params[]) return cmd_swapseats(playerid, params);

CMD:swapkabul(playerid, params[])
{
    if(!IsPlayerInAnyVehicle(playerid)) return HataMesaji(playerid, "Bu komutu sadece aracta kullanabilirsin.");

    if(OyuncuBilgi[playerid][oHapisSure][0] > 0 || OyuncuBilgi[playerid][oHapisSure][1] > 0) return HataMesaji(playerid, "Hapisteyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oYarali] > 0 || OyuncuBilgi[playerid][oYaraliSure] > 0) return HataMesaji(playerid, "Yaraliyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oDonduruldu] > 0) return HataMesaji(playerid, "Etkisiz haldeyken bu komutu kullanamazsin.");
	
	new hedefid;
	if(sscanf(params, "u", hedefid)) return KullanimMesaji(playerid, "/swapkabul [Oyuncu Adi/ID]");
    
    if(OyuncuBilgi[playerid][oGiris] == false)
        return HataMesaji(playerid, "Gecersiz ID veya oyuncu daha sifresini girmemis.");

	if((OyuncuBilgi[hedefid][koltukistek] == playerid && OyuncuBilgi[hedefid][istekyolladi] == 1 && OyuncuBilgi[playerid][istekyolladi] == 0 && OyuncuBilgi[playerid][koltukistek] == hedefid))
	{
		if(IsPlayerInAnyVehicle(playerid) && GetPlayerVehicleID(playerid) == GetPlayerVehicleID(hedefid))
		{
			KoltukDegistir(playerid, hedefid);
			SunucuMesaji(playerid, "Basariyla %s(%d) isimli oyuncuyla koltuklarinizi degistirdin.", OyuncuIsim(hedefid), hedefid);
		    OyuncuBilgi[hedefid][koltukistek] = -1;
		    OyuncuBilgi[playerid][koltukistek] = -1;
        }
		else
			return HataMesaji(playerid, "Koltuk degistirme istegini kabul etmek istedigin kisiyle ayni aracta degilsin!");

		return 1;
	}
	else
	{
		HataMesaji(playerid, "Bu oyuncu size koltuk degistirme istegi atmamis.");
	}
	return 1;
}

stock KoltukDegistir(degistiren, degisecek)
{
	if(!IsPlayerInAnyVehicle(degistiren)) return HataMesaji(degistiren, "Herhangi bir aracta bulunmadiginiz icin koltuk degistirilemedi.");
	if(!IsPlayerInAnyVehicle(degisecek)) return HataMesaji(degisecek, "Herhangi bir aracta bulunmadiginiz icin koltuk degistirilemedi.");

	new aracid = GetPlayerVehicleID(degistiren), seatid = GetPlayerVehicleSeat(degistiren), seatid2 = GetPlayerVehicleSeat(degisecek);

	new Float:x, Float:y, Float:z;

	SetPVarInt(degistiren, "carsurfkoruma", 1);
	SetPVarInt(degisecek, "carsurfkoruma", 1);

	GetPlayerPos(degistiren, x, y, z);
	SetPlayerPos(degistiren, x, y, z+2);

	GetPlayerPos(degisecek, x, y, z);
	SetPlayerPos(degisecek, x, y, z+2);

	SetTimerEx("KoltukDegisimIslem", 300, false, "ddd", degistiren, seatid2, aracid);
	
	SetTimerEx("KoltukDegisimIslem", 500, false, "ddd", degisecek, seatid, aracid);
	return 1;
}

forward KoltukDegisimIslem(playerid, seat, aracid);
public KoltukDegisimIslem(playerid, seat, aracid)
{
	SetPVarInt(playerid, "carsurfkoruma", 0);
	PutPlayerInVehicle(playerid, aracid, seat);
	return 1;
}
