stock BasvuruKontrol(playerid)
{
	new query[512], durum, dcdurum;
	mysql_format(mysqlB, query, sizeof(query), "SELECT `Durum`, `DCDurum` FROM `oyuncular` WHERE `isim` = '%s'", Player_GetName(playerid));
	new Cache:isimarat = mysql_query(mysqlB, query);
	if(cache_num_rows())
	{
       cache_get_value_name_int(0, "Durum", durum);
       cache_get_value_name_int(0, "DCDurum", dcdurum);
	}
	if(durum == 0) return DialogGoster(playerid, 0, DIALOG_STYLE_MSGBOX, "> Uyari", "Yaptiginiz basvuru hala onay surecinde!\nSunucudan atildin.", "Tamam", ""), SetTimerEx("KickTimer", 2000, false, "d", playerid);
	if(dcdurum == 0) return DialogGoster(playerid, 0, DIALOG_STYLE_MSGBOX, "> Uyari", "Sunucuya giris yapabilmek icin hesabinizi Discord uzerinden eslestirmeniz gerekmekte.\nSunucudan atildin.", "Tamam", ""), SetTimerEx("KickTimer", 2000, false, "d", playerid);
	cache_delete(isimarat);
	
	new oyuncu_isim[MAX_PLAYER_NAME + 1], isim, ip[24], bilgi[528];
	GetPlayerName(playerid, oyuncu_isim, sizeof(oyuncu_isim));
	SohbetTemizle(playerid, false);
	SetPlayerColor(playerid, RENK_VARSAYILAN);
    GetPlayerIp(playerid, ip, sizeof(ip));
	format(bilgi, sizeof(bilgi), "{FFFFFF}Richland Roleplay'in eglence dolu dunyasina tekrardan hos geldin {afafaf}%s{FFFFFF}, eglenceye donmek icin asagidaki kutuya sifreni gir.\n\n{FFFFFF}NOT: Sakin unutma, giris yapmak icin 30 saniyen var.", OyuncuIsim(playerid));
	DialogGoster(playerid, DIALOG_GIRIS, DIALOG_STYLE_PASSWORD, "Richland Roleplay: Giris", bilgi, "Tamam", "iptal");
	if(!IsPlayerAdmin(playerid) && !!strcmp(OyuncuIP(playerid), "127.0.0.1")) OyuncuBilgi[playerid][oGirisTimer] = SetTimerEx("GirisKick", 30 * 1000, false, "d", playerid);
	if(OyuncuBilgi[playerid][oGiris] == false) 
	{
        OyuncuPosDegistir(playerid, 1045.441040, -918.800231, -80.0);
		InterpolateCameraPos(playerid, 437.747619, -1469.347900, 36.590827, 1045.441040, -918.800231, 92.159584, 300);
		InterpolateCameraLookAt(playerid, 442.437072, -1467.623413, 36.778285, 1049.376220, -921.691528, 93.234367, 300);
	}
	return 1;
}

