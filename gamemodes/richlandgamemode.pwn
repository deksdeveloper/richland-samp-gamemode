	format(yazi, sizeof(yazi), "(interior: %d) - (VW: %d) - (Maas: %d/60dk) - (Uyarilar: %d) - (Hapis Suresi: %d dk)", vericek_int(0, "interior"), vericek_int(0, "vw"), vericek_int(0, "Dakika"), vericek_int(0, "Uyari"), hapis);
	SendClientMessageEx(playerid, RENK_KARAKTER, "%s", yazi);
	SendClientMessage(playerid, RENK_KARAKTER, "--------------------------------------------------------------------------------------------------------");
	return 1;
}
stock oyuncuDataleri(gosterilenid, oyuncuid)
{
	static ustlevel, yazi[144], bilgimesaji[1152], hesapdurumu[16], vipdurumu[16];
	ustlevel = OyuncuBilgi[oyuncuid][oSeviye] * LEVEL_EXP;
	static birlik[64], rutbe[64], uniforma[6], kiyafet;
	if(OyuncuBilgi[oyuncuid][oBirlik] > -1) {
		format(birlik, sizeof(birlik), "%s", BirlikBilgi[OyuncuBilgi[oyuncuid][oBirlik]][birlikAd]);
		format(rutbe, sizeof(rutbe), "%s", OyuncuRutbeAdi(oyuncuid));
	}
	else if(OyuncuBilgi[oyuncuid][oBirlik] == -1) birlik = "Yok", rutbe = "Yok";
	switch(OyuncuBilgi[oyuncuid][oGeciciKiyafet]) {
		case 0: uniforma = "Yok";
		default: {
			if(OyuncuBilgi[oyuncuid][oGeciciKiyafet] >= 20000) format(uniforma, sizeof(uniforma), "%d", OyuncuBilgi[oyuncuid][oGeciciKiyafet] - 19689);
			else if(OyuncuBilgi[oyuncuid][oGeciciKiyafet] < 20000) format(uniforma, sizeof(uniforma), "%d", OyuncuBilgi[oyuncuid][oGeciciKiyafet]);
		}
	}
	switch(OyuncuBilgi[oyuncuid][oVIP])
	{
		case 1: vipdurumu = "VIP";
		default: vipdurumu = "Yok";
	}
	if(OyuncuBilgi[oyuncuid][oAdmin] > 0) format(hesapdurumu, sizeof(hesapdurumu), "%s", Admin_RutbeIsim(OyuncuBilgi[oyuncuid][oAdmin]));
	else if(OyuncuBilgi[oyuncuid][oHelper] > 0) hesapdurumu = "Tester";
	else if(OyuncuBilgi[oyuncuid][oAdmin] < 1 && OyuncuBilgi[oyuncuid][oHelper] < 1) hesapdurumu = "Oyuncu";
	bilgimesaji[0] = 0;
	SendClientMessageEx(gosterilenid, 0xAA00FFFF, "____________________________________[%s]____________________________________", OyuncuIsim(oyuncuid));
	format(yazi, sizeof(yazi), "|| Karakter: %s | Seviye: %d | Seviye EXP: %d/%d | Kiyafet: %d | uniforma: %s", OyuncuIsim(oyuncuid), OyuncuBilgi[oyuncuid][oSeviye], OyuncuBilgi[oyuncuid][oExp], ustlevel, kiyafet, uniforma);
	SendClientMessageEx(gosterilenid, RENK_KOYUMAVI, "%s", yazi);
	format(yazi, sizeof(yazi), "|| Cinsiyet: %s | Dogum Yeri: %s | Ten Rengi: %s", (OyuncuBilgi[oyuncuid][oCinsiyet] == 1) ? ("Erkek") : ("Kadin"), OyuncuBilgi[oyuncuid][oDogumYeri], (OyuncuBilgi[oyuncuid][oTenRengi] == 1) ? ("Beyaz") : ("Siyah"));
	SendClientMessageEx(gosterilenid, RENK_GRI, "%s", yazi);
	format(yazi, sizeof(yazi), "|| Hesap ID: %d | Para: %s | Oynama Saati: %d | Hesap: %s", OyuncuBilgi[oyuncuid][oHesap], NumaraFormati(OyuncuBilgi[oyuncuid][oPara]), OyuncuBilgi[oyuncuid][oOynamaSaati], hesapdurumu);
	SendClientMessageEx(gosterilenid, RENK_KOYUMAVI, "%s", yazi);
	format(yazi, sizeof(yazi), "|| Birlik: %s | Rutbe: %s | VIP Durumu: %s", birlik, rutbe, vipdurumu);
	SendClientMessageEx(gosterilenid, RENK_GRI, "%s", yazi);
	format(yazi, sizeof(yazi), "|| Telefon: %s | Telefon Numarasi: %d | Banka Hesabi: %s | Hesap No: %d", (OyuncuBilgi[oyuncuid][oTelefon] == 0) ? ("Yok") : ("Mevcut"), OyuncuBilgi[oyuncuid][oTelefonNumarasi], NumaraFormati(OyuncuBilgi[oyuncuid][oBankaPara]), OyuncuBilgi[oyuncuid][oHesapNumarasi]);
	SendClientMessageEx(gosterilenid, RENK_KOYUMAVI, "%s", yazi);
	format(yazi, sizeof(yazi), "|| Son Giris: %s | Meslek: %s", OyuncuBilgi[oyuncuid][oSonGiris], MeslekIsim(OyuncuBilgi[oyuncuid][oMeslek]));
	SendClientMessageEx(gosterilenid, RENK_GRI, "%s", yazi);
	format(yazi, sizeof(yazi), "|| Payday: %d dakika | Maas: %d dakika", 60 - OyuncuBilgi[oyuncuid][oDakika], 60 - OyuncuBilgi[oyuncuid][oDakika]);
	SendClientMessageEx(gosterilenid, RENK_KOYUMAVI, "%s", yazi);
	strcat(bilgimesaji, yazi);
	//DialogGoster(gosterilenid, 0, DIALOG_STYLE_MSGBOX, OyuncuIsim(oyuncuid), bilgimesaji, "Kapat", "");
	return 1;
}
public Uyari_Kontrol(playerid, isim[], sebep[])
{
	new rows, fields;
	cache_get_row_count(rows);
	cache_get_field_count(fields);
	if(!rows) {
		for(new i = 0, len = strlen(isim); i < len; i ++) if(isim[i] == '_') isim[i] = ' ';
		return HataMesaji(playerid, "%s adli oyuncu veritabaninda bulunamadi.", isim);
	}
	new uyari = vericek_int(0, "Uyari"), oyuncu_id = vericek_int(0, "ID"), oyuncu_isim[MAX_PLAYER_NAME + 1], yasaklama[64];
	format(yasaklama, sizeof(yasaklama), "%s", vericek_varchar(0, "Yasaklama"));
	if(!isnull(yasaklama) || uyari > 4) return HataMesaji(playerid, "Bu oyuncu yasaklanmis.");

	format(oyuncu_isim, sizeof(oyuncu_isim), "%s", vericek_varchar(0, "isim"));
	format(isim, MAX_PLAYER_NAME + 1, "%s", vericek_varchar(0, "isim"));
	for(new i = 0, len = strlen(isim); i < len; i ++) if(isim[i] == '_') isim[i] = ' ';
	new sorgu[256];
	format(sorgu, sizeof(sorgu),"UPDATE `oyuncular` SET `Uyari` = '%d' WHERE `ID` = '%d'", uyari + 1, oyuncu_id);
	mysql_query(mysqlB, sorgu);
	SunucuMesaji(playerid, "%s adli oyuncuya %s sebebiyle offline uyari verdiniz.", oyuncu_isim, sebep);
	new bildirim_icerik[144];
	format(bildirim_icerik, sizeof(bildirim_icerik), "%s adli yetkili karakterinize %s sebebiyle offline uyari ekledi.", OyuncuBilgi[playerid][oAdminNick], sebep);
	format(sorgu, sizeof(sorgu), "INSERT INTO `bildirimler` (`Eklenen`, `Sebep`, `Tarih`, `Ekleyen`) VALUES ('%s', '%s', '%s', '%s')", oyuncu_isim, bildirim_icerik, Tarih(), "Sistem");
	mysql_tquery(mysqlB, sorgu);
	new log_icerik[144];
	format(log_icerik, sizeof(log_icerik), "[%s] %s(%d), %s adli kisiye offline uyari verdi. sebep: %s", Tarih(), OyuncuBilgi[playerid][oAdminNick], OyuncuBilgi[playerid][oID], oyuncu_isim, sebep);
	//Gunluk_LogKaydetKaydet(log_icerik);
	Log_Kaydet("loglar/admin_ban.txt", "%s", log_icerik);
	if(uyari + 1 >= 5) {
		format(sorgu, sizeof(sorgu), "UPDATE `oyuncular` SET `Yasaklama` = '%s' WHERE `ID` = '%d'", "5 Uyari", oyuncu_id);
		mysql_tquery(mysqlB, sorgu);
		format(log_icerik, sizeof(log_icerik), "[%s] %s(%d), %s(%d) adli kisinin karakterini yasakladi. (sebep: %s)", Tarih(), OyuncuBilgi[playerid][oAdminNick], OyuncuBilgi[playerid][oID], oyuncu_isim, oyuncu_id, sebep);
		//Gunluk_LogKaydetKaydet(log_icerik);
		Log_Kaydet("loglar/ban.txt", "%s", log_icerik);
	}
	return 1;
}

