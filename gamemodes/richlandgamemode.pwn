Dialog:DIALOG_VPMARKET(playerid, response, listitem, inputtext[])
{
    if(!response) return 0;

		new vnpoint = 0;
        if(listitem == 0) {
		    if(OyuncuBilgi[playerid][oRCCoin] < RICH_KMSIFIRLA) return HataMesaji(playerid, "Bu urunun R-Coin ucretini karsilayamiyorsun.");
			if(!IsPlayerInAnyVehicle(playerid) || GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return HataMesaji(playerid, "Arac kullanmiyorsun.");

			new id = Arac_IDBul(GetPlayerVehicleID(playerid));
			if(!Arac_SahipKontrol(playerid, id)) return HataMesaji(playerid, "Surdugun arac senin degil.");

            vnpoint = RICH_KMSIFIRLA;
			AracBilgi[id][aracKM] = 0.0;
			SunucuMesaji(playerid, "Basariyla Kilometre Sifirlama satin aldin. "R"(-%d)"B"", RICH_KMSIFIRLA);
			Arac_Kaydet(id);
		}
		else if(listitem == 1)
		{
		    if(OyuncuBilgi[playerid][oRCCoin] < RICH_OZELNUMARA) return HataMesaji(playerid, "Bu urunun R-Coin ucretini karsilayamiyorsun.");
		    if(OyuncuBilgi[playerid][oTelefon] == 0) return HataMesaji(playerid, "Telefonun yok, urun kullanmak icin oncelikle telefon almalisin.");

			return DialogGoster(playerid, DIALOG_VNMARKET_OZELNUMARA, DIALOG_STYLE_INPUT, "Richland Roleplay: ozel Telefon Numarasi", "{FFFFFF}Asagidaki kutuya degistirmek istediginiz numarayi girin;\n\n{B70000}NOT: {FFFFFF}Yeni numaraniz 4 harfli sayilardan olusmalidir.", "Tamam", "Kapat");
		}
		if(listitem == 2)
		{
			if(OyuncuBilgi[playerid][oRCCoin] < RICH_SILAHHAKKI) return HataMesaji(playerid, "Bu urunun R-Coin ucretini karsilayamiyorsun.");

			vnpoint = RICH_SILAHHAKKI;
			OyuncuBilgi[playerid][oSilahHakki]++;
			SunucuMesaji(playerid, "Basariyla Silah Hakki satin aldin. "R"(-%d)"B"", RICH_SILAHHAKKI);
		}
		else if(listitem == 3)
		{
		    if(OyuncuBilgi[playerid][oRCCoin] < RICH_MASKEHAKKI) return HataMesaji(playerid, "Bu urunun R-Coin ucretini karsilayamiyorsun.");
			if(OyuncuBilgi[playerid][oMaske] == 1) return HataMesaji(playerid, "Maske kullanimi urunun bu karakterinde bulunuyor.");

            vnpoint = RICH_MASKEHAKKI;
			OyuncuBilgi[playerid][oMaske] = 1;
			if(OyuncuBilgi[playerid][oMaskeID] == 0) {
				new maskeno = randomEx(10000, 99999), sorgu[256];
				format(sorgu, sizeof(sorgu), "SELECT * FROM oyuncular WHERE MaskeID = '%d'", maskeno);
				mysql_tquery(mysqlB, sorgu, "Maske_Sorgula", "dd", playerid, maskeno);
			}
			OyuncuKaydet(playerid);
			SunucuMesaji(playerid, "Basariyla Maske Hakki satin aldin. "R"(-%d)"B"", RICH_MASKEHAKKI);
		}
		else if(listitem == 4) {
			if(OyuncuBilgi[playerid][oRCCoin] < RICH_YAKUTVIP) return HataMesaji(playerid, "Bu urunun R-Coin ucretini karsilayamiyorsun.");
			if(OyuncuBilgi[playerid][oVIP] > 0) return HataMesaji(playerid, "Hesabinda Yakut veya uzeri bir VIP seviyesine sahipsin, urunu almak icin vip surenin bitmesini bekle.");

            vnpoint = RICH_YAKUTVIP;
			OyuncuBilgi[playerid][oVIP] = 1;
			new gun = 15 * 86400;
			OyuncuBilgi[playerid][oVIPSure] = gettime() + gun;
		}
		else if(listitem == 5) {
			if(OyuncuBilgi[playerid][oRCCoin] < RICH_SAFIRVIP) return HataMesaji(playerid, "Bu urunun R-Coin ucretini karsilayamiyorsun.");
			if(OyuncuBilgi[playerid][oVIP] >= 2) return HataMesaji(playerid, "Hesabinda Safir veya uzeri bir VIP seviyesine sahipsin, urunu almak icin vip surenin bitmesini bekle.");

            vnpoint = RICH_SAFIRVIP;
			OyuncuBilgi[playerid][oVIP] = 2;
			new gun = 15 * 86400;
			OyuncuBilgi[playerid][oVIPSure] = gettime() + gun;
		}
		else if(listitem == 6) {
			if(OyuncuBilgi[playerid][oRCCoin] < RICH_ELMASVIP) return HataMesaji(playerid, "Bu urunun R-Coin ucretini karsilayamiyorsun.");
			if(OyuncuBilgi[playerid][oVIP] == 3) return HataMesaji(playerid, "Hesabinda Elmas VIP urunune sahipsin, urunu almak icin vip surenin bitmesini bekle.");

            vnpoint = RICH_ELMASVIP;
			OyuncuBilgi[playerid][oVIP] = 3;
			new gun = 10 * 86400;
			OyuncuBilgi[playerid][oVIPSure] = gettime() + gun;
		}
	return 1;
}

