public IPYasaklama_Kontrol(playerid)
{
	new rows, sorgu[128], isim[MAX_PLAYER_NAME];
	cache_get_row_count(rows);
	if(rows)
	{
		HataMesaji(playerid, "IP adresin %s sebebiyle yasakli oldugu icin oyundan atildin.", vericek_varchar(0, "Sebep"));
