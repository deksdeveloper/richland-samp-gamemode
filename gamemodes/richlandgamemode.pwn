CMD:arackirala(playerid, params[])
{
	if(!IsPlayerInAnyVehicle(playerid)) return HataMesaji(playerid, "Bu komut sadece aracta kullanilabilir.");
	if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return HataMesaji(playerid, "Bu komutu sadece aracin surucu koltugundayken kullanilabilir.");
	static gerekliucret, id, oid;
    id = Arac_IDBul(GetPlayerVehicleID(playerid));

	if(AracBilgi[id][aracSahip] == 0) gerekliucret = 150;
	else gerekliucret = AracBilgi[id][aKiraUcret];

	if(OyuncuBilgi[playerid][oPara] < gerekliucret) return HataMesaji(playerid, "Bu araci kiralamak icin yeterli paran yok.");
	if(AracBilgi[id][aracKiralik] == 0) return HataMesaji(playerid, "Bu arac kiralik degil.");
	if(AracBilgi[id][aracAnahtar] > 0) return HataMesaji(playerid, "Bu arac zaten kiralanmis.");
	if(AracBilgi[id][aracSahip] == OyuncuBilgi[playerid][oID]) return HataMesaji(playerid, "Kendi aracini kiralayamazsin.");

	AracBilgi[id][aracAnahtar] = OyuncuBilgi[playerid][oID];
	AracBilgi[id][aKiraZaman] = 30;
    Arac_Kaydet(id);
    ParaEkle(playerid, -gerekliucret);
   	new log_icerik[144], oyuncu_isim[MAX_PLAYER_NAME + 1];
   	GetPlayerName(playerid, oyuncu_isim, sizeof(oyuncu_isim));
	format(log_icerik, sizeof(log_icerik), "[%s] %s(%d), %s model arac kiraladi.", Tarih(), oyuncu_isim, OyuncuBilgi[playerid][oID], AracModel(AracBilgi[id][aracModel]));
	//Gunluk_LogKaydetKaydet(log_icerik);
	Log_Kaydet("loglar/arac.txt", "%s", log_icerik);
	oid = SQL_AktiflikKontrol(AracBilgi[id][aracSahip]);
	if(oid != -1 && IsPlayerConnected(oid)) {
		OyuncuBilgi[oid][oBankaPara] += AracBilgi[id][aKiraUcret];
		OyuncuKaydet(oid);
		SunucuMesaji(oid, "**300 numarali banka hesabindan banka hesabina %s miktarinda transfer gerceklesti. (Arac Kiralama)", NumaraFormati(AracBilgi[id][aKiraUcret]));
	}
	else if(AracBilgi[id][aracSahip] > 0) OfflineParaVer(AracBilgi[id][aracSahip], AracBilgi[id][aKiraUcret]);
	SunucuMesaji(playerid, "Arac 30 dakikaligina kiralandi, iyi yolculuklar!");
	return 1;
}

CMD:aracabin(playerid, params[])
{
	if(OyuncuBilgi[playerid][oAdmin] < 3) return YetkinizYok(playerid);
	if(OyuncuBilgi[playerid][oAdminDurum] == false && OyuncuBilgi[playerid][oAdmin] < 6) return IsbasiYapmalisin(playerid);
	if(OyuncuBilgi[playerid][oHapisSure][0] > 0 || OyuncuBilgi[playerid][oHapisSure][1] > 0) return HataMesaji(playerid, "Hapisteyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oYarali] > 0 || OyuncuBilgi[playerid][oYaraliSure] > 0) return HataMesaji(playerid, "Yaraliyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oDonduruldu] > 0) return HataMesaji(playerid, "Etkisiz haldeyken bu komutu kullanamazsin");
	if(OyuncuBilgi[playerid][oKelepceli] == 1) return HataMesaji(playerid, "Kelepceliyken bu komutu kullanamazsin.");
	if(GetPlayerState(playerid) == PLAYER_STATE_SPECTATING || OyuncuBilgi[playerid][oIzlenen] != INVALID_PLAYER_ID) return HataMesaji(playerid, "Birini izlerken bu komutu kullanamazsin.");

	new aracid;
	if(sscanf(params, "d", aracid)) return KullanimMesaji(playerid, "/aracabin [arac id]");
	if(aracid == INVALID_VEHICLE_ID || !IsValidVehicle(aracid)) return HataMesaji(playerid, "Bu arac gecersiz.");

	new koltukid = KoltukKontrol(aracid, 0);
	if(koltukid == -1) return HataMesaji(playerid, "Aracta bos koltuk yok.");

	PutPlayerInVehicle(playerid, aracid, koltukid);
	return 1;
}

