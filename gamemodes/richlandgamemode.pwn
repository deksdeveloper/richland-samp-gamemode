CMD:kemerkontrol(playerid, params[])
{
static oyuncuid, emote[128];
    if (sscanf(params, "u", oyuncuid))
        return SunucuMesaji(playerid, "/kemerkontrol [id/isim]");
	if(OyuncuBirlikTuru(playerid) != BIRLIK_POLIS) return HataMesaji(playerid, "Polis memuru degilsin.");
	if(!IsPlayerConnected(oyuncuid)) return HataMesaji(playerid, "Girilen kisi oyunda degil.");
		if(!YakinlikKontrol(playerid, oyuncuid, 5.0))
		return HataMesaji(playerid, "Girilen oyuncuya yakin degilsin.");

	SendClientMessage(oyuncuid, RENK_GRI, "-----------------------------------------------------------");
   SendClientMessageEx(oyuncuid, RENK_GRI, "Emniyet Kemeri: %s", (OyuncuBilgi[playerid][oEmniyetKemeri] == false) ? ("{B70000}Takili degil") : ("{33AA33}Takili"));
	SendClientMessage(oyuncuid, RENK_GRI, "-----------------------------------------------------------");
