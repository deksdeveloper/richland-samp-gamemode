CMD:gasp(playerid, params[])
{
	if(OyuncuBilgi[playerid][oGiris] == false) return HataMesaji(playerid, "Giris yapmadan komut kullanamazsin.");
	if(OyuncuBilgi[playerid][oSeviye] < 8) return HataMesaji(playerid, "Bu komutu 8 level ve ustu kullanabilir.");
	if(OyuncuBilgi[playerid][oMeslek] != 4) return HataMesaji(playerid, "Hirsiz mesleginde degilsin.");
	if(OyuncuBilgi[playerid][oAdminDurum] == true) return HataMesaji(playerid, "Admin isbasinda bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oYaraliSure] > 0) return HataMesaji(playerid, "Yerde yaraliyken gasp yapamazsin.");
	if(OyuncuBilgi[playerid][oDonduruldu] > 0) return HataMesaji(playerid, "Etkisiz haldeyken gasp yapamazsin.");
	if(OyuncuBilgi[playerid][oKelepceli] == 1) return HataMesaji(playerid, "Kelepceliyken gasp yapamazsin.");
	if(OyuncuBilgi[playerid][oHapisSure][0] > 0 || OyuncuBilgi[playerid][oHapisSure][1] > 0) return HataMesaji(playerid, "Hapisteyken gasp yapamazsin.");
	if(OyuncuBirlikTuru(playerid) == BIRLIK_POLIS || OyuncuBirlikTuru(playerid) == BIRLIK_MEDIKAL) return HataMesaji(playerid, "Polis veya medikal birlikleri gasp yapamaz.");
	if(OyuncuBilgi[playerid][oGasp] != INVALID_PLAYER_ID) return HataMesaji(playerid, "su anda bir gasp yapiyorsun, islemin bitmesini bekle.");
	if(OyuncuBilgi[playerid][oGaspSure] > 0) {
		SendClientMessageEx(playerid, RENK_SERVER, "[GASP] {FFFFFF}Yeni gasp icin kalan sure: %s", ConvertTime(OyuncuBilgi[playerid][oGaspSure] - gettime()));
		return 1;
	}

	new oyuncuid;
	if(sscanf(params, "u", oyuncuid)) return KullanimMesaji(playerid, "/gasp [oyuncu id/isim]");
	if(!IsPlayerConnected(oyuncuid)) return HataMesaji(playerid, "Gecersiz oyuncu.");
	if(playerid == oyuncuid) return HataMesaji(playerid, "Kendini gasp edemezsin.");
	if(!YakinlikKontrol(playerid, oyuncuid, 4.0)) return HataMesaji(playerid, "Oyuncuya yakin degilsin.");
	if(OyuncuBilgi[oyuncuid][oGiris] == false) return HataMesaji(playerid, "Bu oyuncu giris yapmamis.");
	if(OyuncuBilgi[oyuncuid][oSeviye] < 5) return HataMesaji(playerid, "Gasp edeceginiz kisi en az 5 Level olmalidir.");
	if(OyuncuBilgi[oyuncuid][oAdminDurum] == true) return HataMesaji(playerid, "isbasindaki bir admini gasp edemezsin.");
	if(OyuncuBilgi[oyuncuid][oHapisSure][0] > 0 || OyuncuBilgi[oyuncuid][oHapisSure][1] > 0) return HataMesaji(playerid, "Hapisteki bir oyuncuyu gasp edemezsin.");
	if(OyuncuBilgi[oyuncuid][oDialog] != -1) return HataMesaji(playerid, "Oyuncu baska bir diyalog kullaniyor lutfen bekleyiniz.");
	if(OyuncuBilgi[oyuncuid][oGasp] != INVALID_PLAYER_ID) return HataMesaji(playerid, "Oyuncu baska birisiyle gasp sistemini kullaniyor, lutfen bekleyin.");
	if((OyuncuBirlikTuru(oyuncuid) == BIRLIK_POLIS || OyuncuBirlikTuru(oyuncuid) == BIRLIK_MEDIKAL) && OyuncuBilgi[oyuncuid][oIsbasi] == true) return HataMesaji(playerid, "isbasindaki devlet kurumu uyeleri gasp edilemez.");

	SunucuMesaji(playerid, "Gasp isteginiz %s adli kisiye iletildi.", OyuncuIsim(oyuncuid));
	new icerik[144 + MAX_PLAYER_NAME];
	OyuncuBilgi[oyuncuid][oGasp] = playerid;
	OyuncuBilgi[playerid][oGasp] = oyuncuid;
	format(icerik, sizeof(icerik), "%s adli kisi tarafindan size gasp istegi gonderildi, karsinizdaki kisinin gasp sistemini kullanmasini onayliyor musunuz?", OyuncuIsim(playerid));
	DialogGoster(oyuncuid, DIALOG_GASP_ONAY, DIALOG_STYLE_MSGBOX, "> Gasp Onayi", icerik, "Kabul", "Red");
	return 1;
}

