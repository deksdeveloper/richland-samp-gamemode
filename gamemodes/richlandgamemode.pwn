					Silahver(playerid, WEAPON_SPRAYCAN, 500);
				}
				case 5: {
				    if(SilahSlotKontrol(playerid, SilahSlotBul[WEAPON_CAMERA]) && SilahSlotBul[WEAPON_CAMERA] != 0) return HataMesaji(playerid, "Bu silah turu sizde bulundugu icin bu ekipmani alamiyorsunuz.");
					Silahver(playerid, WEAPON_CAMERA, 1000);
				}
				case 6: {
				    if(SilahSlotKontrol(playerid, SilahSlotBul[WEAPON_COLT45]) && SilahSlotBul[WEAPON_COLT45] != 0) return HataMesaji(playerid, "Bu silah turu sizde bulundugu icin bu ekipmani alamiyorsunuz.");
					Silahver(playerid, WEAPON_COLT45, 119);
				}
				case 7: {
				    if(SilahSlotKontrol(playerid, SilahSlotBul[WEAPON_M4]) && SilahSlotBul[WEAPON_M4] != 0) return HataMesaji(playerid, "Bu silah turu sizde bulundugu icin bu ekipmani alamiyorsunuz.");
					Silahver(playerid, WEAPON_M4, 500);
				}
			}
	    }
	    else if(!response) cmd_dolap(playerid, "");
	return 1;
}
	
Dialog:DIALOG_POLIS_DOLAP(playerid, response, listitem, inputtext[])
{
	if(!response) return 0;

		if(!strcmp(inputtext, "isbasi", true)) {
			if(OyuncuBilgi[playerid][oIsbasi] == true) {
				cmd_me(playerid, "ekipmanlarini birakir.");
				OyuncuBilgi[playerid][oIsbasi] = false;
				SilahlariSifirla(playerid);
                if(OyuncuBilgi[playerid][oOynamaSaati] >= 12) Silahver(playerid, WEAPON_DEAGLE, 70);
				SetPlayerColor(playerid, RENK_VARSAYILAN);
				ZirhDuzenle(playerid, 0);
				OyuncuBilgi[playerid][oGeciciKiyafet] = 0;
				SetPlayerSkin(playerid, OyuncuBilgi[playerid][oKiyafet]);
			}
			else {
				cmd_me(playerid, "dolaptan rozetini ve ekipmanlarini alir.");
				OyuncuBilgi[playerid][oIsbasi] = true;
				SetPlayerColor(playerid, RENK_PDISBASI);
				SilahlariSifirla(playerid);
				ZirhDuzenle(playerid, 100);
                if(OyuncuBilgi[playerid][oOynamaSaati] >= 12) {
					Silahver(playerid, WEAPON_DEAGLE, 70);
					Silahver(playerid, WEAPON_NITESTICK, 1);
					Silahver(playerid, WEAPON_SPRAYCAN, 500);
				}
			}
			IsimLabelGuncelle(playerid);
		}
		if(!strcmp(inputtext, "Ekipmanlar", true)) {
			if(OyuncuBilgi[playerid][oIsbasi] == false) return HataMesaji(playerid, "isbasinda degilken ekipman alamazsin.");

			Dialog_Show(playerid, DIALOG_POLIS_EKIPMAN, DIALOG_STYLE_TABLIST_HEADERS, "> LSPD - Ekipman", "Ekipman Adi\t\tAdet\nDesert Eagle\t\t(Mermi: 70)\nShotgun\t\t(Mermi: 70)\nMP5\t\t(Mermi: 300)\nSniper Rifle\t\t(Mermi: 20)\nSprey\t\t(Mermi: 500)\nKamera\t\t(Mermi: 1000)\n9mm\t\t(Mermi: 119)\nM4\t\t(Mermi: 500)", "Sec", "iptal");
		}
		if(!strcmp(inputtext, "celik Yelek", true)) {
			if(techizatizni == false && OyuncuBilgi[playerid][oBirlikRutbe] < 11) return HataMesaji(playerid, "Bu secenek tegmen ve uzeri rutbeler tarafindan kullanilabilir.");
			if(OyuncuBilgi[playerid][oIsbasi] == false) return HataMesaji(playerid, "isbasinda degilken zirh alamazsin.");

			ZirhDuzenle(playerid, 100);
			cmd_me(playerid, "dolabi aralar ve icerisinden celik yelegini alir, uzerine gecirir.");
		}
		if(!strcmp(inputtext, "uniformalar", true)) {
			if(OyuncuBilgi[playerid][oIsbasi] == false) return HataMesaji(playerid, "isbasinda degilken uniforma alamazsin.");
			new kiyafetler[sizeof(LSPDUniforma_ErkekBeyaz)];
			for(new c = 0; c != sizeof(LSPDUniforma_ErkekBeyaz); c++) kiyafetler[c] = LSPDUniforma_ErkekBeyaz[c][0];
            ShowModelSelectionMenuEx(playerid, kiyafetler, sizeof(kiyafetler), BirlikBilgi[OyuncuBilgi[playerid][oBirlik]][birlikAd], MODELLER_UNIFORMA, 0.0, 0.0, 0.0);
		}
		if(!strcmp(inputtext, "Aksesuarlar", true)) {
			new aksesuar[sizeof(PolisAksesuar)];
			for(new c = 0; c != sizeof(PolisAksesuar); c++) aksesuar[c] = PolisAksesuar[c][0];

			return ShowModelSelectionMenuEx(playerid, aksesuar, sizeof(aksesuar), BirlikBilgi[OyuncuBilgi[playerid][oBirlik]][birlikAd], MODELLER_AKSESUAR_PD);
		}
		if(!strcmp(inputtext, "Agri Kesici", true)) CanDuzenle(playerid, 100);
		if(!strcmp(inputtext, "Silah Sifirla", true)) SilahlariSifirla(playerid), SunucuMesaji(playerid, "Silahlarini biraktin.");
		if(!strcmp(inputtext, "Beanbag Birak", true)) {
			if(OyuncuBilgi[playerid][oBeanbag] == false) return HataMesaji(playerid, "Beanbag tufeginiz yok, dolaba birakamazsiniz.");

		    OyuncuBilgi[playerid][oBeanbag] = false;
			SilahAyarla(playerid);

			SetPlayerArmedWeapon(playerid, OyuncuBilgi[playerid][oSilah][3]);
		    cmd_me(playerid, "iki eliyle tuttugu beanbag tufegini sirtina asar.");
		}
	return 1;
}

