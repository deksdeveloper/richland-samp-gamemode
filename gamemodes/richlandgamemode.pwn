Dialog:DIALOG_BALIK_SAT(playerid, response, listitem, inputtext[])
{
    if(!response) return UyariMesaji(playerid, "Balik satmaktan vazgectin.");

    if(strfind(inputtext, "Hepsini Sat", true) != -1) 
    {
	        if(OyuncuBilgi[playerid][oBaliklar][0] < 1 && OyuncuBilgi[playerid][oBaliklar][1] < 1 && OyuncuBilgi[playerid][oBaliklar][2] < 1 && OyuncuBilgi[playerid][oBaliklar][3] < 1 && OyuncuBilgi[playerid][oBaliklar][4] < 1) return HataMesaji(playerid, "uzerinde satabilecegin balik yok.");
	        new kazanc = 0;
	        kazanc += OyuncuBilgi[playerid][oBaliklar][0] * GetServerBilgi("Balik1");
	        kazanc += OyuncuBilgi[playerid][oBaliklar][1] * GetServerBilgi("Balik2");
	        kazanc += OyuncuBilgi[playerid][oBaliklar][2] * GetServerBilgi("Balik3");
	        kazanc += OyuncuBilgi[playerid][oBaliklar][3] * GetServerBilgi("Balik4");
	        kazanc += OyuncuBilgi[playerid][oBaliklar][4] * GetServerBilgi("Balik5");
	        OyuncuBilgi[playerid][oBaliklar][0] = 0, OyuncuBilgi[playerid][oBaliklar][1] = 0, OyuncuBilgi[playerid][oBaliklar][2] = 0, OyuncuBilgi[playerid][oBaliklar][3] = 0, OyuncuBilgi[playerid][oBaliklar][4] = 0;
	        SunucuMesaji(playerid, "Uzerindeki butun baliklari sattin ve {33aa33}%s {FFFFFF}kazandin.", NumaraFormati(kazanc));

	        new mesajdeks = 0;
		    new toplamtutar = 0;
		    if(ikixmeslek == false)
		    {
		       ParaEkle(playerid, kazanc);
		    }
		    else
		    {
		 	   new ikixmeslekkazanc = kazanc*2;
		 	   ParaEkle(playerid, ikixmeslekkazanc);
			   toplamtutar = ikixmeslekkazanc;
			   SunucuMesaji(playerid, "2x meslek kazanci aktif oldugu icin toplamda %s kazandiniz.", NumaraFormati(ikixmeslekkazanc));
			}
		    return 1;
	}
	
	    new gramfiyati = 0, kazanc = 0, balikadi[32];
	    switch(listitem) {
			case 0: gramfiyati = GetServerBilgi("Balik1"), balikadi = "cipura";
			case 1: gramfiyati = GetServerBilgi("Balik2"), balikadi = "Levrek";
			case 2: gramfiyati = GetServerBilgi("Balik3"), balikadi = "Lagos";
			case 3: gramfiyati = GetServerBilgi("Balik4"), balikadi = "Lambuka";
			case 4: gramfiyati = GetServerBilgi("Balik5"), balikadi = "Orkinos";
	    }
	    if(OyuncuBilgi[playerid][oBaliklar][listitem] < 1) return HataMesaji(playerid, "%s turunde baliktan ustunde yok.", inputtext);
	    kazanc = OyuncuBilgi[playerid][oBaliklar][listitem] * gramfiyati;
	    OyuncuBilgi[playerid][oBaliklar][listitem] = 0;
        SunucuMesaji(playerid, "%s turundeki baliklarini sattin ve {33aa33}%s {FFFFFF}kazandin.", balikadi, NumaraFormati(kazanc));
	    new toplamtutar = 0;
	    if(ikixmeslek == false)
	    {
	       ParaEkle(playerid, kazanc);
	    }
	    else
	    {
	 	   new ikixmeslekkazanc = kazanc*2;
	 	   ParaEkle(playerid, ikixmeslekkazanc);
		   toplamtutar = ikixmeslekkazanc;
		   SunucuMesaji(playerid, "2x meslek kazanci aktif oldugu icin toplamda %s kazandiniz.", NumaraFormati(ikixmeslekkazanc));
	    }
	return 1;
}

