CMD:yat(playerid)
{
	if(IsPlayerInAnyVehicle(playerid)) return HataMesaji(playerid, "Bu komut aracta kullanilamaz.");
	if(OyuncuBilgi[playerid][oGiris] == false) return HataMesaji(playerid, "su anda bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oDonduruldu] > 0) return HataMesaji(playerid, "su anda bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oHapisSure][0] > 0 || OyuncuBilgi[playerid][oHapisSure][1] > 0) return HataMesaji(playerid, "Hapiste bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oYarali] > 0) return HataMesaji(playerid, "Yaraliyken bu komutu kullanamazsin.");
	new yat_bilgi[512];
	if(IsPlayerInRangeOfPoint(playerid, 3.0, 148.4463, -1854.9377, 3.7734))
 	{
 	    if(yat_kiralama == false) return HataMesaji(playerid, "Yat su anda kiralik degildir, management ile iletisime gecebilirsiniz.");
 		if(YatBilgi[yatKiralayan] != -1) return HataMesaji(playerid, "Yat baskasi tarafindan kiralanmis. (Kalan kira suresi: %d)", floatround(YatBilgi[yatKiraSure] / 60));
 		format(yat_bilgi, sizeof(yat_bilgi), "{FFFFFF}Bu yati saatlik kiralayabilirsiniz, kiralama ucreti {2ECC71}%s {FFFFFF}olarak belirlenmistir. Eger kira suresi bitimine kadar yati teslim etmezseniz para cezasina carptirilacaksiniz.\n\n{FFFFFF}Kira suresini uzatmak icin (/yat) komutunu kullanabilirsiniz.\n\n{FFFFFF}Bu yati kiralamak istediginize emin misiniz?", NumaraFormati(YAT_UCRET));
		DialogGoster(playerid, DIALOG_YAT_KIRALA, DIALOG_STYLE_MSGBOX, "Yat Kiralama", yat_bilgi, "Kirala", "iptal");
		return 1;
	}
    new Float:yatPos[3];
    GetObjectPos(YatBilgi[yatObject], yatPos[0], yatPos[1], yatPos[2]);
    if(!IsPlayerInRangeOfPoint(playerid, 20.0, yatPos[0], yatPos[1], yatPos[2]) && OyuncuBilgi[playerid][oYatKontrol][0] == 0) return HataMesaji(playerid, "Yatiniza yakin degilsiniz!");
    if(YatBilgi[yatKiralayan] != OyuncuBilgi[playerid][oID]) return HataMesaji(playerid, "Yat sizin uzerinize kiralanmamis.");
    new baslik[60];
    format(baslik, sizeof(baslik), "{FFFFFF}Yat: Yonetim Menusu (Sure: %d dakika)", floatround(YatBilgi[yatKiraSure] / 60));
    format(yat_bilgi, sizeof(yat_bilgi), "{FFFFFF}%s\t\t\n{FFFFFF}Muzik Ayarla\t\t\n{FFFFFF}Otomatik Pilot\t\t(%s)\n{FFFFFF}Kira Suresini Uzat\t\t({2ECC71}%s{FFFFFF})", (OyuncuBilgi[playerid][oYatKontrol][0] == 0) ? ("Kontrol Et") : ("Kontrolu Birak"), (YatBilgi[yatOto] == 1) ? ("{33aa33}Acik") : ("{b70000}Kapali"), NumaraFormati(YAT_UCRET));
    DialogGoster(playerid, DIALOG_YAT, DIALOG_STYLE_TABLIST, baslik, yat_bilgi, "Sec", "Kapat");
	return 1;
}

CMD:baliktut(playerid, params[])
{
	if(OyuncuBilgi[playerid][oAdminDurum] == true) return HataMesaji(playerid, "Yonetici olarak isbasindayken bu komutu kullanamazsin.");
	if(IsPlayerInAnyVehicle(playerid)) return HataMesaji(playerid, "Bu komut aracta kullanilamaz.");
	if(!IsPlayerNearWater(playerid)) return HataMesaji(playerid, "{FFFFFF}Burada balik tutamazsin.");
	if(GetPlayerState(playerid) != PLAYER_STATE_ONFOOT || OyuncuBilgi[playerid][oGiris] == false) return HataMesaji(playerid, "su anda bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oDonduruldu] > 0) return HataMesaji(playerid, "su anda bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oHapisSure][0] > 0 || OyuncuBilgi[playerid][oHapisSure][1] > 0) return HataMesaji(playerid, "Hapiste bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oYarali] > 0) return HataMesaji(playerid, "Yaraliyken bu komutu kullanamazsin.");
	if(PetKontrol[playerid] != -1) return HataMesaji(playerid, "Pet kontrol ederken bunu yapamazsin.");
	if(OyuncuBilgi[playerid][oBalikTutuyor] > 0) return HataMesaji(playerid, "Zaten su anda balik tutuyorsun.");
	//if(BalikPosYakin(playerid) == 0) return HataMesaji(playerid, "Santa Maria iskelesinde degilsin.");
	if(avyasagi == true) return HataMesaji(playerid, "Av yasagi var, su anda balik tutamazsin.");

	if(OyuncuBilgi[playerid][oYemler][0] == 0 && OyuncuBilgi[playerid][oYemler][1] == 0 && OyuncuBilgi[playerid][oYemler][2] == 0 && OyuncuBilgi[playerid][oYemler][3] == 0 && OyuncuBilgi[playerid][oYemler][4] == 0)
	    return HataMesaji(playerid, "uzerinde yem yok, balikci dukkanindan satin alabilirsin.");

	new balik_gram = 0;
	balik_gram += OyuncuBilgi[playerid][oBaliklar][0], balik_gram += OyuncuBilgi[playerid][oBaliklar][1], balik_gram += OyuncuBilgi[playerid][oBaliklar][2], balik_gram += OyuncuBilgi[playerid][oBaliklar][3], balik_gram += OyuncuBilgi[playerid][oBaliklar][4];
	if(balik_gram >= 750) return HataMesaji(playerid, "Daha fazla balik tutamazsin, ustundekileri satmalisin.");


	new yem_icerik[256];
	format(yem_icerik, sizeof(yem_icerik), "Yem\tTutma Orani\tuzerindeki Adet\nEkmek\t%s55\t%d adet\nBoru Kurdu\t%s65\t%d adet\nSahte Yem\t%s75\t%d adet\nTavuk Gogsu\t%s85\t%d adet\nKarides\t%s95\t%d adet", "%", OyuncuBilgi[playerid][oYemler][0], "%", OyuncuBilgi[playerid][oYemler][1], "%", OyuncuBilgi[playerid][oYemler][2], "%", OyuncuBilgi[playerid][oYemler][3], "%", OyuncuBilgi[playerid][oYemler][4]);
	DialogGoster(playerid, DIALOG_YEMSEC, DIALOG_STYLE_TABLIST_HEADERS, "Yem Secimi", yem_icerik, "Sec", "Kapat");
	return 1;
}

CMD:balikci(playerid, params[])
{
	if(OyuncuBilgi[playerid][oAdminDurum] == true) return HataMesaji(playerid, "Yonetici olarak isbasindayken bu komutu kullanamazsin.");
	if(IsPlayerInAnyVehicle(playerid)) return HataMesaji(playerid, "Bu komut aracta kullanilamaz.");
	if(GetPlayerState(playerid) != PLAYER_STATE_ONFOOT || OyuncuBilgi[playerid][oGiris] == false) return HataMesaji(playerid, "su anda bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oDonduruldu] > 0) return HataMesaji(playerid, "su anda bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oHapisSure][0] > 0 || OyuncuBilgi[playerid][oHapisSure][1] > 0) return HataMesaji(playerid, "Hapiste bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oYarali] > 0) return HataMesaji(playerid, "Yaraliyken bu komutu kullanamazsin.");
    if (IsPlayerInRangeOfPoint(playerid, 0, dini_Float("/Yazilar/Balikci.ini", "X"), dini_Float("/Yazilar/Balikci.ini", "Y"), dini_Float("/Yazilar/Balikci.ini", "Z"))) return HataMesaji(playerid, "Balikci noktasinda degilsin.");
	if(avyasagi == true) return HataMesaji(playerid, "Av yasagindan dolayi balikci izin yapiyor, sonra tekrar gelin!");

	DialogGoster(playerid, DIALOG_BALIKCI, DIALOG_STYLE_TABLIST, "Balikci", "Yemler\nBalik Sat\nBilgi Al", "Sec", "Kapat");
	return 1;
}

CMD:pompayarat(playerid, params[])
{
    if(OyuncuBilgi[playerid][oAdmin] < 5) return YetkinizYok(playerid);
    if(OyuncuBilgi[playerid][oAdminDurum] == false && OyuncuBilgi[playerid][oAdmin] < 6) return IsbasiYapmalisin(playerid);

	new id, isyeriid = -1, ucret;
	if(sscanf(params, "dd", isyeriid, ucret)) return KullanimMesaji(playerid, "/pompayarat [isyeri id] [litre ucreti]");
	if((isyeriid < 0 || isyeriid >= MAKSIMUM_ISYERI) || !IsyeriBilgi[isyeriid][isyeriKullanimda]) return HataMesaji(playerid, "Gecersiz isyeri.");
	if(IsyeriBilgi[isyeriid][isyeriTur] != 9) return HataMesaji(playerid, "isyeri turu benzin istasyonu degil.");
    if(GetPlayerInterior(playerid) > 0 || GetPlayerVirtualWorld(playerid) > 0) return HataMesaji(playerid, "Benzin pompalari interior ve farkli dunyalara yaratilamaz.");

	id = Pompa_Yarat(playerid, isyeriid, ucret);
	if(id == -1) return HataMesaji(playerid, "Benzin pompasi limitine ulasildigi icin yeni benzin pompasi yaratilamiyor.");

	SunucuMesaji(playerid, "Benzin pompasi basariyla yaratildi. (ID: %d)", id);
	EditDynamicObject(playerid, BenzinPompasi[id][pompaObje]);
	OyuncuBilgi[playerid][oDuzenlenenPompa] = id;
	return 1;
}

CMD:apompa(playerid, params[])
{
    if(OyuncuBilgi[playerid][oAdmin] < 5) return YetkinizYok(playerid);
    if(OyuncuBilgi[playerid][oAdminDurum] == false && OyuncuBilgi[playerid][oAdmin] < 6) return IsbasiYapmalisin(playerid);

	new id, type[24], string[128];
	if(sscanf(params, "ds[24]S()[128]", id, type, string))
 	{
	 	KullanimMesaji(playerid, "/apompa [id] [isim]");
	    SendClientMessage(playerid, RENK_SARI, "[iSiMLER]:{FFFFFF} benzin, yer, ucret, tur, isyeri, x, y, z, a, sil");
		return 1;
	}
	if((id < 0 || id >= MAKSIMUM_BENZIN_POMPASI) || !BenzinPompasi[id][pompaKullanimda])
	    return HataMesaji(playerid, "Gecersiz benzin pompasi.");

    if(!strcmp(type, "benzin", true))
	{
		new miktar;
		if(sscanf(string, "d", miktar))
		    return KullanimMesaji(playerid, "/pompaduzenle [benzin] [miktar]");

		BenzinPompasi[id][pompaBenzin] = miktar;
		Pompa_Yenile(id);
		Pompa_Kaydet(id);

		SunucuMesaji(playerid, "Pompanin benzinini degistirdiniz. (ID: %d | Benzin: %d)", id, NumaraFormati(miktar, ""));
	}
    else if(!strcmp(type, "tur", true))
	{
		new tur;
		if(sscanf(string, "d", tur)) return KullanimMesaji(playerid, "/pompaduzenle [tur] [miktar]");
		if(tur < 0 || tur > 1) return HataMesaji(playerid, "Gecersiz tur belirttiniz. (0-1)");
		BenzinPompasi[id][pompaTur] = tur;
		Pompa_Yenile(id);
		Pompa_Kaydet(id);

		SunucuMesaji(playerid, "Pompanin turunu degistirdiniz. (ID: %d | Tur: %d)", id, tur);
	}
	else if(!strcmp(type, "isyeri", true))
	{
		new isyeri;
		if(sscanf(string, "d", isyeri)) return KullanimMesaji(playerid, "/pompaduzenle [isyeri] [isyeri id]");

		if((id < 0 || id >= MAKSIMUM_ISYERI) || !IsyeriBilgi[isyeri][isyeriKullanimda]) return HataMesaji(playerid, "Gecersiz isyeri.");

		BenzinPompasi[id][pompaIsyeri] = IsyeriBilgi[isyeri][isyeriID];
		Pompa_Yenile(id);
		Pompa_Kaydet(id);

		SunucuMesaji(playerid, "Pompanin isyerini degistirdiniz. (ID: %d | isyeri: %d)", id, isyeri);
	}
    else if(!strcmp(type, "yer", true))
	{
	    EditlemeSifirla(playerid);
		EditDynamicObject(playerid, BenzinPompasi[id][pompaObje]);
		OyuncuBilgi[playerid][oDuzenlenenPompa] = id;
	}
	else if(!strcmp(type, "x", true))
	{
		new Float:x;
		if(sscanf(string, "f", x))
		    return KullanimMesaji(playerid, "/pompaduzenle [x] [x]");

        BenzinPompasi[id][pompaPos][0] = x;
		Pompa_Yenile(id);
		Pompa_Kaydet(id);
	}
	else if(!strcmp(type, "y", true))
	{
		new Float:y;
		if(sscanf(string, "f", y))
		    return KullanimMesaji(playerid, "/pompaduzenle [y] [y]");

		BenzinPompasi[id][pompaPos][1] = y;
		Pompa_Yenile(id);
		Pompa_Kaydet(id);
	}
	else if(!strcmp(type, "z", true))
	{
		new Float:z;
		if(sscanf(string, "f", z))
		    return KullanimMesaji(playerid, "/pompaduzenle [z] [z]");

		BenzinPompasi[id][pompaPos][2] = z;
		Pompa_Yenile(id);
		Pompa_Kaydet(id);
	}
	else if(!strcmp(type, "a", true))
	{
		new Float:a;
		if(sscanf(string, "f", a))
		    return KullanimMesaji(playerid, "/pompaduzenle [a] [a]");

		BenzinPompasi[id][pompaPos][3] = a;
		Pompa_Yenile(id);
		Pompa_Kaydet(id);
	}
	else if(!strcmp(type, "ucret", true))
	{
		new ucret;
		if(sscanf(string, "d", ucret)) return KullanimMesaji(playerid, "/pompaduzenle [ucret] [litre basi ucret]");

		BenzinPompasi[id][pompaUcret] = ucret;
		Pompa_Yenile(id);
		Pompa_Kaydet(id);
		SunucuMesaji(playerid, "Pompanin ucretini degistirdiniz. (ID: %d | ucret: %d)", id, NumaraFormati(ucret));
	}
	else if(!strcmp(type, "sil", true))
	{
 		new log_icerik[144];
		format(log_icerik, sizeof(log_icerik), "[%s] %s(%d), pompa sildi. (SID: %d)", Tarih(), OyuncuBilgi[playerid][oAdminNick], OyuncuBilgi[playerid][oID], BenzinPompasi[id][pompaID]);
		//Gunluk_LogKaydetKaydet(log_icerik);
		Log_Kaydet("loglar/pompa_admin.txt", "%s", log_icerik);
		Pompa_Kaldir(id);
		SunucuMesaji(playerid, "Benzin pompasi %d basariyla kaldirildi.", id);
	}
	return 1;
}

CMD:aisyeri(playerid, params[])
{
	if(OyuncuBilgi[playerid][oAdmin] < 5) return YetkinizYok(playerid);
	if(!strcmp(params, "olustur", true)) {
	    new id = -1;
		id = Isyeri_Yarat(playerid, 1);

		if(id == -1) return HataMesaji(playerid, "Sunucu isyeri limitine ulastigi icin yeni isyeri yaratilamiyor!");

		SunucuMesaji(playerid, "Sunucuya yeni bir isyeri ekledin. (ID: %d)", id);
		new log_icerik[144];
		format(log_icerik, sizeof(log_icerik), "[%s] %s(%d), bir isyeri olusturdu. (SID: %d)", Tarih(), OyuncuBilgi[playerid][oAdminNick], OyuncuBilgi[playerid][oID], IsyeriBilgi[id][isyeriID]);
		//Gunluk_LogKaydetKaydet(log_icerik);
		Log_Kaydet("loglar/isyeri_admin.txt", "%s", log_icerik);
		return 1;
	}
	new id, type[24], string[128];
	if(sscanf(params, "ds[24]S()[128]", id, type, string))
 	{
	 	KullanimMesaji(playerid, "/aisyeri [id] [isim]");
	    SendClientMessage(playerid, RENK_SARI, "[iSiMLER]:{FFFFFF} olustur, lokasyon, interior, isim, fiyat, durum, tur, urun, sil");
	    if(OyuncuBilgi[playerid][oAdmin] >= 6) SendClientMessage(playerid, RENK_SARI, "[iSiMLER]:{FFFFFF} sahip");
		return 1;
	}
	if((id < 0 || id >= MAKSIMUM_ISYERI) || !IsyeriBilgi[id][isyeriKullanimda])
	    return HataMesaji(playerid, "Gecersiz isyeri!");

	if(!strcmp(type, "lokasyon", true))
	{
	    GetPlayerPos(playerid, IsyeriBilgi[id][isyeriPos][0], IsyeriBilgi[id][isyeriPos][1], IsyeriBilgi[id][isyeriPos][2]);
		GetPlayerFacingAngle(playerid, IsyeriBilgi[id][isyeriPos][3]);

		IsyeriBilgi[id][isyeriDisInt] = GetPlayerInterior(playerid);
		IsyeriBilgi[id][isyeriDisVW] = GetPlayerVirtualWorld(playerid);

		Isyeri_Yenile(id);
		Isyeri_Kaydet(id);

		YoneticiMesaji(RENK_ACIKKIRMIZI, "YETKILI: %s bir isyerinin lokasyonunu degistirdi. (ID: %d)", OyuncuBilgi[playerid][oAdminNick], id);
		new log_icerik[144];
		format(log_icerik, sizeof(log_icerik), "[%s] %s(%d), bir isyeri lokasyonunu degistirdi. (SID: %d)", Tarih(), OyuncuBilgi[playerid][oAdminNick], OyuncuBilgi[playerid][oID], IsyeriBilgi[id][isyeriID]);
		//Gunluk_LogKaydetKaydet(log_icerik);
		Log_Kaydet("loglar/isyeri_admin.txt", "%s", log_icerik);
	}
	else if(!strcmp(type, "interior", true))
	{
	    GetPlayerPos(playerid, IsyeriBilgi[id][isyeriIcPos][0], IsyeriBilgi[id][isyeriIcPos][1], IsyeriBilgi[id][isyeriIcPos][2]);
		GetPlayerFacingAngle(playerid, IsyeriBilgi[id][isyeriIcPos][3]);

		IsyeriBilgi[id][isyeriInterior] = GetPlayerInterior(playerid);
		SetPlayerVirtualWorld(playerid, IsyeriBilgi[id][isyeriVW]);

		Isyeri_Kaydet(id);
		YoneticiMesaji(RENK_ACIKKIRMIZI, "YETKILI: %s bir isyerinin interiorunu degistirdi. (ID: %d)", OyuncuBilgi[playerid][oAdminNick], id);
		new log_icerik[144];
		format(log_icerik, sizeof(log_icerik), "[%s] %s(%d), bir isyeri interiorunu degistirdi. (SID: %d)", Tarih(), OyuncuBilgi[playerid][oAdminNick], OyuncuBilgi[playerid][oID], IsyeriBilgi[id][isyeriID]);
		//Gunluk_LogKaydetKaydet(log_icerik);
		Log_Kaydet("loglar/isyeri_admin.txt", "%s", log_icerik);
	}
	else if(!strcmp(type, "isim", true))
	{
	    static yeniisim[128];

	    if(sscanf(string, "s[128]", yeniisim))
	        return KullanimMesaji(playerid, "/aisyeri [id] [isim] [yeni isim]");

	    format(IsyeriBilgi[id][isyeriAd], 128, yeniisim);

	    Isyeri_Yenile(id);
	    Isyeri_Kaydet(id);

		YoneticiMesaji(RENK_ACIKKIRMIZI, "YETKILI: %s bir isyerinin ismini degistirdi. (ID: %d | isim: %s)", OyuncuBilgi[playerid][oAdminNick], id, yeniisim);
		new log_icerik[144];
		format(log_icerik, sizeof(log_icerik), "[%s] %s(%d), isyeri ismini %s olarak degistirdi. (SID: %d)", Tarih(), OyuncuBilgi[playerid][oAdminNick], OyuncuBilgi[playerid][oID], yeniisim, IsyeriBilgi[id][isyeriID]);
		//Gunluk_LogKaydetKaydet(log_icerik);
		Log_Kaydet("loglar/isyeri_admin.txt", "%s", log_icerik);
	}
	else if(!strcmp(type, "sahip", true) && OyuncuBilgi[playerid][oAdmin] >= 6)
	{
        if(!strcmp(string, "devlet", true)) {
			if(IsyeriBilgi[id][isyeriSahip] != 0) return HataMesaji(playerid, "Bu isyerinin bir sahibi oldugu icin devlete satilamiyor.");

			IsyeriBilgi[id][isyeriSahipIsim][0] = 0;
			IsyeriBilgi[id][isyeriSahip] = -1;
			IsyeriBilgi[id][isyeriOrtak] = 0;
			IsyeriBilgi[id][isyeriDurum] = 1;
			Isyeri_Kaydet(id);
			Isyeri_Yenile(id);
			SunucuMesaji(playerid, "isyeri %d basariyla devlete satildi!", id);
			new log_icerik[144];
			format(log_icerik, sizeof(log_icerik), "[%s] %s(%d), isyerini devlete satti. (SID: %d)", Tarih(), OyuncuBilgi[playerid][oAdminNick], OyuncuBilgi[playerid][oID], IsyeriBilgi[id][isyeriID]);
			//Gunluk_LogKaydetKaydet(log_icerik);
			Log_Kaydet("loglar/isyeri_admin.txt", "%s", log_icerik);
			return 1;
        }
	    new oyuncuid;
	    if(sscanf(string, "d", oyuncuid)) return KullanimMesaji(playerid, "/aisyeri [id] [sahip] [oyuncu ID] - (-1 kaldirir)");

		if(oyuncuid == -1) {
		    IsyeriBilgi[id][isyeriSahip] = 0;
		    Isyeri_Kaydet(id);
		    Isyeri_Yenile(id);
		    YoneticiMesaji(RENK_ACIKKIRMIZI, "YETKILI: %s bir isyeri sahibini sildi. (ID: %d)", OyuncuBilgi[playerid][oAdminNick], id);
			new log_icerik[144];
			format(log_icerik, sizeof(log_icerik), "[%s] %s(%d), isyeri sahibini sildi. (SID: %d)", Tarih(), OyuncuBilgi[playerid][oAdminNick], OyuncuBilgi[playerid][oID], IsyeriBilgi[id][isyeriID]);
			//Gunluk_LogKaydetKaydet(log_icerik);
			Log_Kaydet("loglar/isyeri_admin.txt", "%s", log_icerik);
		    return 1;
		}
		else {
			if(!IsPlayerConnected(oyuncuid)) return HataMesaji(playerid, "Girilen kisi oyunda degil.");

			IsyeriBilgi[id][isyeriSahip] = OyuncuBilgi[oyuncuid][oID];
			Isyeri_Kaydet(id);
			IsyeriBilgi[id][isyeriSahipIsim][0] = 0;
			Isyeri_Yenile(id);
			YoneticiMesaji(RENK_ACIKKIRMIZI, "YETKILI: %s bir isyerinin sahibini degisitirdi. (ID: %d | Sahip: %s)", OyuncuBilgi[playerid][oAdminNick], id, OyuncuIsim(oyuncuid));
