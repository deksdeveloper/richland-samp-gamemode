{
	if(OyuncuBilgi[playerid][oGiris] == false) return HataMesaji(playerid, "Giris yapmadan bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oHapisSure][0] > 0) return HataMesaji(playerid, "Hapisteyken bu komutu kullanamazsin.");
	if(isnull(params)) return KullanimMesaji(playerid, "/do [durum]");

