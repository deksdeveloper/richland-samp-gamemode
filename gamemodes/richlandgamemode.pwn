Dialog:DIALOG_BIRLIK_MAKSRUTBE(playerid, response, listitem, inputtext[])
{
	if(!response) return cmd_birlik(playerid, inputtext);
	
		new rutbe, sayi, birlikid = OyuncuBilgi[playerid][oBirlik];
		if(sscanf(inputtext, "d", rutbe)) Dialog_Show(playerid, DIALOG_BIRLIK_MAKSRUTBE, DIALOG_STYLE_INPUT, "> Birlik - Maksimum Rutbe", "Birliginin maksimum rutbesini kac yapmak istiyorsun?", "Ayarla", "iptal");

		if(rutbe < 6 || rutbe > 15) return Dialog_Show(playerid, DIALOG_BIRLIK_MAKSRUTBE, DIALOG_STYLE_INPUT, "> Birlik - Maksimum Rutbe", "{B70000}HATA: {FFFFFF}Maksimum rutbe gecersiz.\nBirliginin maksimum rutbesini kac yapmak istiyorsun?", "Ayarla", "iptal");
        BirlikBilgi[birlikid][birlikMaksRutbe] = rutbe;
        Birlik_Kaydet(birlikid);
        foreach(new i : Player) if(OyuncuBilgi[i][oBirlik] == birlikid && i != playerid) OyuncuBilgi[i][oBirlikRutbe] = 1;
		new sorgu[512];
		format(sorgu, sizeof(sorgu), "SELECT * FROM oyuncular WHERE birlik = '%d'", OyuncuBilgi[playerid][oBirlikSQLID]);
		mysql_tquery(mysqlB, sorgu, "RutbeDuzenlemeBirlik", "d", playerid);
	return 1;
}

Dialog:DIALOG_BIRLIK_ARACEKLE(playerid, response, listitem, inputtext[])
{
	if(!response) return cmd_birlik(playerid, inputtext);
		new aracid = Arac_IDBul(GetPlayerVehicleID(playerid)), adet = 0;
		if(!Arac_SahipKontrol(playerid, aracid)) return HataMesaji(playerid, "Bu arac senin olmadigi icin bu secenegi kullanamazsin."), cmd_birlik(playerid, inputtext);
		for(new i = 0; i != MAKSIMUM_ARAC; i++) if(OyuncuBilgi[playerid][oBirlikSQLID] == AracBilgi[i][aracBirlik]) adet++;

		switch(BirlikBilgi[OyuncuBilgi[playerid][oBirlik]][birlikLevel]) {
		    case 3: if(adet >= 5) return HataMesaji(playerid, "Birliginize zaten maksimum sayida arac eklemissiniz.");
		    case 4: if(adet >= 7) return HataMesaji(playerid, "Birliginize zaten maksimum sayida arac eklemissiniz.");
		    case 5: if(adet >= 10) return HataMesaji(playerid, "Birliginize zaten maksimum sayida arac eklemissiniz.");
		    case 6: if(adet >= 15) return HataMesaji(playerid, "Birliginize zaten maksimum sayida arac eklemissiniz.");
			default: if(adet >= 3) return HataMesaji(playerid, "Birliginize zaten maksimum sayida arac eklemissiniz.");
		}
		if(AracBilgi[aracid][aracBirlik] > 0) return HataMesaji(playerid, "Bu arac zaten bir birlige bagli.");
		if(BirlikBilgi[OyuncuBilgi[playerid][oBirlik]][birlikMaksRutbe] != OyuncuBilgi[playerid][oBirlikRutbe] && OyuncuBilgi[playerid][oBirlikLider] != BirlikBilgi[OyuncuBilgi[playerid][oBirlik]][birlikID]) return HataMesaji(playerid, "Bu secenegi kullanmak icin birlik lideri olman gerekiyor.");

		AracBilgi[aracid][aracSahip] = 0;
		AracBilgi[aracid][aracBirlik] = BirlikBilgi[OyuncuBilgi[playerid][oBirlik]][birlikID];
		Arac_Kaydet(aracid);
		SunucuMesaji(playerid, "%s model aracini birligine bagladin.", AracModel(AracBilgi[aracid][aracModel]));
		new log_icerik[144], oyuncu_isim[MAX_PLAYER_NAME + 1];
		GetPlayerName(playerid, oyuncu_isim, sizeof(oyuncu_isim));
		format(log_icerik, sizeof(log_icerik), "[%s] %s(%d) aracini birlige bagladi. (SID: %d / birlik: %s)", Tarih(), oyuncu_isim, OyuncuBilgi[playerid][oID], AracBilgi[aracid][aracID], BirlikBilgi[OyuncuBilgi[playerid][oBirlik]][birlikAd]);
		//Gunluk_LogKaydetKaydet(log_icerik);
		Log_Kaydet("loglar/birlik.txt", "%s", log_icerik);
	return 1;
}

Dialog:DIALOG_BIRLIK_DUYURU(playerid, response, listitem, inputtext[])
{
	if(!response) return cmd_birlik(playerid, inputtext);

	if(!strcmp(inputtext, "Yok", true))
	{
		BirlikBilgi[OyuncuBilgi[playerid][oBirlik]][birlikDuyuru][0] = 0;
		Birlik_Kaydet(OyuncuBilgi[playerid][oBirlik]);
		return SunucuMesaji(playerid, "Duyuru kaldirildi.");
	}
	else if(strlen(inputtext) < 6 || strlen(inputtext) > 48) 
	{
		return HataMesaji(playerid, "Girilen duyuru en az 6 en fazla 48 harfli olmalidir.");
	}
	else
	{
		new birlikid;
		birlikid = OyuncuBilgi[playerid][oBirlik];
		format(BirlikBilgi[birlikid][birlikDuyuru], 48, "%s", inputtext);
		Birlik_Kaydet(birlikid);
		SunucuMesaji(playerid, "Birlik duyurusu guncellendi.");
		foreach(new o : Player) 
		{
			if(OyuncuBilgi[o][oBirlik] == birlikid) 
			{
				BirlikDuyuruMesaji(o, "%s", inputtext);
			}
			return true;
		}
		return true;
	}
}



