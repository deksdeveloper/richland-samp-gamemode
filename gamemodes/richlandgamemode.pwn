CMD:kartsifre(playerid, params[])
{
    if(OyuncuBilgi[playerid][oAdminDurum] == true) return HataMesaji(playerid, "Admin isbasindayken bu komutu kullanamazsin.");
	new id;
	if(!IsPlayerInRangeOfPoint(playerid, 2.0, 3.6589,-5.2733,1801.1100) && !IsPlayerInRangeOfPoint(playerid, 2.0, 1.8187,-5.3928,1801.1100) && !IsPlayerInRangeOfPoint(playerid, 2.0, -1.7066,-5.6624,1801.1100) && !IsPlayerInRangeOfPoint(playerid, 2.0, -3.6136,-5.0132,1801.1100)) return HataMesaji(playerid, "Bankada degilsin.");
	if(OyuncuBilgi[playerid][oHesapNumarasi] == 0) return HataMesaji(playerid, "Banka hesabin yok.");
	if(OyuncuBilgi[playerid][oBankaSifre] == 0) return HataMesaji(playerid, "sifre olusturmadigin icin sifirlama islemi yapamamaktayiz.");
	if(OyuncuBilgi[playerid][oTelefon] == 0 || OyuncuBilgi[playerid][oTelefonNumarasi] == 0) return HataMesaji(playerid, "Telefonun yok veya operatore bagli degil.");
	if(OyuncuBilgi[playerid][oTelefonDurum] == false) return HataMesaji(playerid, "Telefonun kapaliyken bu sisteme ulasamazsin.");

	new baslik[128];
	format(baslik, sizeof(baslik), "{FFFFFF}%s", BinaBilgi[id][bIsim]);
	OyuncuBilgi[playerid][oDogrulamaKodu] = randomEx(1000, 9999);
	SendClientMessageEx(playerid, RENK_SARI, "SMS: Kart sifresi sifirlama icin dogrulama kodunuz: %d | Gonderen: RichBank", OyuncuBilgi[playerid][oDogrulamaKodu]);
	switch(OyuncuBilgi[playerid][oTelefonSes]) {
		case true: cmd_do(playerid, "Telefonundan mesaj bildirim sesi geldi.");
		case false: SunucuMesaji(playerid, "Telefonunuz titredi.");
	}
    Dialog_Show(playerid, DBANKA_SSIFIRLA, DIALOG_STYLE_INPUT, baslik, "{FFFFFF}Bankaya tekrar hosgeldin, goruyoruz ki sifre sifirlama icin gelmissin.\n\nBilgilerini dogrulayabilmemiz icin lutfen telefonuna gelen sifreyi gir:", "Tamam", "Kapat");
	return 1;
}

