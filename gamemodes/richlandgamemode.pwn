stock SembolKontrol(icerik[])
{
	for(new i = 0, len = strlen(icerik); i < len; i++) {
	    if(icerik[i] == '!' || icerik[i] == '?' || icerik[i] == '@' || icerik[i] == '#' || icerik[i] == '&' || icerik[i] == '%' || icerik[i] == '$') return true;
