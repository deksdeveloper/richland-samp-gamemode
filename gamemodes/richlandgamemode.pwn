CMD:gozbagi(playerid, params[])
{
	if(OyuncuBilgi[playerid][oHapisSure][0] > 0) return HataMesaji(playerid, "Hapisteyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oYarali] > 0) return HataMesaji(playerid, "Yaraliyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oGozBagli] == true) return HataMesaji(playerid, "Gozunuz bagliyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oDonduruldu] > 0) return HataMesaji(playerid, "Etkisiz haldeyken bu komutu kullanamazsin.");

	new oyuncuid;
	if(sscanf(params, "u", oyuncuid)) return KullanimMesaji(playerid, "/gozbagi [oyuncu id/isim]");
	if(!IsPlayerConnected(oyuncuid)) return HataMesaji(playerid, "Girilen kisi oyunda degil.");
	if(!YakinlikKontrol(playerid, oyuncuid, 3.0)) return HataMesaji(playerid, "Oyuncuya yakin degilsin.");
	if(oyuncuid == playerid) return HataMesaji(playerid, "Bu komutu kendi uzerinde kullanamazsin.");

	if(OyuncuBilgi[oyuncuid][oGozBagli] == false) {
		if(OyuncuBilgi[playerid][oDialog] > -1) return HataMesaji(playerid, "Girilen oyuncuya su anda goz baglama onerisi verilemez, diyalogu kapatmasini bekleyin.");

		new mesaj[128 + MAX_PLAYER_NAME];
		format(mesaj, sizeof(mesaj), "{FFFFFF}%s adli oyuncu gozunuzu baglamak istiyor.", OyuncuIsim(playerid));
		DialogGoster(oyuncuid, DIALOG_GOZBAGLA, DIALOG_STYLE_MSGBOX, "> Goz Baglama - Talep", mesaj, "Kabul", "Reddet");
		SetPVarInt(oyuncuid, "gozbaglayanid", playerid);
	}
	else if(OyuncuBilgi[oyuncuid][oGozBagli] == true) {
		OyuncuBilgi[oyuncuid][oGozBagli] = false;
		PlayerTextDrawHide(oyuncuid, siyahekran[oyuncuid]);
		new emote[128 + MAX_PLAYER_NAME];
		format(emote, sizeof(emote), "%s adli kisinin goz bagini acar.", OyuncuIsim(oyuncuid));
		cmd_me(playerid, emote);
	}
	return 1;
}

CMD:boombox(playerid, params[])
{
	if(IsPlayerInAnyVehicle(playerid)) return HataMesaji(playerid, "Bu komut aracta kullanilamaz.");
	if(GetPlayerState(playerid) != PLAYER_STATE_ONFOOT || OyuncuBilgi[playerid][oGiris] == false) return HataMesaji(playerid, "su anda bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oDonduruldu] > 0) return HataMesaji(playerid, "su anda bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oHapisSure][0] > 0 || OyuncuBilgi[playerid][oHapisSure][1] > 0) return HataMesaji(playerid, "Hapiste bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oYarali] > 0) return HataMesaji(playerid, "Yaraliyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oAdminDurum]) return HataMesaji(playerid, "Yonetici isbasindayken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oMuzikKutusu] == 0) return HataMesaji(playerid, "Muzik kutun yok.");
	if(Oyuncu_Isyerinde(playerid) != -1 && IsyeriBilgi[Oyuncu_Isyerinde(playerid)][isyeriTur] == 8) return HataMesaji(playerid, "Bar tipi isyeri icinde boombox koyamazsin.");

	new menu[256];
	menu[0] = 0;
	if(!BoomboxBilgi[playerid][boomboxKullanimda]) strcat(menu, "Yerlestir\n");
	else if(BoomboxBilgi[playerid][boomboxKullanimda]) strcat(menu, "Kaldir\nURL Degistir");
	DialogGoster(playerid, DIALOG_BOOMBOX, DIALOG_STYLE_TABLIST, "Boombox", menu, "Sec", "Kapat");
	return 1;
}

CMD:bbkaldir(playerid, params[])
{
    if(OyuncuBilgi[playerid][oAdmin] < 3) return YetkinizYok(playerid);
    if(OyuncuBilgi[playerid][oAdminDurum] == false && OyuncuBilgi[playerid][oAdmin] < 6) return IsbasiYapmalisin(playerid);

	foreach (new i : Player) if(BoomboxBilgi[i][boomboxKullanimda] && IsPlayerInRangeOfPoint(playerid, 3.0, BoomboxBilgi[i][boomboxPos][0], BoomboxBilgi[i][boomboxPos][1], BoomboxBilgi[i][boomboxPos][2])) {
		Boombox_Kaldir(i);

		SunucuMesaji(playerid, "%s adli oyuncunun muzik kutusunu kaldirdin.", OyuncuIsim(i));
		return SunucuMesaji(i, "%s adli yetkili muzik kutunu kaldirdi.", OyuncuBilgi[playerid][oAdminNick]);
	}
	HataMesaji(playerid, "Yakininda muzik kutusu yok.");
	return 1;
}

CMD:kampatesi(playerid, params[])
{
	if(OyuncuBilgi[playerid][oHapisSure][0] > 0) return HataMesaji(playerid, "Hapisteyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oYarali] > 0) return HataMesaji(playerid, "Yaraliyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oVIP] < 2) return HataMesaji(playerid, "Bu komutu Safir VIP ve uzeri kullanabilir.");

	new tip[24], Float:objex, Float:objey, Float:objez;
	if(sscanf(params, "s[24]", tip))
 	{
	 	KullanimMesaji(playerid, "/kampatesi [isim]");
	    SendClientMessage(playerid, RENK_SARI, "[ISIMLER]:{FFFFFF} kur, kaldir");
		return 1;
	}
    if(!strcmp(tip, "kur", true))
	{
	    if(OyuncuBilgi[playerid][oKampAtesi] == true)
	        return HataMesaji(playerid, "Zaten kamp atesi kurmussunuz.");

		if(OyuncuBilgi[playerid][oOdun] < 1 || OyuncuBilgi[playerid][oCakmak] == false && OyuncuBilgi[playerid][oKibrit] < 1)
		    return HataMesaji(playerid, "Odununuz ya da odunu yakabilecegin bir materyalin yok.");

		if(GetPlayerInterior(playerid) > 0 || GetPlayerVirtualWorld(playerid) > 0)
		    return HataMesaji(playerid, "Interior ve farkli sanal dunyalara kamp atesi kurulamaz.");

		GetPlayerPos(playerid, OyuncuBilgi[playerid][oPos][0], OyuncuBilgi[playerid][oPos][1], OyuncuBilgi[playerid][oPos][2]);
		GetPlayerFacingAngle(playerid, OyuncuBilgi[playerid][oPos][3]);
		OyuncuBilgi[playerid][oKampAtesiObje][0] = CreateDynamicObject(841, OyuncuBilgi[playerid][oPos][0], OyuncuBilgi[playerid][oPos][1], OyuncuBilgi[playerid][oPos][2]-1, 0.000, 0.000, OyuncuBilgi[playerid][oPos][3]);
		OyuncuBilgi[playerid][oKampAtesiObje][1] = CreateDynamicObject(18688, OyuncuBilgi[playerid][oPos][0], OyuncuBilgi[playerid][oPos][1], OyuncuBilgi[playerid][oPos][2]-2.3, 0.000, 0.000, OyuncuBilgi[playerid][oPos][3]);
		OyuncuBilgi[playerid][oKampAtesi] = true;
		OyuncuBilgi[playerid][oOdun]--;
		if(OyuncuBilgi[playerid][oKibrit] > 0) OyuncuBilgi[playerid][oKibrit]--;
		SunucuMesaji(playerid, "Kamp atesi yerlestirdin.");
		return 1;
	}
	else if(!strcmp(tip, "kaldir", true))
	{
	    if(OyuncuBilgi[playerid][oKampAtesi] == false)
	        return HataMesaji(playerid, "Kamp atesi yerlestirmemissin.");

        GetDynamicObjectPos(OyuncuBilgi[playerid][oKampAtesiObje][0], objex, objey, objez);
		if(!IsPlayerInRangeOfPoint(playerid, 3.0, objex, objey, objez))
			return HataMesaji(playerid, "Kamp atesine yakin degilsin.");

        DestroyDynamicObject(OyuncuBilgi[playerid][oKampAtesiObje][0]);
        DestroyDynamicObject(OyuncuBilgi[playerid][oKampAtesiObje][1]);
        OyuncuBilgi[playerid][oKampAtesi] = false;
        SunucuMesaji(playerid, "Kamp atesi basariyla kaldirildi.");
		return 1;
	}
	return 1;
}

CMD:sigarayak(playerid,params[])
{
	if(OyuncuBilgi[playerid][oHapisSure][0] > 0) return HataMesaji(playerid, "Hapisteyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oYarali] > 0) return HataMesaji(playerid, "Yaraliyken bu komutu kullanamazsin.");
    if(OyuncuBilgi[playerid][oSigara] < 1) return HataMesaji(playerid, "Sigaran bulunmuyor.");
	if(OyuncuBilgi[playerid][oCakmak] == false && OyuncuBilgi[playerid][oKibrit] < 1) return HataMesaji(playerid, "cakmagin veya kibritin bulunmuyor.");

    new oyuncuid, mesaj[256];
	if(sscanf(params, "u", oyuncuid) || oyuncuid == playerid)
	{
		if(OyuncuBilgi[playerid][oSigaraDurum] < 1) return HataMesaji(playerid, "Sigaran agzinda degil.");

		cmd_me(playerid, "cakmagi ile kendi sigarasini yakar.");
		SunucuMesaji(playerid, "Sigarayi kullanmak icin {FFFF00}~k~~PED_FIREWEAPON~{FFFFFF} tusunu kullanabilirsiniz.");
		OyuncuBilgi[playerid][oSigaraIciyor] = 1;
		OyuncuBilgi[playerid][oSigaraKalanHak] = 10;
		OyuncuBilgi[playerid][oSigara]--;
		SetPlayerSpecialAction(playerid, SPECIAL_ACTION_SMOKE_CIGGY);
		return 1;
	}

	if(!IsPlayerConnected(oyuncuid)) return HataMesaji(playerid, "Girilen kisi oyunda degil.");
	if(!YakinlikKontrol(playerid, oyuncuid, 3.0)) return HataMesaji(playerid, "Oyuncuya yakin degilsin.");
	if(OyuncuBilgi[oyuncuid][oSigaraDurum] == 0) return HataMesaji(playerid, "Kisinin agzinda sigara yok.");

	format(mesaj, sizeof(mesaj), "cakmagini %s adli kisinin sigarasina dogru yaklastirir ve sigarasini yakar", OyuncuIsim(oyuncuid));
	cmd_me(playerid, mesaj);
	SunucuMesaji(oyuncuid, "Sigarayi kullanmak icin {FFFF00}~k~~PED_FIREWEAPON~{FFFFFF} tusunu kullanabilirsiniz.");
	OyuncuBilgi[oyuncuid][oSigaraIciyor] = 1;
	OyuncuBilgi[oyuncuid][oSigaraKalanHak] = 10;
	OyuncuBilgi[oyuncuid][oSigara]--;
	SetPlayerSpecialAction(oyuncuid, SPECIAL_ACTION_SMOKE_CIGGY);
	return 1;
}

