Dialog:DIALOG_TELEFON_UBER(playerid, response, listitem, inputtext[])
{
	if(!response) return cmd_telefon(playerid, "xmZcQmmp");

	    if(!strcmp(inputtext, "isbasi", true)) {
			if(!IsPlayerInAnyVehicle(playerid)) return HataMesaji(playerid, "Bir aracta degilsin.");
			new aracid = Arac_IDBul(GetPlayerVehicleID(playerid));
			if(!Arac_SahipKontrol(playerid, aracid) || AracBilgi[aracid][uberLisans] == 0) return HataMesaji(playerid, "Aracinda uber lisansi yok.");

		switch(OyuncuBilgi[playerid][oUberIsbasi]) {
			    case true: OyuncuBilgi[playerid][oUberIsbasi] = false, SunucuMesaji(playerid, "Uber isbasindan ciktin.");
			    case false: OyuncuBilgi[playerid][oUberIsbasi] = true, SendClientMessageToAllEx(0x42f445FF, "TAXI: %s taksici isbasi yapti, '/telefon => Uber' kullanarak taksi cagirabilirsin.", OyuncuIsim(playerid));
			}

			cmd_telefon(playerid, "xmZcQmmp");
	    }
	    else if(!strcmp(inputtext, "Taksimetre", true)) {
			if(!IsPlayerInAnyVehicle(playerid)) return HataMesaji(playerid, "Bir aracta degilsin.");
			if(OyuncuBilgi[playerid][oUberIsbasi] == false) return HataMesaji(playerid, "isbasi yapmamissin. (/telefon > Uber)");
			new aracid = Arac_IDBul(GetPlayerVehicleID(playerid));
			if(!Arac_SahipKontrol(playerid, aracid) || AracBilgi[aracid][uberLisans] == 0) return HataMesaji(playerid, "Aracinda uber lisansi yok.");

			switch(AracBilgi[aracid][aTaksiMetreDurum]) {
			    case true: AracBilgi[aracid][aTaksiMetreDurum] = false, AracBilgi[aracid][aTaksiMetre] = 0.0, SunucuMesaji(playerid, "Taksimetre kapatildi.");
			    case false: AracBilgi[aracid][aTaksiMetreDurum] = true, AracBilgi[aracid][aTaksiMetre] = 5.0, SunucuMesaji(playerid, "Taksimetre acildi.");
			}
	    }
	    else if(!strcmp(inputtext, "Uber cagir", true)) {
	        if(OyuncuBilgi[playerid][oUberTalep] != INVALID_PLAYER_ID) return HataMesaji(playerid, "Daha once uber talep etmissin, once onu iptal etmelisin. (/iptal)");

	        new oyuncu_isim[MAX_PLAYER_NAME], aracid, sayi = 0, icerik[1024];
	        format(icerik, sizeof(icerik), "Surucu\tModel\tPlaka\n");
			foreach(new i : Player) if(OyuncuBilgi[i][oGiris] == true && OyuncuBilgi[i][oUberIsbasi] == true) {
			    if(!IsPlayerInAnyVehicle(i)) continue;
				aracid = Arac_IDBul(GetPlayerVehicleID(i));
				GetPlayerName(i, oyuncu_isim, sizeof(oyuncu_isim));
				for(new is = 0, len = strlen(oyuncu_isim); is < len; is++) if(oyuncu_isim[is] == '_') oyuncu_isim[is] = ' ';
			    format(icerik, sizeof(icerik), "%s%s\t%s\t%s\n", icerik, oyuncu_isim, AracModel(AracBilgi[aracid][aracModel]), AracBilgi[aracid][aracPlaka]);
			    sayi++;
			}
			if(!sayi) return HataMesaji(playerid, "Aktif durumda uber surucusu yok, uber cagiramazsiniz.");
			Dialog_Show(playerid, DIALOG_UBER_CAGIR, DIALOG_STYLE_TABLIST_HEADERS, "> Uber cagir", icerik, "Sec", "Kapat");
	    }
	return 1;
}

	
Dialog:DIALOG_UBER_CAGIR(playerid, response, listitem, inputtext[])
{
	if(!response) return cmd_telefon(playerid, "xmZcQmmp");

	    new surucu_isim[MAX_PLAYER_NAME];
		foreach(new i : Player) if(OyuncuBilgi[i][oGiris] == true && OyuncuBilgi[i][oUberIsbasi] == true) {
			GetPlayerName(i, surucu_isim, sizeof(surucu_isim));
