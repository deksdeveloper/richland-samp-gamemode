public Oyuncu_Liste(playerid)
{
    if(!IsPlayerConnected(playerid)) return 0;
    if(OyuncuBilgi[playerid][oGiris] == true) return KickEx(playerid, "Bir hata olustu.");
	new rows, fields, sayi = 0, ekslot = OyuncuBilgi[playerid][oEkSlot], bilgi[1024];
	cache_get_row_count(rows);
	cache_get_field_count(fields);
	format(bilgi, sizeof(bilgi), "Eglence dolu dunyamiza katilmak icin bir adim daha kaldi. Karakterlerinin arasindan birini sec!\t\n", KARAKTER_LIMIT + ekslot);
	for(new i = 0; i < rows; i ++) format(bilgi, sizeof(bilgi), "%s%s\t%s\n", bilgi, vericek_varchar(i, "isim"), vericek_varchar(i, "oSonGiris")), sayi++;
	if(IsPlayerAdmin(playerid)) {
	    new sorgu[256];
		format(sorgu, sizeof(sorgu), "SELECT * FROM oyuncular WHERE isim = '%s' LIMIT 1", vericek_varchar(0, "isim"));
		return mysql_tquery(mysqlB, sorgu, "OyuncuYukle", "d", playerid);
	}
	//if(sayi < KARAKTER_LIMIT + ekslot) strcat(bilgi, "Karakter Olustur\t\n");
	DialogGoster(playerid, DIALOG_KARAKTERSEC, DIALOG_STYLE_TABLIST_HEADERS, "Richland Roleplay: Karakter Secimi", bilgi, "Tamam", "");
	return 1;
}

