CMD:sgit(playerid, params[])
{
	if(OyuncuBilgi[playerid][oHelper] < 2) return YetkinizYok(playerid);
	if(OyuncuBilgi[playerid][oIncelenenSoruSahibi] != INVALID_PLAYER_ID) return HataMesaji(playerid, "Bir soruyla ilgileniyorsun, oncelikle onu bitirmelisin. (/sbitir)");

	new oyuncuid;
	if(sscanf(params, "u", oyuncuid)) return KullanimMesaji(playerid, "/sgit [oyuncu id/isim]");
	if(!IsPlayerConnected(oyuncuid)) return HataMesaji(playerid, "Gecersiz oyuncu.");
	if(isnull(OyuncuBilgi[oyuncuid][oSoru])) return HataMesaji(playerid, "Bu oyuncu soru gondermemis.");
	if(OyuncuBilgi[oyuncuid][oPaintball] != -1) return HataMesaji(playerid, "Bu oyuncu paintballda, yanina gidemezsin.");
	if(OyuncuBilgi[oyuncuid][oHapisSure][0] > 0 || OyuncuBilgi[oyuncuid][oHapisSure][1] > 0) return HataMesaji(playerid, "Bu oyuncu hapiste, yanina gidemezsin.");
	if(oyuncuid == playerid) return HataMesaji(playerid, "Kendi sorunu kabul edemezsin.");

	OyuncuBilgi[playerid][oSoruSayisi]++;
	OyuncuBilgi[playerid][oIncelenenSoruSahibi] = oyuncuid;
	OyuncuBilgi[oyuncuid][oIlgilenenYetkili] = playerid;
	OyuncuBilgi[oyuncuid][oSoru][0] = 0;
	OyuncuBilgi[oyuncuid][oSoruSure] = 0;
	SetPVarInt(playerid, "Helper_Soru", 1);
	EskiVeriler[playerid][eskiVW] = GetPlayerVirtualWorld(playerid);
	EskiVeriler[playerid][eskiInt] = GetPlayerInterior(playerid);
	GetPlayerPos(playerid, EskiVeriler[playerid][eskiPos][0], EskiVeriler[playerid][eskiPos][1], EskiVeriler[playerid][eskiPos][2]);
	GetPlayerFacingAngle(playerid, EskiVeriler[playerid][eskiPos][3]);
	GetPlayerHealth(playerid, EskiVeriler[playerid][eskiCan]);
	GetPlayerArmour(playerid, EskiVeriler[playerid][eskiZirh]);
	GetPlayerPos(oyuncuid, OyuncuBilgi[oyuncuid][oPos][0], OyuncuBilgi[oyuncuid][oPos][1], OyuncuBilgi[oyuncuid][oPos][2]);
	OyuncuPosDegistir(playerid, OyuncuBilgi[oyuncuid][oPos][0], OyuncuBilgi[oyuncuid][oPos][1], OyuncuBilgi[oyuncuid][oPos][2]);
	SetPlayerInterior(playerid, GetPlayerInterior(oyuncuid));
	SetPlayerVirtualWorld(playerid, GetPlayerVirtualWorld(oyuncuid));
	SunucuMesaji(playerid, "%s adli kisinin sorusunu kabul ettin ve yanina isinlandin, /sbitir ile eski konumuna donebilirsin.", OyuncuIsim(oyuncuid));
	SunucuMesaji(oyuncuid, "%s sorunu kabul etti ve yanina isinlandi, sana yardimci olacak.", OyuncuBilgi[playerid][oAdminNick]);
	SunucuMesaji(oyuncuid, "iletisime gecmek icin /scevap, soruyu bitirmek icin /sorubitir komutlarini kullanabilirsin.");

	foreach(new i : Player) if(OyuncuBilgi[i][oGiris] == true && (OyuncuBilgi[i][oAdmin] > 0 || OyuncuBilgi[i][oHelper] > 0) && OyuncuBilgi[i][oSoruGoruntule] == true) {
		if(OyuncuBilgi[playerid][oAdmin] > 0) SendClientMessageEx(i, RENK_SORU, "SORU: {ffffff}%s adli yetkili %s adli kisinin sorusunu kabul etti.", OyuncuBilgi[playerid][oAdminNick], OyuncuIsim(oyuncuid));
		else if(OyuncuBilgi[playerid][oHelper] > 0) SendClientMessageEx(i, RENK_SORU, "SORU: {ffffff}%s adli helper %s adli kisinin sorusunu kabul etti.", OyuncuBilgi[playerid][oAdminNick], OyuncuIsim(oyuncuid));
	}
	new log_icerik[144], oyuncu_isim[MAX_PLAYER_NAME + 1], yetkili_isim[MAX_PLAYER_NAME + 1];
