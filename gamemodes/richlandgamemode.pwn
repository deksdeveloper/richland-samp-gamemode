CMD:nsahipbul(playerid, params[])
{
	if(OyuncuBilgi[playerid][oAdmin] < 3) return YetkinizYok(playerid);
    if(OyuncuBilgi[playerid][oAdminDurum] == false && OyuncuBilgi[playerid][oAdmin] < 6) return IsbasiYapmalisin(playerid);

	new numara;
	if(sscanf(params, "d", numara)) return KullanimMesaji(playerid, "/nsahipbul [telefon numarasi]");

	if(numara == 0) return HataMesaji(playerid, "Gecersiz numara belirttin.");
	if(NumaraSahibiBul(numara) == INVALID_PLAYER_ID) return HataMesaji(playerid, "Girilen numara gecersiz ya da sahibi oyunda degil.");

	SunucuMesaji(playerid, "%d numarasinin sahibi: %s", numara, OyuncuIsim(NumaraSahibiBul(numara)));
	return 1;
}

CMD:hasarlar(playerid, params[])
{
	new oyuncuid;
	if(sscanf(params, "u", oyuncuid)) return KullanimMesaji(playerid, "/hasarlar [oyuncuid/isim]");

	if(!IsPlayerConnected(oyuncuid)) return HataMesaji(playerid, "Girilen kisi oyunda degil.");
	if(!YakinlikKontrol(playerid, oyuncuid, 5.0)) return HataMesaji(playerid, "Girilen oyuncuya yakin degilsin.");
    if(OyuncuBilgi[oyuncuid][oYarali] == 0) return HataMesaji(playerid, "Oyuncu yarali degil.");
    new hasarsayi = 0, mesaj[MAKSIMUM_HASAR * 256];
    hasarsayi = 0;
	mesaj[0] = 0;
	for(new i = 0; i < MAKSIMUM_HASAR; i ++)
	{
		if(HasarBilgi[oyuncuid][i][hasarVarmi] && IsPlayerConnected(HasarBilgi[oyuncuid][i][hasarVeren])) {
			format(mesaj, sizeof(mesaj), "%s%.1f\t%s\t%s\t%s\n", mesaj, HasarBilgi[oyuncuid][i][hasarDeger], HasarBolge(HasarBilgi[oyuncuid][i][hasarBolge]), OyuncuIsim(HasarBilgi[oyuncuid][i][hasarVeren]), SilahIsim(HasarBilgi[oyuncuid][i][hasarSilah]));
			hasarsayi++;
		}
	}
	if(hasarsayi == 0) return HataMesaji(playerid, "Oyuncu hasar almamis.");
	strins(mesaj, "Hasar\tBolge\tHasar Veren\tSilah\n", 0);
	DialogGoster(playerid, 0, DIALOG_STYLE_TABLIST_HEADERS, OyuncuIsim(oyuncuid), mesaj, "Kapat", "");
	return 1;
}

