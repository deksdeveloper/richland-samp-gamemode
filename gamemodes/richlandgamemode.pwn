	new bilgi[128 + MAX_PLAYER_NAME];
	format(bilgi, sizeof(bilgi), "%s model silahi %s adli kisiye verir.", SilahIsim(silahid), OyuncuIsim(oyuncuid));
	cmd_me(playerid, bilgi);
	new log_icerik[144], oyuncu_isim[MAX_PLAYER_NAME + 1], hedef_isim[MAX_PLAYER_NAME + 1];
	GetPlayerName(playerid, oyuncu_isim, sizeof(oyuncu_isim));
	GetPlayerName(oyuncuid, hedef_isim, sizeof(hedef_isim));
	new string[120];
	AdminLogDCM(string, "1023206623294017646", "Silah-Birak");
	format(log_icerik, sizeof(log_icerik), "[%s] %s(%d), %s(%d) adli kisiye %s model silah verdi.", Tarih(), oyuncu_isim, OyuncuBilgi[playerid][oID], hedef_isim, OyuncuBilgi[oyuncuid][oID], SilahIsim(silahid));
	//Gunluk_LogKaydetKaydet(log_icerik);
	return 1;
}

