CMD:izle(playerid, params[])
{
	if(OyuncuBilgi[playerid][oAdmin] < 1) return YetkinizYok(playerid);
	if(OyuncuBilgi[playerid][oAdminDurum] == false && OyuncuBilgi[playerid][oAdmin] < 6) return IsbasiYapmalisin(playerid);
	if(OyuncuBilgi[playerid][oHapisSure][0] > 0 || OyuncuBilgi[playerid][oHapisSure][1] > 0) return HataMesaji(playerid, "Hapisteyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oYarali] > 0) return HataMesaji(playerid, "Yaraliyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oDonduruldu] > 0) return HataMesaji(playerid, "Bu komut su anda kullanilamaz.");
	if(!strcmp(params, "bitir", true) || !strcmp(params, "off", true)) {
		if(OyuncuBilgi[playerid][oIzlenen] == INVALID_PLAYER_ID) return HataMesaji(playerid, "Kimseyi izlemiyorsunuz!");

		OyuncuBilgi[playerid][oIzlenen] = INVALID_PLAYER_ID;
	    PlayerSpectatePlayer(playerid, INVALID_PLAYER_ID);
	    PlayerSpectateVehicle(playerid, INVALID_VEHICLE_ID);
	    OyuncuBilgi[playerid][oPos][0] = eskipos[playerid][0];
	    OyuncuBilgi[playerid][oPos][1] = eskipos[playerid][1];
	    OyuncuBilgi[playerid][oPos][2] = eskipos[playerid][2];
		SetSpawnInfo(playerid, 0, OyuncuBilgi[playerid][oKiyafet], eskipos[playerid][0], eskipos[playerid][1], eskipos[playerid][2], eskipos[playerid][3], 0, 0, 0, 0, 0, 0);
		TogglePlayerSpectating(playerid, false);
		DondurulmaEtkisi(playerid);
	    SilahAyarla(playerid);
	    SetPlayerInterior(playerid, eskiint[playerid]);
	    SetPlayerVirtualWorld(playerid, eskiworld[playerid]);
	    eskiworld[playerid] = -1;
	    SetTimerEx("Kiyafet_Duzelt", 100, false, "i", playerid);
	    return 1;
	}
	new oyuncuid;
	if(sscanf(params, "u", oyuncuid)) return KullanimMesaji(playerid, "/izle [oyuncu id/isim] / [off veya bitir]");

	if(!IsPlayerConnected(oyuncuid)) return HataMesaji(playerid, "Girilen kisi oyunda degil.");
	if(oyuncuid == playerid) return HataMesaji(playerid, "Kendini izleyemezsin.");
	if(!OyuncuBilgi[oyuncuid][oGiris]) return HataMesaji(playerid, "Oyuncu giris yapmamis, izlenebilir durumda degil.");
	if(OyuncuBilgi[oyuncuid][oSpecDurum] == false && OyuncuBilgi[playerid][oAdmin] < 7) return HataMesaji(playerid, "Girilen oyuncu izlenebilir durumda degil.");
    if(OyuncuBilgi[oyuncuid][oIzlenen] != INVALID_PLAYER_ID) return HataMesaji(playerid, "Girilen oyuncu izlenebilir durumda degil.");

	if(GetPlayerState(playerid) != PLAYER_STATE_SPECTATING)
	{
		GetPlayerPos(playerid, eskipos[playerid][0], eskipos[playerid][1], eskipos[playerid][2]);
		GetPlayerFacingAngle(playerid, eskipos[playerid][3]);
		eskiint[playerid] = GetPlayerInterior(playerid);
		eskiworld[playerid] = GetPlayerVirtualWorld(playerid);
	}
	TogglePlayerSpectating(playerid, true);
	if(IsPlayerInAnyVehicle(oyuncuid)) PlayerSpectateVehicle(playerid, GetPlayerVehicleID(oyuncuid));
	else PlayerSpectatePlayer(playerid, oyuncuid);
	SetPlayerInterior(playerid, GetPlayerInterior(oyuncuid));
	SetPlayerVirtualWorld(playerid, GetPlayerVirtualWorld(oyuncuid));
	OyuncuBilgi[playerid][oIzlenen] = oyuncuid;
	SunucuMesaji(playerid, "%s adli kisiyi izliyorsunuz.", OyuncuIsim(oyuncuid));
	if(OyuncuBilgi[oyuncuid][oAdmin] >= 7) UyariMesaji(oyuncuid, "%s (%s) adli kisi seni izlemeye basladi.", OyuncuIsim(playerid), OyuncuBilgi[playerid][oAdminNick]);
	return 1;
}

CMD:aisimdegistir(playerid, params[])
{
	if(OyuncuBilgi[playerid][oAdmin] < 5) return YetkinizYok(playerid);
    if(OyuncuBilgi[playerid][oAdminDurum] == false && OyuncuBilgi[playerid][oAdmin] < 6) return IsbasiYapmalisin(playerid);

	new oyuncuid, isim[32], query[128];
	if(sscanf(params, "us[32]", oyuncuid, isim))
	    return KullanimMesaji(playerid, "/aisimdegistir [oyuncu id/isim] [yeni isim]");

	if(!RolePlayIsimKontrol(isim))
 	   return HataMesaji(playerid, "Girilen isim roleplay formatina uygun olmalidir. (oRN: John_Doe)");

	if(!IsPlayerConnected(oyuncuid))
 	   return HataMesaji(playerid, "Girilen kisi oyunda degil.");

	if(playerid == oyuncuid && OyuncuBilgi[playerid][oAdmin] < 6)
	    return HataMesaji(playerid, "Kendi ismini degistirebilmek icin en az 6 seviye yonetici olman gerekiyor.");

	if(!OyuncuBilgi[oyuncuid][oGiris]) return HataMesaji(playerid, "Oyuncu giris yapmamis, giris yapmasini bekleyin.");
	if(OyuncuBilgi[oyuncuid][oAdminDurum] == true) return HataMesaji(playerid, "Girilen oyuncu isbasinda oldugu icin isim degistirme uygulanamaz.");

	format(query, sizeof(query), "SELECT `isim` FROM oyuncular WHERE isim = '%s' LIMIT 1", isim);
