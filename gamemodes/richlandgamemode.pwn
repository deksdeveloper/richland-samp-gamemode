CMD:kaput(playerid, params[])
{
	if(IsPlayerInAnyVehicle(playerid) && IsValidVehicle(GetPlayerVehicleID(playerid))) {
	    if(!IsDoorVehicle(GetPlayerVehicleID(playerid))) return HataMesaji(playerid, "Bu aracin bir kaputu yok.");

	    if(!GetHoodStatus(GetPlayerVehicleID(playerid))) SetHoodStatus(GetPlayerVehicleID(playerid), true), cmd_me(playerid, "sag eliyle aracin kaputunu acar.");
		else SetHoodStatus(GetPlayerVehicleID(playerid), false), cmd_me(playerid, "sag eliyle aracin kaputunu kapatir.");
	    return true;
	}
	foreach(new i : Vehicle) if(IsValidVehicle(i) && IsPlayerNearHood(playerid, i))
	{
	    if(!IsDoorVehicle(i)) return HataMesaji(playerid, "Bu aracin bir kaputu yok.");

	    if(!GetHoodStatus(i)) SetHoodStatus(i, true), cmd_me(playerid, "sag eliyle aracin kaputunu acar.");
		else SetHoodStatus(i, false), cmd_me(playerid, "sag eliyle aracin kaputunu kapatir.");
	    return true;
	}
	HataMesaji(playerid, "Etrafinda herhangi bir arac yok veya bir aracin icerisinde degilsin.");
	return 1;
}

CMD:bagaj(playerid, params[])
{
	if(IsPlayerInAnyVehicle(playerid)) return HataMesaji(playerid, "Bu komut aracta kullanilamaz.");
	if(OyuncuBilgi[playerid][oYaraliSure] > 0) return HataMesaji(playerid, "Yerde yaraliyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oDonduruldu] > 0) return HataMesaji(playerid, "Etkisiz haldeyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oKelepceli] == 1) return HataMesaji(playerid, "Kelepceliyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oHapisSure][0] > 0 || OyuncuBilgi[playerid][oHapisSure][1] > 0) return HataMesaji(playerid, "Hapisteyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oIlkYardimYapilan] != INVALID_PLAYER_ID) return HataMesaji(playerid, "ilk yardim yaparken bu komutu kullanamazsin.");
	if(MaymuncukVeri[playerid][mSaniye] > 0) return HataMesaji(playerid, "Maymuncuk kullanirken bu komutu kullanamazsin.");

	new id = -1, sorgu[512];
	if((id = Arac_Yakin(playerid)) != -1)
	{
		if(Arac_SahipKontrol(playerid, id) || AracBilgi[id][aracBirlik] > 0 && OyuncuBilgi[playerid][oBirlikSQLID] == AracBilgi[id][aracBirlik])
		{
		    if(GetTrunkStatus(AracBilgi[id][aracYarat])) return HataMesaji(playerid, "Bu aracin bagaji baskasi tarafindan kullaniyor, lutfen bekleyin.");
			if(BagajSlotKontrol(AracBilgi[id][aracModel]) == 0 && AracBilgi[id][aracModel] != 416) return HataMesaji(playerid, "Bu aracin bagaji yok."), SetTrunkStatus(AracBilgi[id][aracYarat], false);

   			format(sorgu, sizeof(sorgu), "SELECT * FROM bagaj WHERE esyaArac = '%d'", AracBilgi[id][aracID]);
			mysql_tquery(mysqlB, sorgu, "Bagaj_Kontrol", "dd", playerid, id);
		    return true;
		}
		else if(!Arac_SahipKontrol(playerid, id) && OyuncuBirlikTuru(playerid) == BIRLIK_POLIS && OyuncuBilgi[playerid][oIsbasi] == true) {
			if(AracBilgi[id][aracKilit]) return HataMesaji(playerid, "Arac kilitli oldugun icin bagaja erisemiyorsun.");
		    if(GetTrunkStatus(AracBilgi[id][aracYarat])) return HataMesaji(playerid, "Bu aracin bagaji baskasi tarafindan kullaniyor, lutfen bekleyin.");
			if(BagajSlotKontrol(AracBilgi[id][aracModel]) == 0) return HataMesaji(playerid, "Bu aracin bagaji yok."), SetTrunkStatus(AracBilgi[id][aracYarat], false);

   			format(sorgu, sizeof(sorgu), "SELECT * FROM bagaj WHERE esyaArac = '%d'", AracBilgi[id][aracID]);
			mysql_tquery(mysqlB, sorgu, "Bagaj_ElKoy", "dd", playerid, id);
			return true;
		}
		else return HataMesaji(playerid, "Bu aracin bagajini kullanamazsin.");
	}
	HataMesaji(playerid, "Etrafinda herhangi bir arac yok.");
	return 1;
}

CMD:atamirci(playerid, params[])
{
	if(OyuncuBilgi[playerid][oAdmin] < 7) return YetkinizYok(playerid);
    if(!strcmp(params, "olustur", true)) {
		new id = -1;
		id = ModifiyeNoktasi_Yarat(playerid, 100);

		if(id == -1) return HataMesaji(playerid, "Sunucu maksimum modifiye noktasi limitine ulastigi icin yeni modifiye noktasi eklenemiyor.");

		SunucuMesaji(playerid, "Modifiye noktasi olusturuldu. (ID: %d)", id);
        return 1;
    }
	new id, tip[24], icerik[128];
	if(sscanf(params, "ds[24]S()[128]", id, tip, icerik))
 	{
	 	KullanimMesaji(playerid, "/atamirci [id] [isim]");
	    SendClientMessage(playerid, RENK_SARI, "[iSiMLER]:{FFFFFF} olustur, lokasyon, ucret, birlik");
		return 1;
	}
	if((id < 0 || id >= MAKSIMUM_MODIFIYENOKTASI) || !ModifiyeNoktasi[id][modifiyeKullanimda]) return HataMesaji(playerid, "Gecersiz modifiye noktasi.");
	if(!strcmp(tip, "lokasyon", true))
	{
	    GetPlayerPos(playerid, ModifiyeNoktasi[id][modifiyePos][0], ModifiyeNoktasi[id][modifiyePos][1], ModifiyeNoktasi[id][modifiyePos][2]);
		ModifiyeNoktasi[id][modifiyeInt] = GetPlayerInterior(playerid);
		ModifiyeNoktasi[id][modifiyeDunya] = GetPlayerVirtualWorld(playerid);

	    ModifiyeNoktasi_Yenile(id);
	    ModifiyeNoktasi_Kaydet(id);
		YoneticiMesaji(RENK_ACIKKIRMIZI, "YETKILI: %s adli yetkili bir modifiye noktasinin yerini degistirdi. (ID: %d)", OyuncuBilgi[playerid][oAdminNick], id);
	}
 	else if(!strcmp(tip, "birlik", true))
	{
	    new birlik;
	    if(sscanf(icerik, "d", birlik)) return KullanimMesaji(playerid, "/atamirci [id] [birlik] [birlik id]");
		if((birlik < 0 || birlik >= MAKSIMUM_BIRLIK) || !BirlikBilgi[birlik][birlikKullanimda]) return HataMesaji(playerid, "Gecersiz birlik.");

		ModifiyeNoktasi[id][modifiyeBirlik] = BirlikBilgi[birlik][birlikID];
	    ModifiyeNoktasi_Yenile(id);
	    ModifiyeNoktasi_Kaydet(id);
		YoneticiMesaji(RENK_ACIKKIRMIZI, "YETKILI: %s adli yetkili modifiye noktasinin birligini degistirdi. (ID: %d | %s)", OyuncuBilgi[playerid][oAdminNick], id, BirlikBilgi[birlik][birlikAd]);
	}
 	else if(!strcmp(tip, "ucret", true))
	{
	    new ucret;
	    if(sscanf(icerik, "d", ucret)) return KullanimMesaji(playerid, "/atamirci [id] [ucret] [yeni ucret]");
		if(ucret < 100) return HataMesaji(playerid, "Girilen ucret en az $100 olmalidir.");

		ModifiyeNoktasi[id][modifiyeUcret] = ucret;
	    ModifiyeNoktasi_Yenile(id);
	    ModifiyeNoktasi_Kaydet(id);
		YoneticiMesaji(RENK_ACIKKIRMIZI, "YETKILI: %s adli yetkili modifiye noktasinin ucretini degistirdi. (ID: %d | %s)", OyuncuBilgi[playerid][oAdminNick], id, NumaraFormati(ucret));
	}
 	else if(!strcmp(tip, "sil", true))
	{
		ModifiyeNoktasi_Kaldir(id);
		SunucuMesaji(playerid, "Modifiye noktasi kaldirildi. (ID: %d)", id);
	}
	return 1;
}

CMD:abagla(playerid, params[])
{
	if(OyuncuBilgi[playerid][oGiris] == false) return HataMesaji(playerid, "su anda bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oAdmin] < 2) return YetkinizYok(playerid);
	if(OyuncuBilgi[playerid][oAdminDurum] == false && OyuncuBilgi[playerid][oAdmin] < 6) return IsbasiYapmalisin(playerid);

	new id;
	if(sscanf(params, "d", id)) return KullanimMesaji(playerid, "/abagla [arac id]");
	if(!IsValidVehicle(id) || Arac_IDBul(id) == -1) return HataMesaji(playerid, "Gecersiz arac.");

    new aracid = Arac_IDBul(id);
    if(Arac_BirlikTuru(id) == BIRLIK_POLIS || Arac_BirlikTuru(id) == BIRLIK_MEDIKAL || Arac_BirlikTuru(id) == BIRLIK_DEVLET) return HataMesaji(playerid, "Devlete bagli kurum araclari baglanamaz.");
    if(IsValidVehicle(AracBilgi[aracid][aracYarat])) for(new i = 0; i <= 13; i++) AracBilgi[aracid][aracModifiye][i] = GetVehicleComponentInSlot(AracBilgi[aracid][aracYarat], i);
    AracBilgi[aracid][aracDurum] = 2;
    AracBilgi[aracid][aracBaglama] = 250;
	Arac_Spawn(aracid, 0);
	Arac_Kaydet(aracid);
	SunucuMesaji(playerid, "%s plakali arac admin olarak baglandi.", AracBilgi[aracid][aracPlaka]);
	new log_icerik[144];
	format(log_icerik, sizeof(log_icerik), "[%s] %s(%d) bir arac bagladi. (%s / %s)", Tarih(), OyuncuBilgi[playerid][oAdminNick], OyuncuBilgi[playerid][oID], AracBilgi[aracid][aracPlaka], AracModel(AracBilgi[aracid][aracModel]));
	//Gunluk_LogKaydetKaydet(log_icerik);
	Log_Kaydet("loglar/admin_arac.txt", "%s", log_icerik);
	return 1;
}

CMD:aracbagla(playerid, params[])
{
	if(OyuncuBilgi[playerid][oGiris] == false) return HataMesaji(playerid, "su anda bu komutu kullanamazsin.");
	if(OyuncuBirlikTuru(playerid) != BIRLIK_POLIS) return HataMesaji(playerid, "LSPD birliginde degilsin.");
	if(OyuncuBilgi[playerid][oBirlikRutbe] < 2) return HataMesaji(playerid, "Bu islem R2 ve uzeri tarafindan yapilabilir.");
    if(OyuncuBilgi[playerid][oIsbasi] == false) return HataMesaji(playerid,"isbasi durumunda degilken bu komutu kullanamazsin.");
    if(OyuncuBilgi[playerid][oYaraliSure] > 0) return HataMesaji(playerid, "Bu islemi yaraliyken yapamazsin.");
    if(OyuncuBilgi[playerid][oHapisSure][0] > 0 || OyuncuBilgi[playerid][oHapisSure][1] > 0) return HataMesaji(playerid, "Bu islemi hapisteyken yapamazsin.");
    if(OyuncuBilgi[playerid][oDonduruldu] > 0) return HataMesaji(playerid, "Bu islemi dondurulma etkisindeyken yapamazsin.");
	if(IsPlayerInAnyVehicle(playerid)) return HataMesaji(playerid, "Bu islem aracta yapilamaz.");
	if(Arac_Yakin(playerid) == -1) return HataMesaji(playerid, "Yakininda bir arac yok.");

	new aracid = Arac_Yakin(playerid), arac_id = AracBilgi[aracid][aracYarat];
    if(Arac_BirlikTuru(arac_id) == BIRLIK_POLIS || Arac_BirlikTuru(arac_id) == BIRLIK_MEDIKAL || Arac_BirlikTuru(arac_id) == BIRLIK_DEVLET) return HataMesaji(playerid, "Devlete bagli kurum araclari baglanamaz.");
    if(IsValidVehicle(AracBilgi[aracid][aracYarat])) for(new i = 0; i <= 13; i++) AracBilgi[aracid][aracModifiye][i] = GetVehicleComponentInSlot(AracBilgi[aracid][aracYarat], i);
	OyuncuBilgi[playerid][oAracBagla] = aracid;
	OyuncuBilgi[playerid][oBaglamaSure] = 10;
	UyariMesaji(playerid, "%s plakali araci baglamaya basladin, islemini iptal etmek icin aractan uzaklas.", AracBilgi[aracid][aracPlaka]);
	return 1;
}

CMD:dmv(playerid, params[])
{
	if(!IsPlayerInRangeOfPoint(playerid, 10.0, 252.3922,117.5891,1003.2188)) return HataMesaji(playerid, "islemler noktasinda degilsin.");
	if(OyuncuBilgi[playerid][oGiris] == false || OyuncuBilgi[playerid][oDonduruldu] > 0) return HataMesaji(playerid, "su anda bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oHapisSure][0] > 0 || OyuncuBilgi[playerid][oHapisSure][1] > 0) return HataMesaji(playerid, "Hapiste bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oYarali] > 0) return HataMesaji(playerid, "Yaraliyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oAdminDurum] == true) return HataMesaji(playerid, "Admin isbasinda bu komutu kullanamazsin.");

	new sayi = 0, veriler[1024];
	format(veriler, sizeof(veriler), "Plaka\tModel\tDurum\tucret\n");
	for(new i = 0; i < MAKSIMUM_ARAC; i ++) if((Arac_SahipKontrol(playerid, i) || AracBilgi[i][aracBirlik] > 0 && AracBilgi[i][aracBirlik] == OyuncuBilgi[playerid][oBirlikSQLID]) && AracBilgi[i][aracDurum] == 2) {
		format(veriler, sizeof(veriler), "%s%s\t%s\tBagli\t%s\n", veriler, AracBilgi[i][aracPlaka], AracModel(AracBilgi[i][aracModel]), NumaraFormati(AracBilgi[i][aracBaglama]));
		sayi++;
	}
	if(!sayi) return HataMesaji(playerid, "Bagli aracin yok.");
	DialogGoster(playerid, DIALOG_DMV, DIALOG_STYLE_TABLIST_HEADERS, "> DMV", veriler, "Sec", "Kapat");
	return 1;
}

CMD:aracicek(playerid, params[])
{
	if(OyuncuBilgi[playerid][oGiris] == false) return HataMesaji(playerid, "su anda bu komutu kullanamazsin.");
	if(OyuncuBirlikTuru(playerid) != BIRLIK_POLIS) return HataMesaji(playerid, "LSPD degilsin.");
    if(OyuncuBilgi[playerid][oIsbasi] == false) return HataMesaji(playerid,"isbasi durumunda degilken bu komutu kullanamazsin.");
	if(!IsPlayerInAnyVehicle(playerid)) return HataMesaji(playerid, "Aracta degilsin.");
	if(GetVehicleModel(GetPlayerVehicleID(playerid)) != 525) return HataMesaji(playerid, "Bu islemi sadece Tow Truck model araclarda yapabilirsin.");
	if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return HataMesaji(playerid, "Surucu koltugunda olmalisin.");

	if(GetVehicleFromBehind(GetPlayerVehicleID(playerid)) == INVALID_VEHICLE_ID) return HataMesaji(playerid, "Yakininda cekilebilecek arac yok.");
	new aracid = GetVehicleFromBehind(GetPlayerVehicleID(playerid));
	AttachTrailerToVehicle(aracid, GetPlayerVehicleID(playerid));
	return 1;
}

