CMD:oykullan(playerid, params[])
{
	if(Sandik_Yakin(playerid) == -1) return HataMesaji(playerid, "Sandiga yakin degilsin.");
    if(GetPVarInt(playerid, "Oy") > 0) return HataMesaji(playerid, "Zaten oy kullanmissin.");
	if(OyuncuBilgi[playerid][oSeviye] < 3 && OyuncuBilgi[playerid][oAdmin] < 1 && OyuncuBilgi[playerid][oHelper] < 1)
		return HataMesaji(playerid, "Oy kullanabilmeniz icin 3 seviye ya da yonetici olman gerekmektedir.");

	if(!strcmp(SandikBilgi[Sandik_Yakin(playerid)][sandikSahip], OyuncuIsim(playerid), true)) return HataMesaji(playerid, "Kendine oy kullanamazsin.");
	if(OyuncuBilgi[playerid][oGiris] == false) return HataMesaji(playerid, "su anda bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oDonduruldu] > 0) return HataMesaji(playerid, "su anda bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oHapisSure][0] > 0 || OyuncuBilgi[playerid][oHapisSure][1] > 0) return HataMesaji(playerid, "Hapiste bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oYarali] > 0) return HataMesaji(playerid, "Yaraliyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oAdminDurum]) return HataMesaji(playerid, "Yonetici isbasindayken bu komutu kullanamazsin.");

	static id;
	id = Sandik_Yakin(playerid);
	SunucuMesaji(playerid, "%s adli kisi icin oy kullandin, tesekkurler.", SandikBilgi[id][sandikSahip]);
	SetPVarInt(playerid, "Oy", 1);
	SandikBilgi[id][sOy]++;
	Sandik_Yenile(id);
	Sandik_Kaydet(id);
	return 1;
}

CMD:hapis(playerid, params[])
{
	if(OyuncuBirlikTuru(playerid) != BIRLIK_POLIS) return HataMesaji(playerid, "LSPD degilsin.");
	if(Tutuklama_Yakin(playerid) == -1) return HataMesaji(playerid, "Tutuklama noktasina yakin degilsin.");
	new oyuncuid;
	if(sscanf(params, "u", oyuncuid)) return KullanimMesaji(playerid, "/hapis [oyuncu id/isim]");

	if(!IsPlayerConnected(oyuncuid)) return HataMesaji(playerid, "Girilen kisi oyunda degil.");
	if(!YakinlikKontrol(playerid, oyuncuid, 3.0)) return HataMesaji(playerid, "Girilen oyuncuya yakin degilsin.");
	if(oyuncuid == playerid) return HataMesaji(playerid, "Kendini hapise atamazsin.");
	if(OyuncuBilgi[oyuncuid][oAdminDurum] == true) return HataMesaji(playerid, "Yonetici isbasindaki kisiler hapise atilamaz.");
	if(OyuncuBirlikTuru(oyuncuid) == BIRLIK_POLIS) return HataMesaji(playerid, "Polisler hapise atilamaz.");
	if(OyuncuBilgi[oyuncuid][oKelepceli] == 0) return HataMesaji(playerid, "Oyuncu kelepcelenmeden hapise atilamaz.");
	if(OyuncuBilgi[oyuncuid][oYarali]) return HataMesaji(playerid, "Bu kisi yarali once tedavi olmasi gerekiyor.");
	if(OyuncuBilgi[oyuncuid][oSurukleniyor] != INVALID_PLAYER_ID) return HataMesaji(playerid, "Bu oyuncu surukleniyor, hapise atilamaz.");

	new isim[MAX_PLAYER_NAME + 1], sorgu[512];
	GetPlayerName(oyuncuid, isim, sizeof(isim));
	format(sorgu, sizeof(sorgu), "SELECT * FROM aranmakayitlari WHERE Oyuncu = '%s'", isim);
	mysql_tquery(mysqlB, sorgu, "Hapis_Sicil", "dd", playerid, oyuncuid);
	return 1;
}

