	if(MaymuncukVeri[playerid][mSaniye] > 0) return HataMesaji(playerid, "Maymuncuk kullanirken bu komutu kullanamazsin.");

	new oyuncuid;
	if(sscanf(params, "u", oyuncuid)) return KullanimMesaji(playerid, "/hastateslim [oyuncu id/isim]");
	if(!IsPlayerConnected(oyuncuid)) return HataMesaji(playerid, "Girilen oyuncu gecersiz.");
