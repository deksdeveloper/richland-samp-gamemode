		//Gunluk_LogKaydetKaydet(log_icerik);
		Log_Kaydet("loglar/ev_admin.txt", "%s", log_icerik);
	}
	else if(!strcmp(type, "int", true))
	{
	    GetPlayerPos(playerid, EvBilgi[id][evIcPos][0], EvBilgi[id][evIcPos][1], EvBilgi[id][evIcPos][2]);
