CMD:kkabul(playerid, params[])
{
	if(OyuncuBilgi[playerid][oGPS] == 0) return HataMesaji(playerid, "GPS Cihazin yok.");
	if(OyuncuBilgi[playerid][oYaris] != -1) return HataMesaji(playerid, "Yaristayken bunu yapamazsin.");
    if(OyuncuBilgi[playerid][oMateryalTasiyor] == true) return HataMesaji(playerid, "Paket tasirken bunu yapamazsin.");
	if(OyuncuBilgi[playerid][oMarker] > -1) return HataMesaji(playerid, "Konum arastirirken bunu yapamazsin.");
	if(OyuncuBilgi[playerid][oKonum] != -1) return HataMesaji(playerid, "su anda bunu yapamazsin. (\"/cpsifirla\")");
	if(OyuncuBilgi[playerid][oLokasyon] != -1) return HataMesaji(playerid, "su anda bunu yapamazsin. (\"/cpsifirla\")");
	if(OyuncuBilgi[playerid][oAdres] != -1) return HataMesaji(playerid, "su anda bunu yapamazsin. (\"/cpsifirla\")");
	if(OyuncuBilgi[playerid][objeLokasyon] != -1) return HataMesaji(playerid, "su anda bunu yapamazsin. (\"/cpsifirla\")");
	if(OyuncuBilgi[playerid][oGPSArac] != INVALID_VEHICLE_ID) return HataMesaji(playerid, "su anda bunu yapamazsin. (\"/cpsifirla\")");
	if(OyuncuBilgi[playerid][oIsaretliKonum] != 0) return HataMesaji(playerid, "su anda bunu yapamazsin. (\"/cpsifirla\")");
	if(OyuncuBilgi[playerid][oIsaretliMeslek] != -1) return HataMesaji(playerid, "su anda bunu yapamazsin. (\"/cpsifirla\")");
	if(OyuncuBilgi[playerid][oPaintball] != -1) return HataMesaji(playerid, "Paintballda bunu yapamazsin.");

	new numara;
	if(sscanf(params, "d", numara)) return KullanimMesaji(playerid, "/kkabul [telefon numarasi]");
	if(numara == 911 || numara == 0) return HataMesaji(playerid, "Gecersiz numara.");

	foreach(new i : Player) if(OyuncuBilgi[i][oGiris] == true && OyuncuBilgi[i][oKonumGonderilme] > 0 && OyuncuBilgi[i][oKonumGonderilme] == OyuncuBilgi[playerid][oTelefonNumarasi] && OyuncuBilgi[i][oTelefonNumarasi] == numara) {
	    if(OyuncuBilgi[i][oPaintball] != -1) return HataMesaji(playerid, "Konum davetine ulasilamadi."), OyuncuBilgi[i][oKonumGonderilme] = 0;
		OyuncuBilgi[playerid][oIsaretliKonum] = numara;
		OyuncuBilgi[i][oKonumGonderilme] = 0;
		SendClientMessageEx(i, RENK_BEYAZ, "{5453AE}[KONUM] {FFFFFF}Konum davetiniz karsi taraf tarafindan kabul edildi.");
		GetPlayerPos(i, OyuncuBilgi[i][oPos][0], OyuncuBilgi[i][oPos][1], OyuncuBilgi[i][oPos][2]);
		SetPlayerCheckpoint(playerid, OyuncuBilgi[i][oPos][0], OyuncuBilgi[i][oPos][1], OyuncuBilgi[i][oPos][2], 5.0);
        SendClientMessageEx(playerid, RENK_BEYAZ, "{5453AE}[KONUM] {FFFFFF}Talep kabul edildi, konumu haritandan goruntuleyebilirsin.");
		return 1;
	}
	HataMesaji(playerid, "Bu numaradan tarafiniza bir konum iletilmemis.");
	return 1;
}

