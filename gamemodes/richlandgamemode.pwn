	if(OyuncuBilgi[oyuncuid][oGiris] == false) return HataMesaji(playerid, "Oyuncu giris yapmamis, ceza kesemezsin.");
	if(oyuncuid == playerid) return HataMesaji(playerid, "Kendine ceza kesemezsin.");
	if(ceza < 1 || ceza > MAKSIMUM_CEZA) {
		HataMesaji(playerid, "Ceza miktari en az $1, en fazla %s olmalidir.", NumaraFormati(MAKSIMUM_CEZA));
		return 1;
	}
	if(isnull(sebep)) return HataMesaji(playerid, "Ceza nedeni bos birakilamaz.");
 	if(!strcmp(sebep, ".", true)) return HataMesaji(playerid, "Sebep \".\" olamaz.");
 	if(!strcmp(sebep, "Yok", true)) return HataMesaji(playerid, "Sebep \"Yok\" olamaz.");

	new id = Ceza_Ekle(oyuncuid, ceza, sebep, OyuncuIsim(playerid)), emote[64 + MAX_PLAYER_NAME];
	if(id == 0) return HataMesaji(playerid, "Ceza yazilamadi, tekrar deneyin.");

    SunucuMesaji(playerid, "%s adli oyuncuya %s para cezasi yazdin, sebep: %s", OyuncuIsim(oyuncuid), NumaraFormati(ceza), sebep);
    SunucuMesaji(oyuncuid, "%s adli memur sana %s para cezasi yazdi, sebep: %s", OyuncuIsim(playerid), NumaraFormati(ceza), sebep);
	format(emote, sizeof(emote), "bir makbuz ve kalem cikarir, doldurup %s adli kisiye verir.", OyuncuIsim(oyuncuid));
	cmd_me(playerid, emote);
	new log_icerik[144], oyuncu_isim[MAX_PLAYER_NAME + 1], hedef_isim[MAX_PLAYER_NAME + 1];
	GetPlayerName(playerid, oyuncu_isim, sizeof(oyuncu_isim));
	GetPlayerName(oyuncuid, hedef_isim, sizeof(hedef_isim));
	format(log_icerik, sizeof(log_icerik), "[%s] %s(SID: %d), %s(SID: %d) adli kisiye ceza yazdi. (miktar: %s)", Tarih(), oyuncu_isim, OyuncuBilgi[playerid][oID], hedef_isim, OyuncuBilgi[oyuncuid][oID], NumaraFormati(ceza));
	//Gunluk_LogKaydetKaydet(log_icerik);
	Log_Kaydet("loglar/mdc.txt", "%s", log_icerik);
	return 1;
}

CMD:cezalarim(playerid, params[])
