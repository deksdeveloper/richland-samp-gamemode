	    else if(OyuncuBilgi[i][oIzlenen] != INVALID_PLAYER_ID) SendClientMessageEx(playerid, RENK_GRI, "[%s] %s - %s(%d) adli kisiyi izliyor.", (OyuncuBilgi[i][oAdmin] == 0) ? ("HiLE OLABiLiR") : ("NORMAL"), oyuncu_isim, OyuncuIsim(OyuncuBilgi[i][oIzlenen]), OyuncuBilgi[i][oIzlenen]);
		sayi++;
	}
