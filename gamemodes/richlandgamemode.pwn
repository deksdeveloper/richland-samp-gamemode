stock NakliyatTeslim(playerid)
{
    if(OyuncuBilgi[playerid][oMeslek] != 10) return HataMesaji(playerid, "Tircilik mesleginde degilsiniz.");
	new id = OyuncuBilgi[playerid][pSecilenFirma];
	if(id != -1)
	{
	   new ucret = randomEx(FirmaData[id][firmaKazanc1], FirmaData[id][firmaKazanc2]);
	   new bonus = 0;
	   if(FirmaData[id][firmaBonus] == 1)
	   {
          bonus = randomEx(30, 40);
	   }
	   else if(FirmaData[id][firmaBonus] == 2)
	   {
          bonus = randomEx(60, 70);
	   }
	   if(FirmaData[id][firmaBonus] == 1)
	   {
          bonus = randomEx(90, 100);
	   }
	   new toplamucret = ucret+bonus;
	   new maasvergi = 3 * toplamucret / 100;
	   new miktar = toplamucret - maasvergi;
	   SunucuMesaji(playerid, "Basariyla aldiginiz yuku teslim ettiniz!");
	   SunucuMesaji(playerid, "Kazanciniz:");

	   new toplamtutar = 0;
	   if(ikixmeslek == false)
	   {
	      ParaEkle(miktar);
	   }
	   else
	   {
		  new ikixmeslekkazanc = miktar*2;
		  ParaEkle(playerid, ikixmeslekkazanc);
		  toplamtutar = ikixmeslekkazanc;
		  SunucuMesaji(playerid, "2x meslek kazanci aktif oldugu icin toplamda %s kazandiniz.", NumaraFormati(ikixmeslekkazanc));
	   }

	   TirSefer(playerid, 1, 1);

	   SendClientMessageEx(playerid, RENK_SERVER, "------- Maas Dekontu -------");
  	   SendClientMessageEx(playerid, RENK_SERVER, "Kazanc: {FFFFFF}%s", NumaraFormati(ucret));
  	   SendClientMessageEx(playerid, RENK_SERVER, "Bonuslar: {FFFFFF}%s", NumaraFormati(bonus));
	   SendClientMessageEx(playerid, RENK_SERVER, "Net Gelir: {FFFFFF}%s (Vergi: %s3 & -$%s)", NumaraFormati(miktar), "%%", NumaraFormati(maasvergi));
	   SendClientMessageEx(playerid, RENK_SERVER, "----------------------------------");
	}
	return 1;
}
CMD:yukal(playerid, params[])
{
    if(OyuncuBilgi[playerid][oMeslek] != 10) return HataMesaji(playerid, "Bu komutu kullanabilmek icin Tircilik mesleginde olmalisiniz.");
	if(!IsPlayerInRangeOfPoint(playerid, 3.0, 2460.61, -2117.70, 13.55)) return HataMesaji(playerid, "Yuk alma noktasinda degilsin.");
	if(OyuncuBilgi[playerid][oHapisSure][0] > 0 || OyuncuBilgi[playerid][oHapisSure][1] > 0) return HataMesaji(playerid, "Hapisteyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oYarali] > 0 || OyuncuBilgi[playerid][oYaraliSure] > 0) return HataMesaji(playerid, "Yaraliyken bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oDonduruldu] > 0) return HataMesaji(playerid, "Etkisiz haldeyken bu komutu kullanamazsin");
	if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return HataMesaji(playerid, "Bir araca yuk almak icin aracin surucu koltugunda olmalisiniz.");
	if(OyuncuBilgi[playerid][oMarker] > -1 || OyuncuBilgi[playerid][oKonum] != -1 || OyuncuBilgi[playerid][oLokasyon] != -1 || OyuncuBilgi[playerid][oAdres] != -1 || OyuncuBilgi[playerid][objeLokasyon] != -1 || OyuncuBilgi[playerid][oGPSArac] != INVALID_VEHICLE_ID || OyuncuBilgi[playerid][oIsaretliKonum] != 0 || OyuncuBilgi[playerid][oIsaretliMeslek] != -1)
		return HataMesaji(playerid, "Haritanda isaretli bir checkpoint var, /cpsifirla yazdiktan sonra bu komutu kullanabilirsin.");

	if(OyuncuBilgi[playerid][pSecilenFirma] == -1) return HataMesaji(playerid, "Herhangi bir firma ile anlasmamissiniz.");
