	if(IsPlayerConnected(oyuncuid)) return HataMesaji(playerid, "Oyuncu oyunda, /uyariver komutunu kullanabilirsiniz.");

	format(query,sizeof(query),"SELECT * FROM oyuncular WHERE isim = '%s'", isim);
	mysql_tquery(mysqlB, query, "Uyari_Kontrol", "iss", playerid, isim, sebep);
	return 1;
}

CMD:paraver(playerid, params[])
{
    if(OyuncuBilgi[playerid][oAdminDurum] == true) return HataMesaji(playerid, "Yonetici isbasinda bu komutu kullanamazsin.");
	if(OyuncuBilgi[playerid][oHapisSure][0] > 0) return HataMesaji(playerid, "Hapiste bu komutu kullanamazsin.");
	if(GetPVarInt(playerid, "paraver_delay") > gettime()) return HataMesaji(playerid, "Bu komut 20 saniyede bir kez kullanilabilir.");
	if(OyuncuBilgi[playerid][oKumarBahis] != 0) return HataMesaji(playerid, "Kumar sistemini kullanirken para veremezsin.");

	new oyuncuid, miktar, yazi[128];
    if(sscanf(params, "ud", oyuncuid, miktar)) return KullanimMesaji(playerid, "/paraver [oyuncu id/isim] [miktar]");

    if(!IsPlayerConnected(oyuncuid)) return HataMesaji(playerid, "Girilen kisi oyunda degil.");
    if(oyuncuid == playerid) return HataMesaji(playerid, "Kendine para veremezsin.");
	if(!YakinlikKontrol(playerid, oyuncuid, 4.0)) return HataMesaji(playerid, "Oyuncuya yakin degilsin.");
	if(!OyuncuBilgi[oyuncuid][oGiris]) return HataMesaji(playerid, "Oyuncu giris yapmamis, komut uzerinde kullanilamaz.");
	if(miktar < 1) return HataMesaji(playerid, "Girilen miktar $1'in altinda olamaz.");
	if(miktar > 100000) return HataMesaji(playerid, "Bir seferde en fazla $100,000 verebilirsin");
	if(OyuncuBilgi[playerid][oPara] < miktar) return HataMesaji(playerid, "Girilen miktar fiyatinda paran yok.");
	if(miktar > 200 && OyuncuBilgi[playerid][oSeviye] < 2) return HataMesaji(playerid, "$200 dolardan fazla verebilmek icin en az 2 seviye olmalisin.");
	if(OyuncuBilgi[oyuncuid][oHapisSure][0] > 0) return HataMesaji(playerid, "Hapiste bu komutu kullanamazsin.");
	if(OyuncuBilgi[oyuncuid][oAdminDurum] == true) return HataMesaji(playerid, "Girilen kisi yetkili isbasinda, komutu uzerinde kullanamazsin.");

    SetPVarInt(playerid, "paraver_delay", gettime() + 20);
    ParaEkle(oyuncuid, miktar);
    ParaEkle(playerid, -miktar);
	SendClientMessageEx(oyuncuid, RENK_GRI, "%s adli kisi sana {33AA33}%s {AFAFAF}verdi.", OyuncuIsim(playerid), NumaraFormati(miktar));
	SendClientMessageEx(playerid, RENK_GRI, "%s adli kisiye {33AA33}%s {AFAFAF}verdin.", OyuncuIsim(oyuncuid), NumaraFormati(miktar));
    format(yazi, sizeof(yazi), "adli kisi %s adli kisiye bir miktar para verdi.", OyuncuIsim(oyuncuid));
    cmd_me(playerid, yazi);
    OyuncuKaydet(playerid);
    OyuncuKaydet(oyuncuid);
	new log_icerik[144], oyuncu_isim[MAX_PLAYER_NAME + 1], hedef_isim[MAX_PLAYER_NAME + 1];
	GetPlayerName(playerid, oyuncu_isim, sizeof(oyuncu_isim));
	GetPlayerName(oyuncuid, hedef_isim, sizeof(hedef_isim));
	new string[120];
    return 1;
}

CMD:vwdegistir(playerid, params[])
{
