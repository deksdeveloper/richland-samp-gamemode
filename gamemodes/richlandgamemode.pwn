	if(OyuncuBilgi[oyuncuid][oYaraliSure] > 0) return HataMesaji(playerid, "Oyuncu yerde yarali, lutfen oncelikle ilk yardim yapin.");
	if(OyuncuBilgi[oyuncuid][oYarali] == 0) return HataMesaji(playerid, "Oyuncu yarali degil.");

